<!-- Example 17-2 A uniqueness constraint -->
<!-- SKIPTEST -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="catalog" type="CatalogType">
    <xs:unique name="prodNumKey">
      <xs:selector xpath="*/product" />
      <xs:field xpath="number" />
    </xs:unique>
  </xs:element>
</xs:schema>
