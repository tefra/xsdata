<!-- Example 19-15 Implementing extensions for sequence groups using redefine -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:redefine schemaLocation="original.xsd">
    <xs:group name="ext">
      <xs:sequence>
        <xs:group ref="ext" />
        <xs:element ref="c" />
        <xs:element ref="d" />
      </xs:sequence>
    </xs:group>
  </xs:redefine>
</xs:schema>
