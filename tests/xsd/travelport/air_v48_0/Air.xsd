				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Type" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Type of pre pay unique identifier,presently only available value is FlightPass.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompanyName">
		<xs:annotation>
			<xs:documentation>Supplier info that is specific to the Unique Id</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="SupplierCode" type="common:typeCarrier" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PrePayAccount">
