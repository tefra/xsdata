	<xs:simpleType name="typeFareGuarantee">
		<xs:annotation>
			<xs:documentation>The status of a fare</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Auto">
				<xs:annotation>
					<xs:documentation>Automatically generated
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Manual">
				<xs:annotation>
					<xs:documentation>Agent has overridden default(s)
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ManualFare">
				<xs:annotation>
					<xs:documentation>Fare has been constructed by agent
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Guaranteed">
				<xs:annotation>
					<xs:documentation>Fare is guaranteed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Invalid">
				<xs:annotation>
					<xs:documentation>Invalid fare, e.g. due to name or
                  itinerary change</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Restored">
				<xs:annotation>
					<xs:documentation>Ticketed stored fare has been restored
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Ticketed"/>
			<xs:enumeration value="Unticketable">
				<xs:annotation>
					<xs:documentation>Unable to ticket</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Reprice">
				<xs:annotation>
					<xs:documentation>Need requote to ticket</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Expired">
				<xs:annotation>
					<xs:documentation>Expired fare due to older fare guarantee date typically older than 7 days
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AutoUsingPrivateFare">
				<xs:annotation>
					<xs:documentation>Agency private fares that are not guaranteed
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GuaranteedUsingAirlinePrivateFare">
				<xs:annotation>
					<xs:documentation>Guaranteed fare using Airline private fare that was filed with a fare distributor.
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Airline">
				<xs:annotation>
					<xs:documentation>Fare guaranteed by Airline.
               </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GuaranteeExpired">
				<xs:annotation>
					<xs:documentation>Guaranteed fare recently got expired as ticketing hadn't been done within a time frame typically midnight local time of POS .</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AgencyPrivateFareNoOverride">
				<xs:annotation>
					<xs:documentation>Agency Private Fare with no rules override</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Unknown">
				<xs:annotation>
					<xs:documentation>To handle new enumerations added by provider but currently not recognized by API</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
