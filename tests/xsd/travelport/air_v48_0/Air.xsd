			<xs:annotation>
				<xs:documentation>This attribute is used to indicate if the vendors responsible for the fare or rate being returned have been determined to be ‘preferred’ based on the associated policy settings.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="Embargo">
		<xs:annotation>
			<xs:documentation>Embargo details. Provider: 1G, 1V, 1P, 1J
        </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="optional"/>
			<xs:attribute name="SegmentRef" type="common:typeRef" use="optional"/>
			<xs:attribute name="Name" use="optional">
				<xs:annotation>
					<xs:documentation>The commercial name of the optional service on
                    which the embargo applies. Provider: 1G, 1V, 1P, 1J
                </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Text" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Brief description of the embargo. Provider: 1G,
                    1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SecondaryType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The secondary type of the optional service on which the embargo applies.  Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Type" type="common:typeMerchandisingService" use="optional">
				<xs:annotation>
					<xs:documentation>The type of optional service on which the embargo applies.  Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Url" type="xs:anyURI" use="optional">
				<xs:annotation>
					<xs:documentation>Website of the operating carrier. Provider: 1G,
                    1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceSubCode" use="optional">
				<xs:annotation>
					<xs:documentation>The service sub code of the optional service on which the embargo applies.  Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="EmbargoList">
		<xs:annotation>
			<xs:documentation>List of embargoes. Provider: 1G, 1V, 1P, 1J
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Embargo" minOccurs="1" maxOccurs="99"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CO2Emissions">
		<xs:annotation>
			<xs:documentation>The carbon emissions produced by the journey</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CO2Emission" minOccurs="0" maxOccurs="99"/>
			</xs:sequence>
			<xs:attribute name="TotalValue" type="xs:float" use="optional">
				<xs:annotation>
					<xs:documentation> The total CO2 emission value for the journey</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Unit" type="common:StringLength1to64" use="optional">
				<xs:annotation>
					<xs:documentation>The unit used in the TotalValue attribute</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Category" type="common:StringLength1to64" use="optional">
				<xs:annotation>
					<xs:documentation>The category name of the type of cabin, either Economy or Premium.  Premium is any cabin that is not considered Economy</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Source" type="common:StringLength1to64" use="optional">
				<xs:annotation>
					<xs:documentation>The source responsible for the values</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CO2Emission">
		<xs:annotation>
			<xs:documentation>Carbon emission values</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="AirSegmentRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>The segment reference</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Value" type="xs:float" use="optional">
				<xs:annotation>
					<xs:documentation>The CO2 emission value for the air segment</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SplitTicketingSearch">
		<xs:annotation>
			<xs:documentation>SplitTicketingSearch is optional. Used to return both One-Way and Roundtrip fares in a single search response. Applicable to 1G, 1V, 1P only, the price points results path, and a simple roundtrip search only. Cannot be used in combination with Flex options.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="RoundTrip" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Percentage of Roundtrip price points to be returned in the search response. This should be an even number. The One-Way price points returned in the response would be evenly distributed between the outbound and the inbound.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AuditData">
		<xs:annotation>
			<xs:documentation>Container for Pricing Audit Data.For providers 1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attributeGroup ref="common:attrPrices"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExchangeEligibilityInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ExchangePenaltyInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="EligibleFares" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies which fares are eligible for Exchange</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RefundableFares" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Fares eligible for refund: All, Some, None</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassedAutomationChecks" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether the itinerary passed initial validation for automated exchange</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExchangePenaltyInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PenaltyInformation" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="PTC" type="common:typePTC" use="optional"/>
			<xs:attribute name="MinimumChangeFee" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Minimum change fee for changes to the itinerary.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MaximumChangeFee" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Maximum change fee for changes  to the itinerary.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PenaltyInformation">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Carrier" type="common:typeCarrier" use="optional">
						<xs:annotation>
							<xs:documentation>Fare-owning carrier</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="FareBasis" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Unique identifier that provides the association to the fare amount and fare rules.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="FareComponent" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation>A portion of a journey or itinerary between two consecutive fare break points.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="PriceableUnit" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation>Identifies FareComponents that are priced together</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="BoardPoint" type="common:typeIATACode" use="optional">
						<xs:annotation>
							<xs:documentation>Origin for the FareComponent</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="OffPoint" type="common:typeIATACode" use="optional">
						<xs:annotation>
							<xs:documentation>Destination for the FareComponent</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MinimumChangeFee" type="common:typeMoney" use="optional">
						<xs:annotation>
							<xs:documentation>Estimated minimum change fee associated with the fare component.  Can be overridden by ChangeFeeApplicationCodes for other fare components.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MaximumChangeFee" type="common:typeMoney" use="optional">
						<xs:annotation>
							<xs:documentation>Estimated maximum change fee associated with the fare component.  Can be overridden by ChangeFeeApplicationCodes for other fare components.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="FiledCurrency" type="common:typeCurrency" use="optional">
						<xs:annotation>
							<xs:documentation>Currency of the filed change fee</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ConversionRate" type="xs:decimal" use="optional">
						<xs:annotation>
							<xs:documentation>Conversion rate from filed change fee currency to reissue location currency</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Refundable" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Answers whether the FareComponent is refundable</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ChangeFeeApplicationCode" use="optional">
						<xs:annotation>
							<xs:documentation>Unique code associated with the PenaltyInformation text which defines how fees will be applied/calculated. E.g. J2 translates to "From among all fare components, changed and unchanged...."</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSegmentTicketingModifiers">
        <xs:annotation>
            <xs:documentation>Specifies modifiers that a particular segment should be priced with. If this is used, then there must be one for each AirSegment in the AirItinerary.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="AirSegmentRef" type="common:typeRef" use="optional"/>
            <xs:attribute name="BrandTier" type="common:StringLength1to10" use="required">
                <xs:annotation>
                    <xs:documentation>Modifier to price by specific brand tier number.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="AirExchangeMulitQuoteList">
		<xs:annotation>
			<xs:documentation>The shared object list of AirExchangeMultiQuotes
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirExchangeMultiQuoteOption" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirExchangeMultiQuoteOption">
		<xs:annotation>
			<xs:documentation>The shared object list of AirExchangeMultiQuoteOptions
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentData" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AirExchangeBundleTotal" minOccurs="0" maxOccurs="01"/>
				<xs:element ref="AirExchangeBundleList" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSegmentData">
		<xs:annotation>
			<xs:documentation>The shared object list of AirsegmentData
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="BaggageAllowance" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Brand" minOccurs="0" maxOccurs="99"/>
			</xs:sequence>
			<xs:attribute name="CabinClass" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Specifies Cabin class for a group of class of services. Cabin class is not identified if it is not present.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ClassOfService" type="common:typeClassOfService" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirExchangeBundleList">
		<xs:annotation>
			<xs:documentation>The shared object list of AirsegmentData
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirExchangeBundle" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="InvoluntaryChange">
		<xs:annotation>
			<xs:documentation>Specify the Ticket Endorsement value</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TicketEndorsement" minOccurs="1" />
			</xs:sequence> 
		</xs:complexType>
	</xs:element> 
</xs:schema>
