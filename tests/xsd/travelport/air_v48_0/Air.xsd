<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.travelport.com/schema/air_v48_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://www.travelport.com/schema/common_v48_0" targetNamespace="http://www.travelport.com/schema/air_v48_0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation>(c) Travelport GDS 2008 All rights reserved.
            Proprietary Information of Travelport GDS.</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://www.travelport.com/schema/common_v48_0" schemaLocation="../common_v48_0/Common.xsd"/>
	<xs:element name="AirReservation">
		<xs:annotation>
			<xs:documentation>The parent container for all booking data</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeBaseAirReservation"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeBaseAirReservation">
		<xs:annotation>
			<xs:documentation>Parent Container for Air Reservation</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="common:BaseReservation">
				<xs:sequence>
					<xs:element ref="OptionalServices" minOccurs="0"/>
					<xs:element ref="common:SupplierLocator" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="common:ThirdPartyInformation" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="DocumentInfo" minOccurs="0"/>
					<xs:element ref="common:BookingTravelerRef" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="common:ProviderReservationInfoRef" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="AirSegment" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="SvcSegment" minOccurs="0" maxOccurs="999">
						<xs:annotation>
							<xs:documentation>Service segment added to collect additional fee. 1P only</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="AirPricingInfo" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="common:Payment" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="common:CreditCardAuth" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="FareNote" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999"/>
					<xs:element name="TaxInfo" type="typeTaxInfoWithPaymentRef" minOccurs="0" maxOccurs="999">
						<xs:annotation>
							<xs:documentation>Itinerary level taxes</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="TicketingModifiers" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="AssociatedRemark" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="PocketItineraryRemark" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="AirExchangeBundleTotal" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="AirExchangeBundle" minOccurs="0" maxOccurs="999">
						<xs:annotation>
							<xs:documentation>Bundle exchange, pricing, and penalty information. Providers ACH/1G/1V/1P</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="typeAirReservationWithFOP">
		<xs:annotation>
			<xs:documentation>Air Reservation With Form Of Payment.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="typeBaseAirReservation">
				<xs:sequence>
					<xs:element ref="common:FormOfPayment" minOccurs="0" maxOccurs="999"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="AssociatedRemark">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:typeAssociatedRemarkWithSegmentRef"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PocketItineraryRemark">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:typeAssociatedRemarkWithSegmentRef"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirPricingInfo">
		<xs:annotation>
			<xs:documentation>Per traveler type pricing breakdown. This will
                reflect the pricing for all travelers of the specified type.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareStatus" minOccurs="0"/>
				<xs:element ref="FareInfoRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="BookingInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareCalc" minOccurs="0"/>
				<xs:element ref="PassengerType" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:BookingTravelerRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="WaiverCode" minOccurs="0"/>
				<!--  xs:element ref="PaymentRef" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>The reference to the Payment if Air Pricing
                            is charged</xs:documentation>
                    </xs:annotation>
                </xs:element-->
				<xs:element ref="PaymentRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>The reference to the Payment if Air Pricing
                            is charged</xs:documentation>
					</xs:annotation>
				</xs:element>
				<!-- New Elements -->
				<xs:element name="ChangePenalty" type="typeFarePenalty" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>The penalty (if any) to change the itinerary
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CancelPenalty" type="typeFarePenalty" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>The penalty (if any) to cancel the fare
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NoShowPenalty" type="typeFarePenalty" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>The NoShow penalty (if any)
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Adjustment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Yield" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AirPricingModifiers" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="TicketingModifiersRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AirSegmentPricingModifiers" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FlightOptionsList" minOccurs="0"/>
				<xs:element ref="BaggageAllowances" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="FareRulesFilter" minOccurs="0"/>
				<xs:element ref="PolicyCodesList" minOccurs="0">
					<xs:annotation>
						<xs:documentation>A list of codes that indicate why an item was determined to be ‘out of policy’</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PriceChange" type="PriceChangeType" minOccurs="0" maxOccurs="99">
					<xs:annotation>
						<xs:documentation>Indicates a price change is found in Fare Control Manager
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ActionDetails" minOccurs="0"/>
				<xs:element ref="common:Commission" minOccurs="0" maxOccurs="999">
				<xs:annotation>
				<xs:documentation>Allows an agency to update the commission
                     to a new or different commission rate which will be applied at
                     time of ticketing. The commission Modifier allows the user
                     specify how the commission change is to applied
                </xs:documentation>
				</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Origin" type="common:typeIATACode">
				<xs:annotation>
					<xs:documentation>The IATA location code for this origination of this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode">
				<xs:annotation>
					<xs:documentation>The IATA location code for this destination of this entity.</xs:documentation>
			</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="CommandKey" use="optional">
				<xs:annotation>
					<xs:documentation>The command identifier used when this is in
                        response to an AirPricingCommand. Not used in any request
                        processing.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrPrices"/>
			<xs:attributeGroup ref="common:attrProviderSupplier"/>
			<xs:attribute name="AmountType" type="common:StringLength1to32" use="optional">
				<xs:annotation>
					<xs:documentation>This field displays type of payment amount when it is non-monetary. Presently available/supported value is "Flight Pass Credits".</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IncludesVAT" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether the Base Price
                        includes VAT. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExchangeAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The amount to pay to cover the exchange of the
                        fare (includes penalties).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ForfeitAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The amount forfeited when the fare is
                        exchanged.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Refundable" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether the fare is refundable
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Exchangeable" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether the fare is
                        exchangeable</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LatestTicketingTime" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The latest date/time at which this pricing
                        information is valid</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PricingMethod" type="typePricingMethod" use="required"/>
			<xs:attribute name="Checksum" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>A security value used to guarantee that the
                        pricing data sent in matches the pricing data previously returned
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ETicketability" type="typeEticketability" use="optional">
				<xs:annotation>
					<xs:documentation>The E-Ticketability of this AirPricing
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PlatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The Plating Carrier for this journey
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider reservation reference key.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirPricingInfoGroup" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation>This attribute is added to support multiple
                        store fare in Host. All AirPricingInfo with same group number will
                        be stored together.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TotalNetPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The total price of a negotiated fare.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Ticketed" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates if the associated stored fare
                        is ticketed or not.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PricingType" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the Pricing Type used.
                        The possible values are TicketRecord, StoredFare, PricingInstruction.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TrueLastDateToTicket" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>This date indicates the true last date/time to ticket for the fare. This date comes from the filed fare . There is no guarantee the fare will still be available on that date or that the fare amount may change.  
    It is merely the last date to purchase a ticket based on the carriers fare rules at the time the itinerary was quoted and stored      
                        </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
			<xs:attributeGroup ref="attrPolicyMarking">
				<xs:annotation>
					<xs:documentation>Attributes related to policy marking.</xs:documentation>
				</xs:annotation>
			</xs:attributeGroup>
			<xs:attribute name="FareCalculationInd" use="optional">
				<xs:annotation>
					<xs:documentation>Fare calculation that was used to price the itinerary.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Cat35Indicator" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>A true value indicates that the fare has a Cat35 rule.
				A false valud indicates that the fare does not have a Cat35 rule</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ActionDetails">
		<xs:annotation>
			<xs:documentation>Information related to the storing of the fare: Agent, Date and Action for Provider: 1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="PseudoCityCode" type="common:typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>PCC in the host of the agent who stored the fare for Provider: 1P/1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AgentSine" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The sign in of the user who stored the fare for Provider: 1P/1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EventDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Date at which the fare was stored for Provider: 1P/1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EventTime" type="xs:time" use="optional">
				<xs:annotation>
					<xs:documentation>Time at which the fare was stored for Provider: 1P/1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Text" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The type of action the agent performed for Provider: 1P/1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PolicyCodesList">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PolicyCode" type="common:typePolicyCode" minOccurs="1" maxOccurs="10">
					<xs:annotation>
						<xs:documentation>A code that indicates why an item was determined to be ‘out of policy’.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeLowFareSearchId">
		<xs:annotation>
			<xs:documentation>Low Fare Search Id</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--*** Request Types ***-->
	<xs:element name="AirPricingInfoList">
		<xs:annotation>
			<xs:documentation>The shared object list of AirSegments
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightOptionsList">
		<xs:annotation>
			<xs:documentation>List of Flight Options for the itinerary.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FlightOption" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightOption">
		<xs:annotation>
			<xs:documentation>List of Options available for any search air leg.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Option" minOccurs="1" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>List of BookingInfo available for the search air leg.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="LegRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies the Leg Reference for this Flight Option.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Origin" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The IATA location code for this origination of this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The IATA location code for this destination of this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Option">
		<xs:annotation>
			<xs:documentation>List of segment and fare available for the search air leg.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BookingInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Connection" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="TravelTime" type="xs:duration" use="optional">
				<xs:annotation>
					<xs:documentation>Total traveling time that is difference between the departure time of the first segment and the arrival time of the last segments for that particular entire set of connection.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--   -->
	<xs:element name="AirPricingCommand">
		<xs:annotation>
			<xs:documentation>A containter to identify individual pricing
                events. A pricing result will be returned for each pricing command
                according to its parameters.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingModifiers" minOccurs="0"/>
				<xs:element ref="AirSegmentPricingModifiers" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="CommandKey" use="optional">
				<xs:annotation>
					<xs:documentation>An identifier to link the pricing responses to
                        the pricing commands. The value passed here will be returned in
                        the resulting AirPricingInfo(s) from this command.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CabinClass" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Specify the cabin type to price the entire
                        itinerary in. If segment level cabin selection is required, this
                        attribute should not be used.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExchangeAirSegment">
		<xs:annotation>
			<xs:documentation>A container to define segment and cabin class in
                order to process an exchange</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegment"/>
				<xs:element ref="common:CabinClass"/>
			</xs:sequence>
			<xs:attribute name="FareBasisCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The fare basis code to be used for exchange of
                        this segment.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirExchangeBundle">
		<xs:annotation>
			<xs:documentation>Bundle exchange, pricing, and penalty
                information for one ticket number</xs:documentation>
			<xs:documentation>Used both in request and response
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:AirExchangeInfo"/>
				<xs:element ref="AirPricingInfoRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:Penalty" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Only used within an AirExchangeQuoteRsp
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SvcSegment">
		<xs:annotation>
			<xs:documentation>Service segment added to collect additional fee. 1P only
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>The Key of SVC Segment.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Carrier" type="common:typeCarrier">
				<xs:annotation>
					<xs:documentation>The platting carrier</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" type="xs:string" />
			<xs:attribute name="NumberOfItems" type="xs:integer" />
			<xs:attribute name="Origin" type="common:typeIATACode">
				<xs:annotation>
					<xs:documentation>Origin location - Airport code. 1P only.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode">
				<xs:annotation>
					<xs:documentation>Destination location - Airport code. 1P only.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="StartDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Start date of the segment. Generally it is the
						next date after the last air segment. 1P only
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelOrder" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>To identify the appropriate travel sequence for
						Air/Car/Hotel/Passive segments/reservations based on travel dates.
						This ordering is applicable across the UR not provider or traveler
						specific
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" />
			<xs:attribute name="RFIC" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>1P - Reason for issuance</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RFISC" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>1P - Resaon for issuance sub-code
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SvcDescription" type="xs:string"
				use="optional">
				<xs:annotation>
					<xs:documentation>1P - SVC fee description</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Fee" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The fee to be collected using SVC segment
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EMDNumber" type="typeEMDNumber" use="optional">
				<xs:annotation>
					<xs:documentation>Generated EMD number, if EMD is issued on the SVC
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="TCRExchangeBundle">
		<xs:annotation>
			<xs:documentation>Bundle exchange, pricing, and penalty
                information for one ticketless carrier reservation
            </xs:documentation>
			<xs:documentation>Used in AirExchangeReq request and
                AirExchangeQuoteRsp response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:AirExchangeInfo"/>
				<xs:element ref="AirPricingInfoRef" maxOccurs="999"/>
				<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Itinerary level taxes</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:Penalty" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Only used within an AirExchangeQuoteRsp
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="TCRNumber" type="typeTCRNumber" use="required">
				<xs:annotation>
					<xs:documentation>The identifying number for a Ticketless Air
                        Reservation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirExchangeTicketBundle">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:TicketNumber"/>
				<xs:element ref="common:FormOfPayment" minOccurs="0" maxOccurs="2"/>
				<xs:element ref="common:FormOfPaymentRef" minOccurs="0"/>
				<xs:element ref="WaiverCode" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Enumeration">
		<xs:annotation>
			<xs:documentation>Provides the capability to group the
                results into differnt trip type and diversification strategies.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SolutionGroup" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SolutionGroup">
		<xs:annotation>
			<xs:documentation>Specifies the trip type and diversity of
                all or a subset of the result solutions.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PermittedAccountCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:AccountCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PreferredAccountCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:AccountCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProhibitedAccountCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:AccountCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<!-- 2 new blocks added for ITA3.2 -->
				<xs:element name="PermittedPointOfSales" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:PointOfSale" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProhibitedPointOfSales" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:PointOfSale" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Count" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The number of solution to include in this
                        group. If only one group specified, this can be left blank. If
                        multiple groups specified, all counts must add up to the
                        MaxResults of the request.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TripType" type="typeTripType" use="required">
				<xs:annotation>
					<xs:documentation>Specifies the trip type for this group
                        of results. Allows targeting a result set to a particular set of
                        characterists.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Diversification" type="typeDiversity" use="optional">
				<xs:annotation>
					<xs:documentation>Specifies the diversification of this
                        group of results, if specified. Allows targeting a result set to
                        ensure they contain more unique results.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Tag" use="optional">
				<xs:annotation>
					<xs:documentation>An arbitrary name for this group of solutions.
                        Will be returned with the solution for idetification.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Primary" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates that this is a primary
                        SolutionGroup when using alternate pricing concepts
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--*** Shared Response Reference Elements ***-->
	<xs:element name="AirSegmentList">
		<xs:annotation>
			<xs:documentation>The shared object list of AirSegments
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegment" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirScheduleChangedInfo">
		<xs:annotation>
			<xs:documentation>Returned when the requested schedule does
                not match the host system schedule</xs:documentation>
			<xs:documentation>Contents will be a new AirPricingSolution
                that contains all the new schedule information as well as the
                pricing information.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingSolution"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSolutionChangedInfo">
		<xs:annotation>
			<xs:documentation>If RetainReservation is None, this will contain
                the new values returned from the provider. If RetainReservation is
                Price, Schedule, or Both and there is a price/schedule change, this
                will contain the new values that were returned from the provider. If
                RetainReservation is Price, Schedule, or Both and there isn’t a
                price/schedule change, this element will not be returned.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingSolution"/>
			</xs:sequence>
			<xs:attribute name="ReasonCode" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Price"/>
						<xs:enumeration value="Schedule"/>
						<xs:enumeration value="Both"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirPriceResult">
		<xs:annotation>
			<xs:documentation>A solution will be returned if one exists.
                Otherwise an error will be present</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingSolution" minOccurs="0" maxOccurs="99"/>
				<xs:element ref="FareRule" minOccurs="0" maxOccurs="999"/>
				<xs:element name="AirPriceError" type="common:typeResultMessage" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="CommandKey" use="optional">
				<xs:annotation>
					<xs:documentation>The command identifier used when this is in
                        response to an AirPricingCommand. Not used in any request
                        processing.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareInfoList">
		<xs:annotation>
			<xs:documentation>The shared object list of FareInfos
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareInfo" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRemarkList">
		<xs:annotation>
			<xs:documentation>The shared object list of FareInfos
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareRemark" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRemark">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Text" type="xs:string" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="URL" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="Name" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="URL">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Type" type="xs:string" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightDetailsList">
		<xs:annotation>
			<xs:documentation>The shared object list of FlightDetails
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FlightDetails" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareNoteList">
		<xs:annotation>
			<xs:documentation>The shared object list of Notes
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareNote" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HostTokenList">
		<xs:annotation>
			<xs:documentation>The shared object list of Host Tokens
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:HostToken" minOccurs="1" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--*** Seat Map Reference Elements***-->
	<xs:element name="Rows">
		<xs:annotation>
			<xs:documentation>A wrapper for all the information regarding each of the rows. Providers: ACH, 1G, 1V, 1P, 1J
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Row" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Provider: 1G,1V,1P,1J,ACH,MCH.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="SegmentRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Specifies the AirSegment the seat map is for. Providers: ACH, 1G, 1V, 1P, 1J 
</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Row">
		<xs:annotation>
			<xs:documentation>Identifies the row of in a seat map
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Facility" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Characteristic" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Number" type="xs:integer" use="required"/>
			<xs:attribute name="SearchTravelerRef" type="common:typeRef" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Facility">
		<xs:annotation>
			<xs:documentation>The facility definition for a part of a row or a
                seat map</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Characteristic" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:Remark" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="PassengerSeatPrice" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Tax information related to seat price. This is presently populated for MCH and ACH content. Applicable providers are MCH/ACH</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="EMD" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="common:ServiceData" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TourCode" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Type" type="typeFacility" use="required">
				<xs:annotation>
					<xs:documentation>The type of facility</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SeatCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>If a seat type, the seat identifier
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Availability" type="typeSeatAvailability" use="optional">
				<xs:annotation>
					<xs:documentation>If a seat type, the availability of the seat
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SeatPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The price of the seat, if applicable.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Paid" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Set to True if either SeatPrice or GroupSeatPrice are returned.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceSubCode" use="optional">
				<xs:annotation>
					<xs:documentation>The service subcode associated with the
                        Facility </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SSRCode" type="common:typeSSRCode" use="optional">
				<xs:annotation>
					<xs:documentation>The SSR Code associated with the
                        Facility </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IssuanceReason" use="optional">
				<xs:annotation>
					<xs:documentation>A one-letter RFIC value filed by the airline in each Optional Service will be mapped to this attribute. 
                       RFIC is IATA Reason for Issuance Code. Possible codes are A (Air transportation),B (Surface Transportation),C(Bagage),
                       D(Financial Impact),E(Airport Services),F(Merchandise),G(Inflight Services),I (Individual Airline use).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="BaseSeatPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Price of the seats excluding Taxes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Taxes" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Tax amount for the seat price.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Quantity" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The number of units availed for each optional
                        service (e.g. 2 baggage availed will be specified as 2 in quantity
                        for optional service BAGGAGE)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SequenceNumber" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The sequence number associated with the
                        OptionalService </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InclusiveOfTax" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies if the service was filed with a fee that is inclusive of tax.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InterlineSettlementAllowed" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies if the interline settlement is allowed in service .</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GeographySpecification" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Sector, Portion, Journey.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Source" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The Source of the optional service. The source can be ACH, MCE, or MCH.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OptionalServiceRef" type="common:typeRef">
				<xs:annotation>
					<xs:documentation>References the OptionalService for the Row/Facility. Providers: ACH, 1G, 1V, 1P, 1J 
               </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SeatInformationRef" type="common:typeRef">
				<xs:annotation>
					<xs:documentation>Specifies the seat information for the seat. Providers: ACH, 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Characteristic">
		<xs:complexType>
			<xs:attribute name="Value" type="xs:string" use="required"/>
			<xs:attribute name="Position" type="typePosition" use="optional"/>
			<xs:attribute name="RowLocation" type="typeRowLocation" use="optional"/>
			<xs:attribute name="PADISCode" type="common:StringLength1to99" use="optional">
				<xs:annotation>
					<xs:documentation>Industry standard code that defines seat and row characteristic.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PassengerSeatPrice">
		<xs:annotation>
			<xs:documentation>Only used when a passenger has a different
                price than the default.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="required"/>
			<xs:attribute name="Amount" type="common:typeMoney" use="required"/>
		</xs:complexType>
	</xs:element>
	<!--*** Fare Display Elements ***-->
	<xs:element name="FareRoutingInformation" type="xs:string">
		<xs:annotation>
			<xs:documentation>Contains Fare/Tariff Display Routing
                Information</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FareMileageInformation" type="xs:string">
		<xs:annotation>
			<xs:documentation>Contains Fare/Tariff Display Mileage
                Information</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FareDisplay">
		<xs:annotation>
			<xs:documentation>Fare/Tariff Display</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareDisplayRule"/>
				<xs:element ref="FarePricing" maxOccurs="999"/>
				<xs:element ref="FareRestriction" minOccurs="0" maxOccurs="99"/>
				<xs:element ref="FareRoutingInformation" minOccurs="0"/>
				<xs:element ref="FareMileageInformation" minOccurs="0"/>
				<xs:element ref="AirFareDisplayRuleKey" minOccurs="0"/>
				<xs:element ref="BookingCode" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:AccountCode" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AddlBookingCodeInformation" minOccurs="0"/>
				<xs:element ref="FareRuleFailureInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Returns fare rule failure info for Non
                            Valid fares.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PriceChange" type="PriceChangeType" minOccurs="0" maxOccurs="99">
					<xs:annotation>
						<xs:documentation>Indicates a price change is found in Fare Control Manager
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="required"/>
			<xs:attribute name="FareBasis" type="xs:string" use="required"/>
			<xs:attribute name="Amount" type="common:typeMoney" use="required"/>
			<xs:attribute name="TripType" type="typeFareTripType" use="optional"/>
			<xs:attribute name="FareTypeCode" type="typeFareTypeCode" use="optional"/>
			<xs:attribute name="SpecialFare" type="xs:boolean" use="optional"/>
			<xs:attribute name="InstantPurchase" type="xs:boolean" use="optional"/>
			<xs:attribute name="EligibilityRestricted" type="xs:boolean" use="optional"/>
			<xs:attribute name="FlightRestricted" type="xs:boolean" use="optional"/>
			<xs:attribute name="StopoversRestricted" type="xs:boolean" use="optional"/>
			<xs:attribute name="TransfersRestricted" type="xs:boolean" use="optional"/>
			<xs:attribute name="BlackoutsExist" type="xs:boolean" use="optional"/>
			<xs:attribute name="AccompaniedTravel" type="xs:boolean" use="optional"/>
			<xs:attribute name="MileOrRouteBasedFare" type="typeMileOrRouteBasedFare" use="optional"/>
			<xs:attribute name="GlobalIndicator" type="typeATPCOGlobalIndicator" use="optional"/>
			<xs:attribute name="Origin" type="common:typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the origin airport or city code
                        for which this tariff is applicable.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the destination airport or city
                        code for which this tariff is applicable.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareTicketingCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the ticketing code for which this tariff is applicable.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareTicketingDesignator" type="typeTicketDesignator" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the ticketing designator for which this tariff is applicable.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddlBookingCodeInformation">
		<xs:annotation>
			<xs:documentation>Returns additional booking codes for the
                selected fare</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="common:typeNonBlanks"/>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareDisplayRule">
		<xs:annotation>
			<xs:documentation>Fare Display Rule Container</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RuleAdvancedPurchase" minOccurs="0"/>
				<xs:element ref="RuleLengthOfStay" minOccurs="0"/>
				<xs:element ref="RuleCharges" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="RuleNumber" type="xs:string" use="optional"/>
			<xs:attribute name="Source" type="xs:string" use="optional"/>
			<xs:attribute name="TariffNumber" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRestriction">
		<xs:annotation>
			<xs:documentation>Fare Restriction</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareRestrictionDaysOfWeek" minOccurs="0" maxOccurs="3"/>
				<xs:element ref="FareRestrictionDate" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareRestrictionSaleDate" minOccurs="0"/>
				<xs:element ref="FareRestrictionSeasonal" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="FareRestrictiontype" type="typeFareRestrictionType" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRestrictionDaysOfWeek">
		<xs:annotation>
			<xs:documentation>Days of the week that the restriction applies
                too.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Direction" type="typeFareDirectionality" use="optional"/>
			<xs:attribute name="TripType" type="typeFareTripType" use="optional"/>
			<xs:attribute name="Monday" type="xs:boolean" use="optional"/>
			<xs:attribute name="Tuesday" type="xs:boolean" use="optional"/>
			<xs:attribute name="Wednesday" type="xs:boolean" use="optional"/>
			<xs:attribute name="Thursday" type="xs:boolean" use="optional"/>
			<xs:attribute name="Friday" type="xs:boolean" use="optional"/>
			<xs:attribute name="Saturday" type="xs:boolean" use="optional"/>
			<xs:attribute name="Sunday" type="xs:boolean" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRestrictionDate">
		<xs:annotation>
			<xs:documentation>Fare restriction based on date ranges.
                StartDate and EndDate are strings representing respective dates. If
                a year component is present then it signifies an exact date. If only
                day and month components are present then it signifies a seasonal
                date, which means applicable for that date in any year
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Direction" type="typeFareDirectionality" use="optional"/>
			<xs:attribute name="StartDate" type="xs:string" use="optional"/>
			<xs:attribute name="EndDate" type="xs:string" use="optional"/>
			<xs:attribute name="EndDateIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>This field indicates the end date/last date
                        for which travel on the fare component being validated must be
                        commenced or completed </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Commence"/>
						<xs:enumeration value="Complete"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRestrictionSaleDate">
		<xs:annotation>
			<xs:documentation>Restrict when this fare can be sold
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="StartDate" type="xs:date" use="optional"/>
			<xs:attribute name="EndDate" type="xs:date" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRestrictionSeasonal">
		<xs:annotation>
			<xs:documentation>Fares Restricted based on the season requested.
				StartDate and EndDate are strings representing respective dates. If
                a year component is present then it signifies an exact date. If only
                day and month components are present then it signifies a seasonal
                date, which means applicable for that date in any year
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Comment" type="xs:string" use="optional"/>
			<xs:attribute name="VariationByTravelDates" type="xs:string" use="optional"/>
			<xs:attribute name="SeasonalRange1Ind" type="xs:string" use="optional"/>
			<xs:attribute name="SeasonalRange1StartDate" type="xs:string" use="optional"/>
			<xs:attribute name="SeasonalRange1EndDate" type="xs:string" use="optional"/>
			<xs:attribute name="SeasonalRange2Ind" type="xs:string" use="optional"/>
			<xs:attribute name="SeasonalRange2StartDate" type="xs:string" use="optional"/>
			<xs:attribute name="SeasonalRange2EndDate" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FarePricing">
		<xs:annotation>
			<xs:documentation>Container for Fare Pricing Information.
                One per PTC type.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="PassengerType" type="common:typePTC" use="optional"/>
			<xs:attribute name="TotalFareAmount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="PrivateFare" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>NegotiatedFare attribute from
                        earlier version of schema used to imply whether the fare is
                        private fare or not. So, this attribute is renamed to PrivateFare
                        as it best suited.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NegotiatedFare" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies the fare as a Negotiated
                        Fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AutoPriceable" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies the fare as Autopriceable or not. False value means the fare filing is incomplete and the fare should not be used.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TotalNetFareAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Total Net fare amount.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BaseFare" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Base fare amount.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Taxes" type="common:typeMoney" use="optional"/>
			<xs:attribute name="MMid" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Contains the Reference id which is generated when the request was ReturnMM=”true”.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!--*** Fare Rule Elements ***-->
	<xs:element name="FareRuleLookup">
		<xs:annotation>
			<xs:documentation>Parameters to use for a fare rule lookup
                that is not associated with an Air Reservation Locator Code.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:AccountCode" minOccurs="0"/>
				<xs:element ref="common:PointOfSale" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Origin" type="common:typeIATACode" use="required"/>
			<xs:attribute name="Destination" type="common:typeIATACode" use="required"/>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="required"/>
			<xs:attribute name="FareBasis" type="xs:string" use="required"/>
			<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required"/>
			<xs:attribute name="DepartureDate" type="xs:date" use="optional"/>
			<xs:attribute name="TicketingDate" type="xs:date" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!--*** Modifiers ***-->
	<xs:element name="AirExchangeModifiers">
		<xs:annotation>
			<xs:documentation>Provides controls and switches for the
                Exchange process
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ContractCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="ContractCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="BookingDate" type="xs:string" use="optional"/>
			<xs:attribute name="TicketingDate" type="xs:string" use="optional"/>
			<xs:attribute name="AccountCode" type="xs:string" use="optional"/>
			<xs:attribute name="TicketDesignator" type="typeTicketDesignator" use="optional"/>
			<xs:attribute name="AllowPenaltyFares" type="xs:boolean" use="optional" default="true"/>
			<xs:attribute name="PrivateFaresOnly" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="UniversalRecordLocatorCode" type="common:typeLocatorCode" use="optional">
				<xs:annotation>
					<xs:documentation>Which UniversalRecord should this new reservation
                        be applied to. If blank, then a new one is created.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderLocatorCode" type="common:typeLocatorCode" use="optional">
				<xs:annotation>
					<xs:documentation>Which Provider reservation does this reservation
                        get added to.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>To be used with ProviderLocatorCode, which host
                        the
                        reservation being added to belongs to.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlexExploreModifiers">
		<xs:annotation>
			<xs:documentation>This is the container for a set of modifiers which allow the user to perform a special kind of low fare search, depicted as flex explore, based on different parameters like Area, Zone, Country, State, Specific locations, Distance around the actual destination of the itinerary. Applicable for providers 1G,1V,1P</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Destination" type="common:typeIATACode" minOccurs="0" maxOccurs="59">
					<xs:annotation>
						<xs:documentation>List of specific destinations for performing flex explore. Applicable only with flex explore type - Destination</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Type" use="required">
				<xs:annotation>
					<xs:documentation>Type of flex explore to be performed</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="AnyWhere"/>
						<xs:enumeration value="Area"/>
						<xs:enumeration value="Zone"/>
						<xs:enumeration value="Country"/>
						<xs:enumeration value="State"/>
						<xs:enumeration value="DistanceInMiles"/>
						<xs:enumeration value="DistanceInKilometers"/>
						<xs:enumeration value="Destination"/>
						<xs:enumeration value="Group"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Radius" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Radius around the destination of actual itinerary in which the search would be performed. Supported only with types - DistanceInMiles and DistanceInKilometers</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GroupName">
				<xs:annotation>
					<xs:documentation>Group name for a set of destinations to be searched.  Use with Type=Group. Group names are defined in the Search Control Console. Supported Providers:  1G/1V/1P</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirFareDisplayModifiers">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TripType" type="typeFareTripType" minOccurs="0" maxOccurs="3"/>
				<xs:element ref="common:CabinClass" minOccurs="0"/>
				<xs:element ref="PenaltyFareInformation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Request Fares with specific Penalty
                            Information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FareSearchOption" type="typeFareSearchOption" minOccurs="0" maxOccurs="5"/>
			</xs:sequence>
			<xs:attribute name="MaxResponses" type="xs:integer" use="optional" default="200"/>
			<xs:attribute name="DepartureDate" type="xs:date" use="optional"/>
			<xs:attribute name="TicketingDate" type="xs:date" use="optional"/>
			<xs:attribute name="ReturnDate" type="xs:date" use="optional"/>
			<xs:attribute name="BaseFareOnly" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="UnrestrictedFaresOnly" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="FaresIndicator" type="typeFaresIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether only public fares
                        should be returned or specific type of private fares
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CurrencyType" type="common:typeCurrency" use="optional"/>
			<xs:attribute name="IncludeTaxes" type="xs:boolean" use="optional"/>
			<xs:attribute name="IncludeEstimatedTaxes" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Indicates to include estimated taxes i.e. if set to true 
                        estimated total fare,base fare and taxes would be
                        returned. 
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IncludeSurcharges" type="xs:boolean" use="optional"/>
			<xs:attribute name="GlobalIndicator" type="typeATPCOGlobalIndicator" use="optional"/>
			<xs:attribute name="ProhibitMinStayFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitMaxStayFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitAdvancePurchaseFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitNonRefundableFares" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates whether it prohibits
                        NonRefundable Fares.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ValidatedFaresOnly" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates that the requested Fares
                        should be Validated Fares only.
                        If set to true, then only valid fares will be returned.
                        If set to false, both valid and non valid fares will be returned.
                        If not sent, then no validation will be done. All fares will be
                        returned.
                 </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProhibitTravelRestrictedFares" type="xs:boolean" use="optional" default="true">
				<xs:annotation>
					<xs:documentation>Indicates that the Fares not complying
                        Travel Restrictions and Seasonality fare rules are prohibited
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FiledCurrency" type="common:typeCurrency" use="optional">
				<xs:annotation>
					<xs:documentation>Represents the filed currency of the fare
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<!--
                xs:attribute name="PrivateFaresOnly" type="xs:boolean"
                use="optional" default="false" /
            -->
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSegmentPricingModifiers">
		<xs:annotation>
			<xs:documentation>Specifies modifiers that a particular
                segment should be priced in. If this is used, then there must be one
                for each AirSegment in the AirItinerary.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PermittedBookingCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="BookingCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="AirSegmentRef" type="common:typeRef" use="optional"/>
			<xs:attribute name="CabinClass" type="xs:string" use="optional"/>
			<xs:attribute name="AccountCode" type="xs:string" use="optional"/>
			<xs:attribute name="ProhibitAdvancePurchaseFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitNonRefundableFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitPenaltyFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="FareBasisCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The fare basis code to be used for pricing.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareBreak" type="typeFareBreak" use="optional">
				<xs:annotation>
					<xs:documentation>Fare break point modifier to instruct Fares
                        where it should or should not break the fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ConnectionIndicator" type="typeConnectionIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>ConnectionIndicator attribute will be used to map connection indicators 
                        AvailabilityAndPricing, TurnAround and Stopover. This attribute is for Wordspan/1P only.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandTier" type="common:StringLength1to10" use="optional">
				<xs:annotation>
					<xs:documentation>Modifier to price by specific brand tier number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSearchModifiers">
		<xs:annotation>
			<xs:documentation>Controls and switches for the Air Search
                request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DisfavoredProviders" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:Provider" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PreferredProviders" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:Provider" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DisfavoredCarriers" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:Carrier" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="PermittedCarriers" minOccurs="0"/>
				<xs:element ref="ProhibitedCarriers" minOccurs="0"/>
				<xs:element ref="PreferredCarriers" minOccurs="0"/>
				<xs:element ref="PermittedCabins" minOccurs="0"/>
				<xs:element ref="PreferredCabins" minOccurs="0"/>
				<xs:element name="PreferredAlliances" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Alliance" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DisfavoredAlliances" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Alliance" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PermittedBookingCodes" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the container to specify all permitted booking codes</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="BookingCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="PreferredBookingCodes" minOccurs="0"/>
				<xs:element name="ProhibitedBookingCodes" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the container to specify all prohibited booking codes</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="BookingCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="FlightType" minOccurs="0"/>
				<xs:element name="MaxLayoverDuration" type="MaxLayoverDurationType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the maximum duration the layover may have for each trip in the request. Supported providers 1P, 1J.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NativeSearchModifier" type="typeNativeSearchModifier" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Container for Native command modifiers. Providers supported : 1P</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="DistanceType" type="common:typeDistance" use="optional" default="MI"/>
			<xs:attribute name="IncludeFlightDetails" type="xs:boolean" use="optional" default="true"/>
			<xs:attribute name="AllowChangeOfAirport" type="xs:boolean" use="optional" default="true"/>
			<xs:attribute name="ProhibitOvernightLayovers" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>If true, excludes connections if arrival time of first flight and departure time of second flight 
                   is on 2 different calendar days. When used in conjunction with MaxConnectionTime, it would exclude all connections if the 
                   connecting flights wait time exceeds the time specified in MaxConnectionTime.    
                   </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MaxSolutions" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The maximum number of solutions to return.
                        Decreasing this number</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MaxConnectionTime" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The maximum anount of time (in minutes) that a
                        solution can contain for connections between flights.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SearchWeekends" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>A value of true indicates that search should be
                        expanded to include weekend combinations, if applicable.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IncludeExtraSolutions" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>If true, indicates that search should be made
                        for returning more solutions, if available. For example, for
                        certain providers, premium members may have the facility to get
                        more solutions. This attribute may have to be combined with other
                        applicable modifiers (like SearchWeekends) to return more results.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProhibitMultiAirportConnection" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates whether to restrict multi-airport connections</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PreferNonStop" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>When non-stops are preferred, the distribution of search results should skew heavily toward non-stop flights while still returning 
                  some one stop flights for comparison and price competitiveness. The search request will ‘boost' the preference towards non-stops. If true then Non Stop 
                  flights will be preferred.
                  </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OrderBy" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether to sort by Journey Time, Deparature Time or Arrival Time. Applicable to air availability only.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="JourneyTime"/>
						<xs:enumeration value="DepartureTime"/>
						<xs:enumeration value="ArrivalTime"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ExcludeOpenJawAirport" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This option ensures that travel into/out of each location will be into/out of the same airport of that location. Values are true or false. Default value is 'false'. If value is true then open jaws are exclude. If false the open jaws are included. The supported providers: 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExcludeGroundTransportation" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates whether to allow the user to exclude ground transportation or not. Default value is 'false'. If value is true then ground transportations are excluded. If false then ground transportations are included. The supported providers: 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MaxJourneyTime" type="typeMaxJourneyTime" use="optional">
				<xs:annotation>
					<xs:documentation>Maximum Journey Time for all legs (in hours) 0-99. For LFS Supported Providers are 1G,1V,1P,1J. For AirAvail Supported Providers are 1G,1V.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="JetServiceOnly" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Restricts results to Jet service flights only.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OfferAvailabilityModifiers">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ServiceType" type="common:typeMerchandisingService" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>To restrict offers to only this type.
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Carrier" type="common:typeCarrier" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>The carrier whose paid seat optional services is to be returned by uAPI.
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="CurrencyType" type="common:typeCurrency">
				<xs:annotation>
					<xs:documentation>Currency code override. Providers: ACH, 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Alliance">
		<xs:annotation>
			<xs:documentation>Alliance Code</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The possible values are *A for Star Alliance,*O for One world,*S for Sky team etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeAlliance">
		<xs:restriction base="xs:string">
			<xs:enumeration value="StarAlliance"/>
			<xs:enumeration value="OneWorld"/>
			<xs:enumeration value="KLMNorthwestAlliance"/>
			<xs:enumeration value="SkyTeam"/>
			<xs:enumeration value="OWCode"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="FlightType">
		<xs:annotation>
			<xs:documentation>Modifier to request flight type options example non-stop only, non-stop and direct only, 
                include single online connection etc.
        </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="RequireSingleCarrier" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="MaxConnections" use="optional" default="-1">
				<xs:annotation>
					<xs:documentation>The maximum number of connections within a
                        segment group.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:annotation>
							<xs:documentation>To support other provider like 1P maxConnection values ranged from -1 to 3. Here -1 means user don't know about connection information. 
                                     In case of maxConnection -1, maxStop value will be considered.   
                               </xs:documentation>
						</xs:annotation>
						<xs:minInclusive value="-1"/>
						<xs:maxInclusive value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="MaxStops" use="optional" default="-1">
				<xs:annotation>
					<xs:documentation>The maximum number of stops within a
                        connection.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:annotation>
							<xs:documentation>To support other provider like 1P maxStop values ranged from -1 to 3. Here -1 means provider can ignore the maxStop value.</xs:documentation>
						</xs:annotation>
						<xs:minInclusive value="-1"/>
						<xs:maxInclusive value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="NonStopDirects" type="xs:boolean" use="optional"/>
			<xs:attribute name="StopDirects" type="xs:boolean" use="optional"/>
			<xs:attribute name="SingleOnlineCon" type="xs:boolean" use="optional"/>
			<xs:attribute name="DoubleOnlineCon" type="xs:boolean" use="optional"/>
			<xs:attribute name="TripleOnlineCon" type="xs:boolean" use="optional"/>
			<xs:attribute name="SingleInterlineCon" type="xs:boolean" use="optional"/>
			<xs:attribute name="DoubleInterlineCon" type="xs:boolean" use="optional"/>
			<xs:attribute name="TripleInterlineCon" type="xs:boolean" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- Begin Merchandising Elements -->
	<xs:element name="AirSolution">
		<xs:annotation>
			<xs:documentation>Defines an air solution that is comprised of
                an itinerary (the segments) along with the passengers.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SearchTraveler" minOccurs="0" maxOccurs="9"/>
				<xs:element ref="AirSegment" maxOccurs="16"/>
				<xs:element ref="common:HostToken" minOccurs="0" maxOccurs="16"/>
				<xs:element ref="FareBasis" minOccurs="0" maxOccurs="16"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="HostReservation">
		<xs:annotation>
			<xs:documentation>Defines a reservation that already exists in
                the host system (e.g an agent using Galileo Desktop already booked a
                reservation on Midwest in Sabre host system).</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="required">
				<xs:annotation>
					<xs:documentation>The carrier code (e.g. YX, UA, ...) that is
                        providing the merchandising</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CarrierLocatorCode" type="common:typeLocatorCode" use="required">
				<xs:annotation>
					<xs:documentation>The locator code in the supplier system (also
                        could be defined as locator in the carrier host system).
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required">
				<xs:annotation>
					<xs:documentation>Contains the GDS or other provider code
                        of the entity actually housing the reservation. This is optional
                        when used on Merchandising Availability but required on
                        MerchandisingFulfillment. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderLocatorCode" type="common:typeProviderLocatorCode" use="required">
				<xs:annotation>
					<xs:documentation>Contains the locator of the reservation
                        actually housed in the provider.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="UniversalLocatorCode" type="common:typeLocatorCode" use="optional">
				<xs:annotation>
					<xs:documentation>The locator of the Universal Record, if one
                        exists.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ETicket" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>An flag to indicate if ticket has been issued for the PNR.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- End Merchandising Elements -->
	<xs:simpleType name="typeDiversity">
		<xs:annotation>
			<xs:documentation>Used in Low Fare Search to better promote
                unique results</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Blend"/>
			<xs:enumeration value="Airports"/>
			<xs:enumeration value="Carrier"/>
			<xs:enumeration value="Origin"/>
			<xs:enumeration value="Destination"/>
			<xs:enumeration value="DateCombination"/>
			<xs:enumeration value="FirstODDate"/>
			<xs:enumeration value="SecondODDate"/>
			<xs:enumeration value="FirstOD"/>
			<xs:enumeration value="SecondOD"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeTripType">
		<xs:annotation>
			<xs:documentation>Used in Low Fare Search to better target the
                results</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Cheapest"/>
			<xs:enumeration value="Quickest"/>
			<xs:enumeration value="MostConvenient"/>
			<xs:enumeration value="Leisure"/>
			<xs:enumeration value="Business"/>
			<xs:enumeration value="Luxury"/>
			<xs:enumeration value="PreferFirst"/>
			<xs:enumeration value="BusinessOrFirst"/>
			<xs:enumeration value="NoPenalty"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typePosition">
		<xs:annotation>
			<xs:documentation>Facility position with respect to position
                within the aircraft cabin. Possible values are – Left, Right,
                Center, Left Center, Right Center</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Left"/>
			<xs:enumeration value="Right"/>
			<xs:enumeration value="Center"/>
			<xs:enumeration value="LeftCenter"/>
			<xs:enumeration value="RightCenter"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRowLocation">
		<xs:annotation>
			<xs:documentation>Facility Position with respect to a Row.
                Possible values are Rear, Front</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Rear"/>
			<xs:enumeration value="Front"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeFacility">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Seat"/>
			<xs:enumeration value="Aisle"/>
			<xs:enumeration value="Open"/>
			<xs:enumeration value="Unknown"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeReportingType">
		<xs:annotation>
			<xs:documentation>The valid reporting types</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AvailabilityFailure"/>
			<xs:enumeration value="PriceDiscrepancies"/>
			<xs:enumeration value="MarriageDiscrepancies"/>
			<xs:enumeration value="Success"/>
			<xs:enumeration value="ScheduleDiscrepancies"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeSeatAvailability">
		<xs:annotation>
			<xs:documentation>Seat availability info of a seat map
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Available"/>
			<xs:enumeration value="Occupied"/>
			<xs:enumeration value="Reserved"/>
			<xs:enumeration value="AdvancedBoardingPass"/>
			<xs:enumeration value="InterlineCheckin"/>
			<xs:enumeration value="Codeshare"/>
			<xs:enumeration value="Protected"/>
			<xs:enumeration value="PartnerAirline"/>
			<xs:enumeration value="AdvSeatSelection"/>
			<xs:enumeration value="Blocked"/>
			<xs:enumeration value="Extra"/>
			<xs:enumeration value="RBDRestriction"/>
			<xs:enumeration value="Group"/>
			<xs:enumeration value="NoSeat"/>
			<xs:enumeration value="UnoccupiedButNotEligible"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="OverrideCode">
		<xs:annotation>
			<xs:documentation>Code corresponding to the type of
                OverridableException the user wishes to override.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="4"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AirPricingSolution">
		<xs:annotation>
			<xs:documentation>The pricing container for an air travel
            itinerary</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AirSegmentRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Journey" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="LegRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AirPricingInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareNote" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareNoteRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Connection" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:MetaData" minOccurs="0" maxOccurs="999"/>
				<xs:element name="AirPricingResultMessage" type="common:typeResultMessage" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Itinerary level taxes</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="AirItinerarySolutionRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:HostToken" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="OptionalServices" minOccurs="0"/>
				<xs:element ref="AvailableSSR" minOccurs="0"/>
				<xs:element ref="PricingDetails" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="CompleteItinerary" type="xs:boolean" use="optional" default="true">
				<xs:annotation>
					<xs:documentation>This attribute is used to return whether complete Itinerary is present in the AirPricingSolution structure or not. If set to true means AirPricingSolution contains the result for full requested itinerary.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="QuoteDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>This date will be equal to the date of the transaction unless the request included a modified ticket date.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrPrices"/>
			<xs:attribute name="Itinerary" use="optional">
				<xs:annotation>
					<xs:documentation>For an exchange request this tells if the itinerary is the original one or new one. A value of Original will only apply to 1G/1V/1P/1S/1A. A value of New will apply to 1G/1V/1P/1S/1A/ACH.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="New"/>
						<xs:enumeration value="Original"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirPricePointList">
		<xs:annotation>
			<xs:documentation>Provides the list of AirPricePoint (Non Solutioned Result)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricePoint" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>The container which holds the Non Solutioned result. Different options for each search leg requested will be returned and one option for each search leg can be selected.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirPricePoint">
		<xs:annotation>
			<xs:documentation>The container which holds the Non Solutioned result.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element name="AirPricingResultMessage" type="common:typeResultMessage" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Supported by ACH only</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="FareNote" minOccurs="0" maxOccurs="99"/>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Itinerary level taxes</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attributeGroup ref="common:attrPrices"/>
			<xs:attribute name="CompleteItinerary" type="xs:boolean" use="optional" default="true">
				<xs:annotation>
					<xs:documentation>This attribute is used to return whether complete Itinerary is present in the AirPricePoint structure or not. If set to true means AirPricePoint contains the result for full requested itinerary.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Journey">
		<xs:annotation>
			<xs:documentation>Information about all connecting segment list and total traveling time</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentRef" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="TravelTime" type="xs:duration" use="optional">
				<xs:annotation>
					<xs:documentation>Total traveling time that is difference between the departure time of the first segment and the arrival time of the last segments for that particular entire set of connection.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirPricingInfoRef">
		<xs:annotation>
			<xs:documentation>Reference to a AirPricing from a shared
                list</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirItinerarySolution">
		<xs:annotation>
			<xs:documentation>The pricing container for an air travel
                itinerary</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Connection" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirItinerary">
		<xs:annotation>
			<xs:documentation>A container for an Air only travel itinerary.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegment" maxOccurs="999"/>
				<xs:element ref="common:HostToken" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="APISRequirements" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ETR">
		<xs:annotation>
			<xs:documentation>Result of ticketing request
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirReservationLocatorCode" minOccurs="0"/>
				<xs:element ref="common:AgencyInfo" minOccurs="0"/>
				<xs:element ref="common:BookingTraveler"/>
				<xs:element ref="common:FormOfPayment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:Payment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:CreditCardAuth" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>This is a container to display detail information of credit card auth. Providers supported: Worldspan and JAL.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:SupplierLocator" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareCalc"/>
				<xs:element ref="Ticket" maxOccurs="999"/>
				<xs:element ref="common:Commission" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AirPricingInfo" minOccurs="0"/>
				<xs:element ref="AuditData" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="common:Restriction" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="WaiverCode" minOccurs="0"/>
				<xs:element ref="BaggageAllowances" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Baggage Allowance Info after Ticketing</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attributeGroup ref="common:attrPrices"/>
			<xs:attribute name="Refundable" type="xs:boolean" use="optional"/>
			<xs:attribute name="Exchangeable" type="xs:boolean" use="optional"/>
			<xs:attribute name="TourCode" type="typeTourCode" use="optional"/>
			<xs:attribute name="IssuedDate" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Ticket issue date.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BulkTicket" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Whether the ticket was issued as bulk.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional">
				<xs:annotation>
					<xs:documentation>Contains the Provider Code of the provider that houses this ETR.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderLocatorCode" type="common:typeProviderLocatorCode" use="optional">
				<xs:annotation>
					<xs:documentation>Contains the Locator Code of the Provider Reservation that houses this ETR.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IATANumber" type="common:typeIATA" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Contains the IATA Number of the agent initiating the request.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="common:typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Contain Pseudo City, city/office number, branch ID, etc.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CountryCode" type="common:typeCountry" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Contains Ticketed PCC’s Country code.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PlatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Contains the Plating Carrier of this ETR.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketFailureInfo">
		<xs:annotation>
			<xs:documentation>Will be optionally returned as part of
                AirTicketingRsp if one or all ticket requests fail. Atrributes are
                faiilure code, failure message, and passenger reference key.
                Passenger name is a child element.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingInfoRef" minOccurs="1" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Returns related air pricing infos.
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:Name"/>
			</xs:sequence>
			<xs:attribute name="Code" type="xs:integer" use="required"/>
			<xs:attribute name="Message" type="xs:string" use="optional"/>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RefundFailureInfo">
		<xs:annotation>
			<xs:documentation>Will be optionally returned as part of
                AirRefunTicketingRsp if one or all ticket refund requests fail.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="common:TicketNumber"/>
						<xs:element ref="common:Name"/>
					</xs:sequence>
					<xs:element name="TCRNumber" type="typeTCRNumber">
						<xs:annotation>
							<xs:documentation>The identifying number for a Ticketless Air
                                Reservation.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="BookingTravelerRef" type="common:typeRef" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Code" type="xs:integer" use="required"/>
			<xs:attribute name="Message" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="VoidFailureInfo">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="TicketNumber" type="xs:string" use="required"/>
					<xs:attribute name="Code" type="xs:integer" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TCR">
		<xs:annotation>
			<xs:documentation>Information related to Ticketless
                carriers</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:FormOfPayment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:Payment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:BookingTraveler" maxOccurs="999"/>
				<xs:element ref="PassengerTicketNumber" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AirPricingInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:AgencyInfo" minOccurs="0"/>
				<xs:element ref="AirReservationLocatorCode" minOccurs="0"/>
				<xs:element ref="common:SupplierLocator" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:RefundRemark" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="TCRNumber" type="typeTCRNumber" use="required">
				<xs:annotation>
					<xs:documentation>The identifying number for a Ticketless Air
                        Reservation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" type="typeTCRStatus" use="required">
				<xs:annotation>
					<xs:documentation>The current status of this TCR. Some status
                        values are not applicable by some Airlines.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ModifiedDate" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The date at which the status was changed on
                        this TCR due to an action event (itemized from the booleans
                        below).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ConfirmedDate" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The date at which this TCR was confirmed (not
                        created). This mean the payment was approved and processed and
                        travel for this TCR is confirmed.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BasePrice" type="common:typeMoney" use="required">
				<xs:annotation>
					<xs:documentation>The base price of this TCR as a whole as it
                        was when it was first booked.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Taxes" type="common:typeMoney" use="required">
				<xs:annotation>
					<xs:documentation>The taxes of this TCR as a whole as it was
                        when it was first booked.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Fees" type="common:typeMoney" use="required">
				<xs:annotation>
					<xs:documentation>The fees of this TCR as a whole as it was when
                        it was first booked.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Refundable" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Is it possible to perform a Refund for this
                        TCR.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Exchangeable" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Is it possible to perform an Exchange for this
                        TCR.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Voidable" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Is it possible to perform a Void on this TCR.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Modifiable" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Is it possible to modify this TCR (opposed to
                        Refund/Exchange/Void).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:ProviderReservation"/>
			<xs:attribute ref="RefundAccessCode" use="optional"/>
			<xs:attribute name="RefundAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Total Amount refunded to the customer.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RefundFee" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Charges incurred for processing refund.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ForfeitAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Amount forfeited as a result of refund.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:attribute name="RefundAccessCode">
		<xs:annotation>
			<xs:documentation>For some vendors a code/password is required to avail any amount retained during refund.User can define their own password too This attribute will be used to show/accept this code.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="32"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:element name="PassengerTicketNumber">
		<xs:annotation>
			<xs:documentation>Information related to Ticket Number
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="TicketNumber" type="typePassengerTicketNumber" use="optional">
				<xs:annotation>
					<xs:documentation>The identifying number for a Ticket for a
                        passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to a passenger associated with
                        a ticket.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirFareDiscount">
		<xs:annotation>
			<xs:documentation>Fare Discounts</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Percentage" type="xs:double" use="optional"/>
			<xs:attribute name="Amount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="DiscountMethod" type="typeFareDiscount" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareNoteRef">
		<xs:annotation>
			<xs:documentation>A reference to a fare note from a shared list.
                Used to minimize xml results.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="SearchAirLeg">
		<xs:annotation>
			<xs:documentation>Search version of AirLeg used to specify
                search criteria</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SearchOrigin" type="common:typeSearchLocation" maxOccurs="999"/>
				<xs:element name="SearchDestination" type="common:typeSearchLocation" minOccurs="0" maxOccurs="999"/>
				<xs:choice>
					<xs:element name="SearchDepTime" type="common:typeFlexibleTimeSpec" maxOccurs="999"/>
					<xs:element name="SearchArvTime" type="common:typeTimeSpec" maxOccurs="999">
						<xs:annotation>
							<xs:documentation>Specifies the preferred time within the time range. For 1G, 1V, 1P, 1J, it is supported for AvailabilitySearchReq (TimeRange must also be specified) and not supported for LowFareSearchReq. ACH does not support search by arrival time.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element ref="AirLegModifiers" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirLegModifiers">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PermittedCabins" minOccurs="0"/>
				<xs:element ref="PreferredCabins" minOccurs="0"/>
				<xs:element ref="PermittedCarriers" minOccurs="0"/>
				<xs:element ref="ProhibitedCarriers" minOccurs="0"/>
				<xs:element ref="PreferredCarriers" minOccurs="0"/>
				<xs:element name="PermittedConnectionPoints" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the container to specify all permitted connection points. Applicable for 1G/1V/1P/1J.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:ConnectionPoint" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProhibitedConnectionPoints" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the container to specify all prohibited connection points. Applicable for 1G/1V/1P/1J.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:ConnectionPoint" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PreferredConnectionPoints" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the container to specify all preferred connection points. Applicable for 1G/1V only.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:ConnectionPoint" maxOccurs="99"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PermittedBookingCodes" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the container to specify all permitted booking codes</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="BookingCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="PreferredBookingCodes" minOccurs="0"/>
				<xs:element name="PreferredAlliances" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Alliance" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProhibitedBookingCodes" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This is the container to specify all prohibited booking codes</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="BookingCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DisfavoredAlliances" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Alliance" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="FlightType" minOccurs="0"/>
				<xs:element name="AnchorFlightData" type="typeAnchorFlightData" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<xs:attribute name="ProhibitOvernightLayovers" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>If true, excludes connections if arrival time of first flight and departure time of second flight 
                   is on 2 different calendar days. When used in conjunction with MaxConnectionTime, it would exclude all connections if the 
                   connecting flights wait time exceeds the time specified in MaxConnectionTime.    
                   </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MaxConnectionTime" type="xs:integer" use="optional"/>
			<xs:attribute name="ReturnFirstAvailableOnly" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>If it is true then it will search for first
                        available for the booking code designated or any booking code in
                        same cabin.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AllowDirectAccess" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>If it is true request will be sent directly to the carrier.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProhibitMultiAirportConnection" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates whether to restrict multi-airport connections</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PreferNonStop" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>When non-stops are preferred, the distribution of search results should skew heavily toward non-stop flights while still returning 
                  some one stop flights for comparison and price competitiveness. The search request will ‘boost' the preference towards non-stops. If true then Non Stop 
                  flights will be preferred.
                  </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OrderBy" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether to sort by Journey Time, Deparature Time or Arrival Time</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="JourneyTime"/>
						<xs:enumeration value="DepartureTime"/>
						<xs:enumeration value="ArrivalTime"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="MaxJourneyTime" type="typeMaxJourneyTime" use="optional">
				<xs:annotation>
					<xs:documentation>Maximum Journey Time for this leg (in hours) 0-99. Supported Providers 1G,1V.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSegment" type="typeBaseAirSegment">
		<xs:annotation>
			<xs:documentation>An Air marketable travel segment.
            </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AirRefundBundle">
		<xs:annotation>
			<xs:documentation>Bundle refund, pricing, and penalty
                information for one ticket number</xs:documentation>
			<xs:documentation>Used both in request and response
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirRefundInfo"/>
				<xs:element ref="common:Name" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TaxInfo" maxOccurs="999"/>
				<xs:element ref="WaiverCode" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="TicketNumber" type="xs:string"/>			
			<xs:attribute name="PTC">
				<xs:annotation>
					<xs:documentation>Specifies the passenger type code for 1P</xs:documentation>
				</xs:annotation>
			</xs:attribute>			
			<xs:attribute name="RefundType">
				<xs:annotation>
					<xs:documentation>Specifies whether this bundle was auto
                        or manually generated</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Auto"/>
						<xs:enumeration value="Manual"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="TCRRefundBundle">
		<xs:annotation>
			<xs:documentation>Bundle refund, pricing, and penalty
                information for a TCR reservation</xs:documentation>
			<xs:documentation>Used both in request and response
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirRefundInfo"/>
				<xs:element ref="WaiverCode" minOccurs="0"/>
				<xs:element ref="AirSegment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Itinerary level taxes</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:HostToken" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="TCRNumber" type="typeTCRNumber" use="required">
				<xs:annotation>
					<xs:documentation>The identifying number for a Ticketless Air
                        Reservation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RefundType" use="required">
				<xs:annotation>
					<xs:documentation>Specifies whether this bundle was auto
                        or manually generated</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Auto"/>
						<xs:enumeration value="Manual"/>
						<xs:enumeration value="Ignored"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="RefundAccessCode" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirRefundInfo">
		<xs:annotation>
			<xs:documentation>Provides results of a refund quote
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:RefundRemark" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="RefundAmount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="RetainAmount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="RefundFee" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Refund fee for ACH/1P</xs:documentation>
				</xs:annotation>			
			</xs:attribute>			
			<xs:attribute name="RefundableTaxes" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>1P - None : All taxes are not refundable. 
					Unknown : Refundability of taxes are not known.</xs:documentation>
				</xs:annotation>			
			</xs:attribute>			
			<xs:attribute name="FiledCurrency" type="common:typeCurrency" use="optional">
				<xs:annotation>
					<xs:documentation>1P  Currency of filed CAT33 refund fee</xs:documentation>
				</xs:annotation>			
			</xs:attribute>			
			<xs:attribute name="ConversionRate" type="xs:decimal" use="optional">
				<xs:annotation>
					<xs:documentation>1P - Currency conversion rate used for conversion    
					between FiledCurrency and PCC base currency in which the response is
					returned.</xs:documentation>
				</xs:annotation>			
			</xs:attribute>			
			<xs:attribute name="Taxes" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>1P - The total value of taxes.</xs:documentation>
				</xs:annotation>			
			</xs:attribute>			
			<xs:attribute name="OriginalTicketTotal" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>1P - The original ticket amount.</xs:documentation>
				</xs:annotation>			
			</xs:attribute>
			<xs:attribute name="ForfeitAmount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="Retain" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This indicates whether any amount is retained by the provider.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Refund" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This indicates whether carrier/host supports refund for the correcponding pnr.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirRefundModifiers">
		<xs:annotation>
			<xs:documentation>Provides controls and switches for the
                Refund process</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="RefundDate" type="xs:string" use="optional"/>
			<xs:attribute name="AccountCode" type="xs:string" use="optional"/>
			<xs:attribute name="TicketDesignator" type="typeTicketDesignator" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSegmentRef">
		<xs:annotation>
			<xs:documentation>Reference to a complete AirSegment from a
                shared list</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirItinerarySolutionRef">
		<xs:annotation>
			<xs:documentation>Reference to a complete
                AirItinerarySolution from a shared list</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AutoSeatAssignment">
		<xs:annotation>
			<xs:documentation>Request object used to request seats
                automatically by seat type</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="SegmentRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>The segment that this assignment belongs to
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Smoking" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates that the requested seat type
                        should be a smoking seat.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SeatType" type="common:typeReqSeat" use="required">
				<xs:annotation>
					<xs:documentation>The type of seat that is requested
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Group" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates that this seat request is for
                        group seating for all passengers. If no SegmentRef is included,
                        group seating will be requested for all segments.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>The booking traveler that this seat assignment
                        is for. If not entered, this applies to the primary booking
                        traveler and other passengers are adjacent.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareInfo">
		<xs:annotation>
			<xs:documentation>Information about this fare component
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareTicketDesignator" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareSurcharge" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:AccountCode" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="ContractCode" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:Endorsement" minOccurs="0" maxOccurs="999"/>
				<!-- temp FareRule -->
				<xs:element ref="BaggageAllowance" minOccurs="0"/>
				<xs:element ref="FareRuleKey" minOccurs="0"/>
				<xs:element ref="FareRuleFailureInfo" minOccurs="0"/>
				<xs:element ref="FareRemarkRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Brand" minOccurs="0"/>
				<xs:element ref="common:Commission" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Specifies the Commission for Agency for a particular Fare component. Apllicable Providers are 1G and 1V.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FareAttributes" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Returns all fare attributes separated by pipe ‘|’. Attribute information is returned by comma separated values for each attribute. These information include attribute number, chargeable indicator and supplementary info. Attribute numbers: 1 - Checked Bag, 2 - Carry On, 3 - Rebooking, 4 - Refund, 5 - Seats, 6 - Meals, 7 - WiFi. Chargeable Indicator: Y - Chargeable, N - Not Chargeable. Supplementary Information that will be returned is : For 1 and 2 - Baggage weights. For 3 – Changeable Info. For 4 – Refundable Info. For 5 - Seat description. For 6 – Meal description. For 7 – WiFi description. Example: 1,Y,23|1,N,50|2,N,8|3,N,CHANGEABLE|4,Y,REFUNDABLE|5,N,SEATING|5,N,MIDDLE|6,Y,SOFT DRINK|6,N,ALCOHOLIC DRINK|6,Y,SNACK|7,X,WIFI</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ChangePenalty" type="typeFarePenalty" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The penalty (if any) to change the itinerary
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CancelPenalty" type="typeFarePenalty" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The penalty (if any) to cancel the fare
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="FareRulesFilter" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="FareBasis" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The fare basis code for this fare
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassengerTypeCode" type="common:typePTC" use="required">
				<xs:annotation>
					<xs:documentation>The PTC that is associated with this fare.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Origin" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the airport or city code that
                        defines the origin market for this fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the airport or city code that
                        defines the destination market for this fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EffectiveDate" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Returns the date on which this fare was
                        quoted</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the departure date of the first
                        segment that uses this fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DepartureDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the departure date of the first
                        segment of the journey.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Amount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="PrivateFare" type="typePrivateFare" use="optional"/>
			<xs:attribute name="NegotiatedFare" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies the fare as a Negotiated
                        Fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TourCode" type="typeTourCode" use="optional"/>
			<xs:attribute name="WaiverCode" type="xs:string" use="optional"/>
			<xs:attribute name="NotValidBefore" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Fare not valid before this date.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NotValidAfter" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Fare not valid after this date.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="common:typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>Provider PseudoCityCode associated with
                        private fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareFamily" type="common:typeFareFamily" use="optional">
				<xs:annotation>
					<xs:documentation>An alpha-numeric string which denotes fare
                        family. Some carriers may return this in lieu of or in addition to
                        the CabinClass. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PromotionalFare" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Boolean to describe whether the Fare is Promotional fare or not.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CarCode" type="typeCarCode" use="optional"/>
			<xs:attribute name="ValueCode" type="typeValueCode" use="optional"/>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
			<xs:attribute name="BulkTicket" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Whether the ticket can be issued as bulk for this
                        fare. Providers supported: Worldspan and JAL</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InclusiveTour" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Whether the ticket can be issued as part of
                        included package for this fare. Providers supported: Worldspan and
                        JAL</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Value" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Used in rapid reprice</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SupplierCode" type="common:typeSupplierCode" use="optional">
				<xs:annotation>
					<xs:documentation>Code of the provider returning this fare info</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TaxAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Currency code and value for the approximate tax amount for this fare component.  </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeCarCode">
		<xs:annotation>
			<xs:documentation>Car code value. Maximum 15 characters. Applicable
                provider is 1G and 1V
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeValueCode">
		<xs:annotation>
			<xs:documentation>Value code value. Maximum 15 characters. Applicable
                provider is 1G and 1V
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typePrivateFare">
		<xs:annotation>
			<xs:documentation>List the types of private fares, Agency private fare, Airline private Fare and Unknown. 
    Also, this enumaration list includes PrivateFare to indetify private fares for GDSs where we can not identify specific private fares.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="UnknownType"/>
			<xs:enumeration value="PrivateFare"/>
			<xs:enumeration value="AgencyPrivateFare"/>
			<xs:enumeration value="AirlinePrivateFare"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="RouteList">
		<xs:annotation>
			<xs:documentation>Identifies the routes and sub-routes that
                were requested</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Route" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AlternateRouteList">
		<xs:annotation>
			<xs:documentation>Identifies the alternate routes for the
                request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AlternateRoute" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AlternateLocationDistanceList">
		<xs:annotation>
			<xs:documentation>Provides the Distance Information between
                Original Search Airports or City to Alternate Search Airports
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AlternateLocationDistance" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExpertSolutionList">
		<xs:annotation>
			<xs:documentation>Identifies the Expert Solutions retrieved
                from the Knowledge Base.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ExpertSolution" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Route">
		<xs:annotation>
			<xs:documentation>Information about this Route component
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Leg" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AlternateRoute">
		<xs:annotation>
			<xs:documentation>Information about this Alternate Route
                component</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Leg" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareInfoMessage">
		<xs:annotation>
			<xs:documentation>A simple textual fare information message.Providers supported : 1G/1V/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Key" type="common:typeRef" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AlternateLocationDistance">
		<xs:annotation>
			<xs:documentation>Information about the Original Search
                Airport to Alternate Search Airport.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:Distance" minOccurs="1"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="SearchLocation" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The Searching City or Airport specified in the
                        Request.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AlternateLocation" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The nearby Alternate City or Airport to
                        SearchLocation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExpertSolution">
		<xs:annotation>
			<xs:documentation>Information about Expert Solution Route
                component retrieved from Knowledge Base</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LegPrice" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="TotalPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The Total Price for the Solution.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ApproximateTotalPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The Converted Total Price in Agency's Default
                        Currency Value</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CreatedDate" type="xs:date" use="required">
				<xs:annotation>
					<xs:documentation>The Date on which this solution was created
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegPrice">
		<xs:annotation>
			<xs:documentation>Information about the journey Leg Price
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LegDetail" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="TotalPrice" type="common:typeMoney" use="required">
				<xs:annotation>
					<xs:documentation>The Total Prices for the Combination of
                        Journey legs for this Price.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ApproximateTotalPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The Converted Total Price in Agency's Default
                        Currency Value</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Leg">
		<xs:annotation>
			<xs:documentation>Information about the journey Leg
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LegDetail" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="Group" type="xs:int" use="required">
				<xs:annotation>
					<xs:documentation>Returns the Group Number for the leg.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Origin" type="common:typeRailLocationCode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the origin airport or city code
                        for the leg.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeRailLocationCode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the destination airport or city
                        code for the leg.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegDetail">
		<xs:annotation>
			<xs:documentation>Information about the journey Leg,
                Shared by Leg and LegPrice Elements</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="OriginAirport" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the origin airport code for the
                        Leg Detail.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DestinationAirport" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the destination airport code for
                        the Leg Detail.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="required">
				<xs:annotation>
					<xs:documentation>Carrier for the Search Leg Detail.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelDate" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The Departure date and time for this Leg
                        Detail.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FlightNumber" type="common:typeFlightNumber" use="optional">
				<xs:annotation>
					<xs:documentation>Flight Number for the Search Leg Detail.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegRef">
		<xs:annotation>
			<xs:documentation>Reference to a Leg</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareBasis">
		<xs:annotation>
			<xs:documentation>Fare Basis Code</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The fare basis code for this fare
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SegmentRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>The segment to which this FareBasis Code is to
                        connected</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRule">
		<xs:annotation>
			<xs:documentation>Fare Rule Container</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareRuleLong" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FareRuleShort" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="RuleAdvancedPurchase" minOccurs="0"/>
				<xs:element ref="RuleLengthOfStay" minOccurs="0"/>
				<xs:element ref="RuleCharges" minOccurs="0"/>
				<xs:element name="FareRuleResultMessage" type="common:typeResultMessage" minOccurs="0" maxOccurs="999"/>
				<xs:element name="StructuredFareRules" type="StructuredFareRulesType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="FareInfoRef" type="xs:string" use="optional"/>
			<xs:attribute name="RuleNumber" type="xs:string" use="optional"/>
			<xs:attribute name="Source" type="xs:string" use="optional"/>
			<xs:attribute name="TariffNumber" type="xs:string" use="optional"/>
			<xs:attributeGroup ref="common:attrProviderSupplier"/>
		</xs:complexType>
	</xs:element>
<xs:complexType name="StructuredFareRulesType">
		<xs:sequence>
			<xs:element name="FareRuleCategoryType" type="FareRuleCategoryTypes"
				minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>For FareRulesType element</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FareRuleCategoryTypes">
		<xs:sequence>
			<xs:element name="CategoryDetails" type="ValueDetails"
				minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>To indicate details of which category is displayed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VariableCategoryDetails" type="CategoryDetailsType"
				minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>If the specified category of Structured Fare
						Rules is of variable lenght</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Value" type="xs:string" use="required"></xs:attribute>
	</xs:complexType>
	<xs:complexType name="ValueDetails">
		<xs:attribute name="Name" type="xs:string" use="required"></xs:attribute>
		<xs:attribute name="Value" type="xs:string" use="required"></xs:attribute>
	</xs:complexType>
	<xs:complexType name="CategoryDetailsType">
		<xs:sequence>
			<xs:element name="CategoryDetails" type="ValueDetails"
				minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>For each category Details of Structured Fare
						Rules</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Value" type="xs:string" use="required"></xs:attribute>
	</xs:complexType>
	<xs:element name="FareSurcharge">
		<xs:annotation>
			<xs:documentation>Surcharges for a fare component
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="Type" type="xs:string" use="required"/>
			<xs:attribute name="Amount" type="common:typeMoney" use="required"/>
			<xs:attribute name="SegmentRef" type="common:typeRef" use="optional"/>
			<xs:attribute name="CouponRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>The coupon to which that surcharge is relative
                        (if applicable)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRuleShortRef">
		<xs:annotation>
			<xs:documentation>A reference to an Short Text Rule in a Shared List
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRuleLongRef">
		<xs:annotation>
			<xs:documentation>A reference to an Long Text Rule in a Shared List
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRuleShort">
		<xs:annotation>
			<xs:documentation>Short Text Fare Rule</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareRuleNameValue" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Category" type="xs:integer" use="required"/>
			<xs:attribute name="TableNumber" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRuleLong">
		<xs:annotation>
			<xs:documentation>Long Text Fare Rule</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Category" type="xs:integer" use="required"/>
					<xs:attribute name="Type" type="xs:string" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareInfoRef">
		<xs:annotation>
			<xs:documentation>Reference to a complete FareInfo from a
                shared list</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRemarkRef">
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Brand">
		<xs:annotation>
			<xs:documentation>Commercially recognized product offered by an airline</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Title" minOccurs="0" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>The additional titles associated to the brand</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Text" minOccurs="0" maxOccurs="5">
					<xs:annotation>
						<xs:documentation>Text associated to the brand</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ImageLocation" minOccurs="0" maxOccurs="3">
					<xs:annotation>
						<xs:documentation>Images associated to the brand</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="OptionalServices" minOccurs="0"/>
				<xs:element ref="Rules" minOccurs="0" maxOccurs="99">
					<xs:annotation>
						<xs:documentation>Brand rules</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ServiceAssociations" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Service associated with this brand</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="UpsellBrand" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The unique identifier of the Upsell brand</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ApplicableSegment" type="typeApplicableSegment" minOccurs="0" maxOccurs="99"/>
				<xs:element ref="DefaultBrandDetail" minOccurs="0" maxOccurs="99">
					<xs:annotation>
						<xs:documentation>Default brand details.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef">
				<xs:annotation>
					<xs:documentation>Brand Key</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandID" type="typeBrandId">
				<xs:annotation>
					<xs:documentation>The unique identifier of the brand</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Name" type="xs:string">
				<xs:annotation>
					<xs:documentation>The Title of the brand</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirItineraryDetailsRef" type="common:typeRef">
				<xs:annotation>
					<xs:documentation>AirItinerary associated with this brand</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="UpSellBrandID" type="typeBrandId"/>
			<xs:attribute name="BrandFound" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates whether brand for the fare was found for carrier or not</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="UpSellBrandFound" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates whether upsell brand for the fare was found for carrier or not</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandedDetailsAvailable" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates if full details of the brand is available</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="optional"/>
			<xs:attribute name="BrandTier" type="common:StringLength1to10" use="optional">
				<xs:annotation>
					<xs:documentation>Modifier to price by specific brand tier number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandMaintained" type="common:StringLength1to99" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether the brand was maintained from the original ticket.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Title" type="typeTextElement">
		<xs:annotation>
			<xs:documentation>The additional titles associated to the brand or optional service. Providers: ACH, RCH, 1G, 1V, 1P, 1J.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BrandInfo">
		<xs:annotation>
			<xs:documentation>Commercially recognized product offered by an airline
          </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Brand Key</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandID" type="typeBrandId" use="required">
				<xs:annotation>
					<xs:documentation>The unique identifier of the brand</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirPricingInfoRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to a AirPricing. Providers: ACH, 1G, 1V, 1P, 1J.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareInfoRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to a FareInfo. Providers: ACH, 1G, 1V, 1P, 1J.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeBrandId">
		<xs:annotation>
			<xs:documentation>The unique identifier of the brand</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="19"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ImageLocation">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Type" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>Type of Image Location. E.g., "Agent", "Consumer".</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ImageWidth" type="xs:nonNegativeInteger" use="required">
						<xs:annotation>
							<xs:documentation>The width of the image</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ImageHeight" type="xs:nonNegativeInteger" use="required">
						<xs:annotation>
							<xs:documentation>The height of the image</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Rules">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RulesText" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Rules text</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ServiceAssociations">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ApplicableSegment" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Applicable air segment associated with this brand.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element ref="common:ResponseMessage" minOccurs="0"/>
							<xs:element ref="OptionalServiceRef" minOccurs="0"/>
						</xs:choice>
						<xs:attribute name="Key" type="common:typeRef">
							<xs:annotation>
								<xs:documentation>Applicable air segment key</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="UpsellBrand">
		<xs:annotation>
			<xs:documentation>Upsell brand reference</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="FareBasis" type="xs:string" use="optional"/>
			<xs:attribute name="FareInfoRef" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- xs:element name="OptionalServiceRef">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="OptionalService" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element-->
	<xs:element name="OptionalServiceRef" type="common:typeRef">
		<xs:annotation>
			<xs:documentation>Reference to optional service</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SeatInformation">
		<xs:annotation>
			<xs:documentation>Additional information about seats. Providers: 1G, 1V, 1P, 1J,ACH</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Power" type="xs:string">
					<xs:annotation>
						<xs:documentation>Detail about any electrical power the seat might have. For example: No Power Providers: 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Video" type="xs:string">
					<xs:annotation>
						<xs:documentation>Detail about any video components the seat might have. For example: No Video Providers: 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Type" type="xs:string">
					<xs:annotation>
						<xs:documentation>Detail about the type of seat. For example: Exit Row, Standard, etc. Providers: 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Description" type="xs:string">
					<xs:annotation>
						<xs:documentation>Detailed description of the seat. Providers: 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Rating">
					<xs:annotation>
						<xs:documentation>Definition of the seat rating. Providers: 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="Number" type="xs:integer" use="required">
									<xs:annotation>
										<xs:documentation>Numerical rating of the seat from 1 to 5 with 1 being bad and 5 being good. Providers: 1G, 1V, 1P, 1J</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRuleNameValue">
		<xs:annotation>
			<xs:documentation>Fare Rule Name Value Pair, used in Short Rules
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Name" type="xs:string" use="required"/>
			<xs:attribute name="Value" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareType">
		<xs:annotation>
			<xs:documentation>Used to request fares based on the ATPCO type
                code</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="typeFareTypeCode" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightDetails">
		<xs:annotation>
			<xs:documentation>Specific details within a flight segment.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Connection" minOccurs="0"/>
				<xs:element ref="Meals" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="InFlightServices" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attributeGroup ref="common:attrOrigDestDepatureInfo"/>
			<xs:attributeGroup ref="common:attrFlightTimes"/>
			<xs:attribute name="Equipment" type="typeEquipment" use="optional"/>
			<xs:attribute name="OnTimePerformance" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Represents flight on time performance
                        as a percentage from 0 to 100</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OriginTerminal" type="xs:string" use="optional"/>
			<xs:attribute name="DestinationTerminal" type="xs:string" use="optional"/>
			<xs:attribute name="GroundTime" type="xs:integer" use="optional"/>
			<xs:attribute name="AutomatedCheckin" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>“True” indicates that the flight allows automated check-in. The default is “False”.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightDetailsRef">
		<xs:annotation>
			<xs:documentation>Reference to a complete FlightDetails from
                a shared list</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AlternateLocationDistanceRef">
		<xs:annotation>
			<xs:documentation>Reference to a AlternateLocationDistance
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FeeInfo" type="common:typeFeeInfo">
		<xs:annotation>
			<xs:documentation>A generic type of fee for those charges which are incurred by the passenger, but not necessarily shown on tickets</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WaiverCode">
		<xs:annotation>
			<xs:documentation>Waiver code to override fare validations
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="TourCode" type="typeTourCode" use="optional"/>
			<xs:attribute name="TicketDesignator" type="typeTicketDesignator" use="optional"/>
			<xs:attribute name="Endorsement" use="optional">
				<xs:annotation>
					<xs:documentation>Endorsement. Size can be up to 100
                        characters</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
						<xs:minLength value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PaymentRef">
		<xs:annotation>
			<xs:documentation>Reference to one of the air reservation
                payments</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Ticket">
		<xs:annotation>
			<xs:documentation>The ticket that resulted from an air booking
           </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Coupon" maxOccurs="4"/>
				<xs:element ref="TicketEndorsement" minOccurs="0" maxOccurs="6"/>
				<xs:element ref="TourCode" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="ExchangedTicketInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="TicketNumber" type="common:typeTicketNumber" use="required"/>
			<xs:attribute name="TicketStatus" type="common:typeTicketStatus" use="optional"/>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="SpecificSeatAssignment">
		<xs:annotation>
			<xs:documentation>Request object used to specify a specific
                seat</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>The passenger that this seat assignment is for
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SegmentRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>The segment that we will assign this seat on
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FlightDetailRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>The Flight Detail ref of the AirSegment used
                        when requesting seats on Change of Guage flights
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SeatId" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The actual seat ID that is being requested.
                        Special Characters are not supported in this field.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RailCoachNumber" use="optional">
				<xs:annotation>
					<xs:documentation>Coach number for which rail seatmap/coachmap is returned.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RuleAdvancedPurchase">
		<xs:annotation>
			<xs:documentation>Container for rules regarding advance
                purchase restrictions. TicketingEarliestDate and TicketingLatestDate
                are strings representing respective dates. If a year component is
                present then it signifies an exact date. If only day and month
                components are present then it signifies a seasonal date, which
                means applicable for that date in any year</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ReservationLatestPeriod" type="xs:string" use="optional"/>
			<xs:attribute name="ReservationLatestUnit" type="typeStayUnit" use="optional"/>
			<xs:attribute name="TicketingEarliestDate" type="xs:string" use="optional"/>
			<xs:attribute name="TicketingLatestDate" type="xs:string" use="optional"/>
			<xs:attribute name="MoreRulesPresent" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>If true, specifies that advance purchase
                        information will be present in fare rules.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RuleCharges">
		<xs:annotation>
			<xs:documentation>Container for rules related to charges
                such as deposits, surcharges, penalities, etc..</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="PenaltyType" type="xs:string" use="optional"/>
			<xs:attribute name="DepartureStatus" type="xs:string" use="optional"/>
			<xs:attribute name="Amount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="Percent" type="xs:decimal" use="optional"/>
			<xs:attribute name="MoreRulesPresent" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>If true, specifies that advance purchase
                        information will be present in fare rules.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RuleLengthOfStay">
		<xs:annotation>
			<xs:documentation>Container for rules providing minimum and
                maximum stay requirements.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MinimumStay" type="typeRestrictionLengthOfStay" minOccurs="0"/>
				<xs:element name="MaximumStay" type="typeRestrictionLengthOfStay" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocumentInfo">
		<xs:annotation>
			<xs:documentation>Container for the document information
               summary line.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TicketInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element name="MCOInfo" type="common:MCOInformation" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TCRInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareCalc" type="xs:string">
		<xs:annotation>
			<xs:documentation>The complete fare calculation line.
            </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FareTicketDesignator">
		<xs:annotation>
			<xs:documentation>Ticket Designator used to further qualify a
                Fare</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Value" type="typeTicketDesignator" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirPricingModifiers">
		<xs:annotation>
			<xs:documentation>Controls and switches for a Air Search
                request that contains Pricing Information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ProhibitedRuleCategories" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="FareRuleCategory" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="AccountCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:AccountCode" maxOccurs="999">
								<xs:annotation>
									<xs:documentation>Used to get negotiated pricing. Provider:ACH.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="PermittedCabins" minOccurs="0"/>
				<xs:element name="ContractCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="ContractCode" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="ExemptTaxes" minOccurs="0"/>
				<xs:element ref="PenaltyFareInformation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Request Fares with specific Penalty
                            Information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:DiscountCard" minOccurs="0" maxOccurs="9">
					<xs:annotation>
						<xs:documentation>Discount request for rail.
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PromoCodes" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="PromoCode" minOccurs="1" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="ManualFareAdjustment" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Represents increment/discount applied manually by agent.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:PointOfSale" minOccurs="0">
					<xs:annotation>
						<xs:documentation>User can use this node to send a specific PCC to access fares allowed only for that PCC. This node gives the capability for fare redistribution at stored fare level. As multiple UAPI AirPricingInfos (all having same AirPricingInfoGroup) can converge to a single stored fare, UAPI will map PoinOfSale information from the first available one from each group</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="BrandModifiers" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Used to specify the level of branding requested.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="MultiGDSSearchIndicator" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="PreferredCabins" minOccurs="0" maxOccurs="99"/>
			</xs:sequence>
			<xs:attribute name="ProhibitMinStayFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitMaxStayFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="CurrencyType" type="common:typeCurrency" use="optional"/>
			<xs:attribute name="ProhibitAdvancePurchaseFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitNonRefundableFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ProhibitRestrictedFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="FaresIndicator" type="typeFaresIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether only public fares
                        should be returned or specific type of private fares
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FiledCurrency" type="common:typeCurrency" use="optional">
				<xs:annotation>
					<xs:documentation>Currency in which Fares/Prices will be filed if supported by the supplier else approximated to.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PlatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The Plating Carrier for this journey.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OverrideCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The Plating Carrier for this journey.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ETicketability" type="typeEticketability" use="optional">
				<xs:annotation>
					<xs:documentation>Request a search based on whether only
                        E-ticketable fares are required.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AccountCodeFaresOnly" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether or not the private
                        fares returned should be restricted to only those specific to the
                        input account code and contract code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="ProhibitNonExchangeableFares" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="ForceSegmentSelect" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This indicator allows agent to force segment select option in host while selecting all air segments to store price on a PNR. This is relevent only when agent selects all air segmnets to price. if agent selects specific segments to price then this attribute will be ignored by the system. This is currently used by Worldspan only.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InventoryRequestType" type="typeInventoryRequest" use="optional">
				<xs:annotation>
					<xs:documentation>This allows user to make request for a particular source of inventory for pricing modifier purposes. This is currently used by Worldspan only. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OneWayShop" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>Via this attribute one way shop can be requested. Applicable provider is 1G </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProhibitUnbundledFareTypes" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>A "True" value wiill remove fares with EOU and ERU fare types from consideration. A "False" value is the same as no value.  Default is no value. Applicable providers:  1P/1J/1G/1V</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReturnServices" type="xs:boolean" use="optional" default="true">
				<xs:annotation>
					<xs:documentation>When set to false, ATPCO filed Optional Services will not be returned. Default is true. Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ChannelId" use="optional">
				<xs:annotation>
					<xs:documentation>A Channel ID is 2 to 4 alpha-numeric characters used to activate the Search Control Console filter for a specific group of travelers being served by the agency credential.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="2"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ReturnFareAttributes" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Returns attributes that are associated to a fare</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SellCheck" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Checks if the segment is bookable before pricing</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReturnFailedSegments" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>If "true", returns failed segments information.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ManualFareAdjustment">
		<xs:complexType>
			<xs:attribute name="AppliedOn" type="common:typeAdjustmentTarget" use="required">
				<xs:annotation>
					<xs:documentation>Represents pricing component upon which manual increment/discount to be applied. Presently supported values are Base and Total. Other is present as a future place holder but presently no request processing logic is available for value Other</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AdjustmentType" type="common:typeAdjustmentType" use="required">
				<xs:annotation>
					<xs:documentation>Represents process used for applying manual discount/increment. Presently supported values are Flat, Percentage.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Value" type="xs:decimal" use="required">
				<xs:annotation>
					<xs:documentation>Represents value of increment/discount applied. Negative value is considered as discount whereas positive value represents increment</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassengerRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Represents passenger association.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketDesignator" type="typeTicketDesignator" use="optional">
				<xs:annotation>
					<xs:documentation>Providers: 1p/1j</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareType" type="typeFareTypeCode" use="optional">
				<xs:annotation>
					<xs:documentation>Providers: 1p/1j</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSearchAsynchModifiers">
		<xs:annotation>
			<xs:documentation>Controls and switches for the Air Search
                request for Asynch Request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="InitialAsynchResult" minOccurs="0">
					<xs:complexType>
						<xs:attribute name="MaxWait" type="xs:positiveInteger" use="optional">
							<xs:annotation>
								<xs:documentation>Max wait time in seconds.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExemptTaxes">
		<xs:annotation>
			<xs:documentation>Request tax exemption for specific tax
                category and/or all taxes of a specific country</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CountryCode" type="common:typeCountry" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Specify ISO country code for which tax
                         exemption is requested.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TaxCategory" type="xs:string" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Specify tax category for which tax
                         exemption is requested.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="AllTaxes" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Request exemption of all taxes.
                   </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TaxTerritory" use="optional">
				<xs:annotation>
					<xs:documentation>exemption is achieved by sending in the TaxTerritory in the tax exempt price request.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CompanyName" use="optional">
				<xs:annotation>
					<xs:documentation>The federal government body name must be provided in this element. This field is required by AC</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="24"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeFaresIndicator">
		<xs:annotation>
			<xs:documentation>Defines the type of fares to return (Only
                public fares, Only private fares, Only agency private fares, Only
                airline private fares or all fares)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PublicFaresOnly"/>
			<xs:enumeration value="PrivateFaresOnly"/>
			<xs:enumeration value="AgencyPrivateFaresOnly"/>
			<xs:enumeration value="AirlinePrivateFaresOnly"/>
			<xs:enumeration value="PublicAndPrivateFares"/>
			<xs:enumeration value="NetFaresOnly"/>
			<xs:enumeration value="AllFares">
				<xs:annotation>
					<xs:documentation>Applicable for 1G/1V air shop only</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="FareRuleCategory">
		<xs:annotation>
			<xs:documentation>Rule Categories to filter on.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Category" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirAvailInfo">
		<xs:annotation>
			<xs:documentation>Matches class of service information with
                availability counts. Only provided on search results.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BookingCodeInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element name="FareTokenInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Associates Fare with HostToken
                        </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="FareInfoRef" type="xs:string" use="required"/>
						<xs:attribute name="HostTokenRef" type="xs:string" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional"/>
			<xs:attribute name="HostTokenRef" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BaggageAllowance">
		<xs:annotation>
			<xs:documentation>Free Baggage Allowance</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="NumberOfPieces" type="xs:integer" minOccurs="0"/>
				<xs:element name="MaxWeight" type="typeWeight" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- *************************************************************************************************************************************** -->
	<!-- *************************************************************************************************************************************** -->
	<!-- *************************************************************************************************************************************** -->
	<xs:element name="BaggageAllowances">
		<xs:annotation>
			<xs:documentation>Details of Baggage allowance</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BaggageAllowanceInfo" minOccurs="1" maxOccurs="999"/>
				<xs:element ref="CarryOnAllowanceInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="EmbargoInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="BaseBaggageAllowanceInfo">
		<xs:annotation>
			<xs:documentation>This contains common elements that are used for Baggage Allowance info, carry-on allowance info and embargo Info. Supported providers are 1V/1G/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="URLInfo" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Contains the text and URL information as published by carrier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="TextInfo" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Text information as published by carrier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Origin" type="common:typeIATACode" use="optional"/>
		<xs:attribute name="Destination" type="common:typeIATACode" use="optional"/>
		<xs:attribute name="Carrier" type="common:typeCarrier" use="optional"/>
	</xs:complexType>
	<xs:element name="BaggageAllowanceInfo">
		<xs:annotation>
			<xs:documentation>Information related to Baggage allowance like URL,Height,Weight etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="BaseBaggageAllowanceInfo">
					<xs:sequence>
						<xs:element ref="BagDetails" minOccurs="0" maxOccurs="999"/>
					</xs:sequence>
					<xs:attribute name="TravelerType" type="common:typePTC" use="optional"/>
					<xs:attribute name="FareInfoRef" type="common:typeRef" use="optional"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CarryOnAllowanceInfo">
		<xs:annotation>
			<xs:documentation>Information related to Carry-On allowance like URL, pricing etc</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="BaseBaggageAllowanceInfo">
					<xs:sequence>
						<xs:element name="CarryOnDetails" minOccurs="0" maxOccurs="999">
							<xs:annotation>
								<xs:documentation>Information related to Carry-On Bag details .</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="BaggageRestriction" minOccurs="0" maxOccurs="99"/>
								</xs:sequence>
								<xs:attribute name="ApplicableCarryOnBags" type="xs:string">
									<xs:annotation>
										<xs:documentation>Applicable Carry-On baggage "First", "Second", "Third" etc</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="BasePrice" type="common:typeMoney"/>
								<xs:attribute name="ApproximateBasePrice" type="common:typeMoney"/>
								<xs:attribute name="Taxes" type="common:typeMoney"/>
								<xs:attribute name="TotalPrice" type="common:typeMoney"/>
								<xs:attribute name="ApproximateTotalPrice" type="common:typeMoney"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="EmbargoInfo" type="BaseBaggageAllowanceInfo">
		<xs:annotation>
			<xs:documentation>Information related to Embargo</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="URLInfo">
		<xs:annotation>
			<xs:documentation>Contains the text and URL of baggage as published by carrier.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Text" type="common:typeGeneralText" minOccurs="0" maxOccurs="999"/>
				<xs:element name="URL" type="xs:anyURI" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BagDetails">
		<xs:annotation>
			<xs:documentation>Information related to Bag details .</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BaggageRestriction" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="AvailableDiscount" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="ApplicableBags" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Applicable baggage like Carry-On,1st Check-in,2nd Check -in etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BasePrice" type="common:typeMoney" use="optional"/>
			<xs:attribute name="ApproximateBasePrice" type="common:typeMoney" use="optional"/>
			<xs:attribute name="Taxes" type="common:typeMoney" use="optional"/>
			<xs:attribute name="TotalPrice" type="common:typeMoney" use="optional"/>
			<xs:attribute name="ApproximateTotalPrice" type="common:typeMoney" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TextInfo">
		<xs:annotation>
			<xs:documentation>Information on baggage as published by carrier.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Text" type="common:typeGeneralText" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Title" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BaggageRestriction">
		<xs:annotation>
			<xs:documentation>Information related to  Baggage restriction rules .</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Dimension" minOccurs="0" maxOccurs="999"/>
				<xs:element name="MaxWeight" type="typeUnitOfMeasure" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TextInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Dimension">
		<xs:annotation>
			<xs:documentation>Information related to Length,Height,Width of a baggage.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeUnitOfMeasure">
					<xs:attribute name="type" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Type denotes Length,Height,Width of a baggage.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeWeight">
		<xs:attribute name="Value" type="xs:integer"/>
		<xs:attribute name="Unit" type="typeUnitWeight"/>
	</xs:complexType>
	<xs:complexType name="typeUnitOfMeasure">
		<xs:attribute name="Value" type="xs:float"/>
		<xs:attribute name="Unit" type="xs:string">
			<xs:annotation>
				<xs:documentation>Unit values would be lb,Lb,kg etc.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="AvailableDiscount">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:LoyaltyProgram" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Customer Loyalty Program Detail.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Amount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="Percent" type="common:typePercentageWithDecimal" use="optional"/>
			<xs:attribute name="Description" type="xs:string" use="optional"/>
			<xs:attribute name="DiscountQualifier" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- *************************************************************************************************************************************** -->
	<!-- *************************************************************************************************************************************** -->
	<!-- *************************************************************************************************************************************** -->
	<xs:element name="FareRuleKey">
		<xs:annotation>
			<xs:documentation>The Fare Rule requested using a Key. The key is
                typically a provider specific string which is required to make a
                following Air Fare Rule Request. This Key is returned in Low Fare
                Shop or Air Price Response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="common:typeNonBlanks">
					<xs:attribute name="FareInfoRef" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>The Fare Component to which this Rule Key
                                applies
                    </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirFareDisplayRuleKey">
		<xs:annotation>
			<xs:documentation>The Tariff Fare Rule requested using a Key. The
                key is typically a provider specific string which is required to
                make either a following Air Fare Tariff request for Mileage/Routing
                information or Air Fare Tariff Rule Request.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="common:typeNonBlanks">
					<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BookingCode">
		<xs:annotation>
			<xs:documentation>The Booking Code (Class of Service) for a
                segment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="common:typeClassOfService" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BookingInfo">
		<xs:annotation>
			<xs:documentation>Links segments and fares together
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="BookingCode" type="xs:string" use="required"/>
			<xs:attribute name="BookingCount" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Seat availability of the BookingCode</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CabinClass" type="xs:string" use="optional"/>
			<xs:attribute name="FareInfoRef" type="common:typeRef" use="required"/>
			<xs:attribute name="SegmentRef" type="common:typeRef" use="optional"/>
			<xs:attribute name="CouponRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>The coupon to which that booking is relative
                        (if applicable)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirItinerarySolutionRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to an Air Itinerary Solution
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="HostTokenRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>HostToken Reference for this segment and fare combination.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SponsoredFltInfo">
		<xs:annotation>
			<xs:documentation>This describes whether the segment is determined to be a sponsored flight. The SponsoredFltInfo node will only come back for Travelport UIs and not for other customers.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="SponsoredLNB" type="xs:nonNegativeInteger" use="required">
				<xs:annotation>
					<xs:documentation>The line number of the sponsored flight item</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NeutralLNB" type="xs:nonNegativeInteger" use="required">
				<xs:annotation>
					<xs:documentation>The neutral line number for the flight item.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FltKey" use="required">
				<xs:annotation>
					<xs:documentation>The unique identifying key for the sponsored flight.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CodeshareInfo">
		<xs:annotation>
			<xs:documentation>Describes the codeshare disclosure (simple
                text string) or the specific operating flight information (as
                attributes).</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="OperatingCarrier" type="common:typeCarrier" use="optional">
						<xs:annotation>
							<xs:documentation>The actual carrier that is operating the
                                flight.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="OperatingFlightNumber" type="common:typeFlightNumber" use="optional">
						<xs:annotation>
							<xs:documentation>The actual flight number of the carrier that
                                is operating the flight.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Connection">
		<xs:annotation>
			<xs:documentation>Flight Connection Information
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareNote" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="ChangeOfPlane" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates the traveler must change
                        planes between flights.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ChangeOfTerminal" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates the traveler must change
                        terminals between flights.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ChangeOfAirport" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates the traveler must change
                        airports between flights.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="StopOver" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates that there is a significant
                        delay between flights (usually 12 hours or more)
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MinConnectionTime" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation>The minimum time needed to connect between the
                        two different destinations.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Duration" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation>The actual duration (in minutes) between
                        flights.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SegmentIndex" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation>The sequential AirSegment number that this
                        connection information applies to.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FlightDetailsIndex" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation>The sequential FlightDetails number that this
                        connection information applies to.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IncludeStopOverToFareQuote" type="typeIgnoreStopOver" use="optional">
				<xs:annotation>
					<xs:documentation>The field determines to quote fares with or
                        without stop overs,the values can be NoStopOver,StopOver and
                        IgnoreSegment.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeIgnoreStopOver">
		<xs:annotation>
			<xs:documentation>The stop over inluded to quote fare.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="NoStopOver">
				<xs:annotation>
					<xs:documentation>No Stop over included.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StopOver">
				<xs:annotation>
					<xs:documentation>Stop over included.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IgnoreSegment">
				<xs:annotation>
					<xs:documentation>Segment Ignored.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Coupon">
		<xs:annotation>
			<xs:documentation>The flight coupon that resulted from the
                ticketing operation.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TicketDesignator" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="CouponNumber" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The sequential number of this coupon.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OperatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The true carrier.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OperatingFlightNumber" type="common:typeFlightNumber" use="optional">
				<xs:annotation>
					<xs:documentation>The true carrier's flight number.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MarketingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>If codeshare applies to this, this is the
                        marketing carrier (as opposed to the operating carrier).
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MarketingFlightNumber" type="common:typeFlightNumber" use="optional">
				<xs:annotation>
					<xs:documentation>If codeshare applies to this, this is the
                        marketing flight number (as opposed to the operating flight
                        number).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Origin" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the airport or city code that
                        defines the origin market for this fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>Returns the airport or city code that
                        defines the destination market for this fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DepartureTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>The date and time at which this entity
                        departs. This does not include time zone information since it can
                        be derived from the origin location. In case of open segment this
                        will not be returned.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ArrivalTime" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The date and time at which this entity arrives
                        at the destination. This does not include time zone information
                        since it can be derived from the origin location.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="StopoverCode" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Stopover code - indicator that stopover
                        is allowed at Origin Airport or City.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingClass" use="required">
				<xs:annotation>
					<xs:documentation>Booked fare class for coupon.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FareBasis" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The fare basis code for this fare
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NotValidBefore" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Fare not valid before this date.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NotValidAfter" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Fare not valid after this date.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" use="required">
				<xs:annotation>
					<xs:documentation>The status of this coupon returend from host is mapped as follows
                              Code="A" Status="Airport Controlled"
                              Code="C" Status="Checked In"
                              Code="F" Status="Flown/Used"
                              Code="L" Status="Boarded/Lifted"
                              Code="O" Status="Open"
                              Code="P" Status="Printed"
                              Code="R" Status="Refunded"
                              Code="E" Status="Exchanged"
                              Code="V" Status="Void"
                              Code="Z" Status="Archived/Carrier Modified"
                              Code="U" Status="Unavailable"
                              Code="S" Status="Suspended"
                              Code="I" Status="Irregular Ops"
                              Code="D" Status="Deleted/Removed"
                              Code="X" Status="Unknown"
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SegmentGroup" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the grouping in which this
                        segment resides based on Origin/Destination pairs in itinerary
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MarriageGroup" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Airline Marrraige group indicator
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PromoCode">
		<xs:annotation>
			<xs:documentation>A container to specify Promotional code with Provider code and Supplier code.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" use="required">
				<xs:annotation>
					<xs:documentation>To be used to specify Promotional Code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required">
				<xs:annotation>
					<xs:documentation>To be used to specify Provider Code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SupplierCode" type="common:typeSupplierCode" use="required">
				<xs:annotation>
					<xs:documentation>To be used to specify Supplier Code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SegmentIndex" type="xs:int">
		<xs:annotation>
			<xs:documentation>Identifies the segment that is part of
                this group</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TicketEndorsement">
		<xs:complexType>
			<xs:attribute name="Value" type="common:typeEndorsement" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TourCode">
		<xs:annotation>
			<xs:documentation>Tour Code Fare Basis</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Value" type="typeTourCode" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TCRInfo">
		<xs:complexType>
			<xs:attribute name="Status" type="typeTCRStatus" use="required"/>
			<xs:attribute name="Date" type="xs:dateTime" use="optional"/>
			<xs:attribute name="TCRNumber" type="typeTCRNumber" use="required">
				<xs:annotation>
					<xs:documentation>The identifying number for a Ticketless Air
                        Reservation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Provider reservation reference key.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:Name"/>
				<xs:element ref="ConjunctedTicketInfo" minOccurs="0" maxOccurs="3"/>
				<xs:element ref="ExchangedTicketInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Number" type="xs:string" use="required"/>
			<xs:attribute name="IATANumber" type="common:typeIATA" use="optional" />
			<xs:attribute name="TicketIssueDate" type="xs:dateTime"	use="optional" />
			<xs:attribute name="TicketingAgentSignOn" use="optional" >
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CountryCode" type="common:typeCountry" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Contains Ticketed PCC’s Country code.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" type="common:typeTicketStatus" use="required"/>
			<xs:attribute name="BulkTicket" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Whether the ticket was issued as bulk.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>A reference to a passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirPricingInfoRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to a AirPricing.Applicable Providers 1G and 1V.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
