	<xs:element name="PenaltyFareInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PenaltyInfo" type="typeFarePenalty" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Penalty Limit if requested.
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ProhibitPenaltyFares" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Indicates whether user wants penalty
