						<xs:attributeGroup ref="common:attrDOW"/>
						<xs:attribute name="RestrictionExistsInd" type="xs:boolean" use="optional"/>
						<xs:attribute name="Application" type="xs:string" use="optional"/>
						<xs:attribute name="IncludeExcludeUseInd" type="xs:boolean" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="RestrictionPassengerTypes" minOccurs="0" maxOccurs="999">
					<xs:complexType>
						<xs:attribute name="MaxNbrTravelers" type="xs:string" use="optional"/>
						<xs:attribute name="TotalNbrPTC" type="xs:string" use="optional"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocumentRequired">
		<xs:annotation>
			<xs:documentation>Additional Details, Documents , Project IDs</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="DocType" type="xs:string" use="optional"/>
			<xs:attribute name="IncludeExcludeUseInd" type="xs:boolean" use="optional"/>
			<xs:attribute name="DocId" type="xs:string" use="optional"/>
			<xs:attribute name="AllowedIds" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RoutingRules">
		<xs:annotation>
			<xs:documentation>Rules related to routing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Routing" minOccurs="0" maxOccurs="999">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DirectionInfo" minOccurs="0" maxOccurs="999">
								<xs:complexType>
									<xs:attribute name="LocationCode" type="common:typeIATACode"/>
									<xs:attribute name="Direction">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="To"/>
												<xs:enumeration value="From"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="RoutingConstructedInd" type="xs:boolean"/>
						<xs:attribute name="Number" type="xs:string"/>
						<xs:attribute name="RoutingRestriction" type="xs:string"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PrePayPriceInfo">
		<xs:annotation>
			<xs:documentation>Pricing detail for the Pre Pay Account</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Detailed tax information for the pre pay account</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="BaseFare" type="common:typeMoney" use="optional"/>
			<xs:attribute name="TotalFare" type="common:typeMoney" use="optional"/>
			<xs:attribute name="TotalTax" type="common:typeMoney" use="optional"/>
		</xs:complexType>
	</xs:element>
