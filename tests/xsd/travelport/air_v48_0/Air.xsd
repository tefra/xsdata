			<xs:sequence>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="BaseFare" type="common:typeMoney" use="optional"/>
			<xs:attribute name="TotalFare" type="common:typeMoney" use="optional"/>
			<xs:attribute name="TotalTax" type="common:typeMoney" use="optional"/>
			<xs:attribute name="EquivFare" type="common:typeMoney" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ElectronicMiscDocument">
		<xs:annotation>
			<xs:documentation>Electronic miscellaneous document. Supported providers are 1G/1V/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="EMDCoupon" maxOccurs="999">
