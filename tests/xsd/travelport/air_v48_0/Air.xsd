	<xs:element name="ContractCode">
		<xs:annotation>
			<xs:documentation>Some private fares (non-ATPCO) are secured to a
                contract code.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" use="required">
				<xs:annotation>
					<xs:documentation>The 1-64 character string which uniquely
                        identifies a Contract.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CompanyName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Providers supported : ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrProviderSupplier"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeTicketFailureInfo">
		<xs:annotation>
			<xs:documentation>Will be optionally returned as part if one or
                all ticketing requests fail.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="common:TicketNumber"/>
					<xs:element ref="common:Name"/>
				</xs:sequence>
				<xs:element name="TCRNumber" type="typeTCRNumber">
					<xs:annotation>
						<xs:documentation>The identifying number for a Ticketless Air
                            Reservation.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="BookingTravelerRef" type="common:typeRef" maxOccurs="999"/>
		</xs:sequence>
		<xs:attribute name="Code" type="xs:integer" use="required"/>
		<xs:attribute name="Message" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="typeFailureInfo">
		<xs:attribute name="Code" type="xs:integer" use="required"/>
		<xs:attribute name="Message" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:simpleType name="typeMealService">
		<xs:annotation>
			<xs:documentation>Available Meal Service</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Meal"/>
			<xs:enumeration value="ColdMeal"/>
			<xs:enumeration value="HotMeal"/>
			<xs:enumeration value="Breakfast"/>
			<xs:enumeration value="ContinentalBreakfast"/>
			<xs:enumeration value="Lunch"/>
			<xs:enumeration value="Dinner"/>
			<xs:enumeration value="SnackOrBrunch"/>
			<xs:enumeration value="FoodForPurchase"/>
			<xs:enumeration value="ComplimentaryRefreshments"/>
			<xs:enumeration value="AlcoholicBeveragesForPurchase"/>
			<xs:enumeration value="ComplimentaryAlcoholicBeverages"/>
			<xs:enumeration value="FoodAndBeveragesForPurchase"/>
			<xs:enumeration value="NoMealService"/>
			<xs:enumeration value="RefreshmentsForPurchase"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AirReservationLocatorCode">
		<xs:annotation>
			<xs:documentation>Identifies the AirReservation LocatorCode
                within the Universal Record</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="common:typeLocatorCode"/>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AvailableSSR">
		<xs:annotation>
			<xs:documentation>A wrapper for all the information regarding each
                of the available SSR</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:SSR" minOccurs="0" maxOccurs="999"/>
				<xs:element name="SSRRules" type="common:ServiceRuleType" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Holds the rules for selecting the SSR in
                            the itinerary</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:IndustryStandardSSR" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OptionalServices">
		<xs:annotation>
			<xs:documentation>A wrapper for all the information regarding each
                of the Optional services</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OptionalServicesTotal" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The total fares, fees and taxes associated
                            with the Optional Services</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999"/>
							<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999"/>
						</xs:sequence>
						<xs:attributeGroup ref="common:attrPrices"/>
					</xs:complexType>
				</xs:element>
				<xs:element ref="OptionalService" maxOccurs="999"/>
				<xs:element ref="GroupedOptionInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Details about an unselected or "other" option when optional services are grouped together.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OptionalServiceRules" type="common:ServiceRuleType" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Holds the rules for selecting the optional
                            service in the itinerary</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OptionalService">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Describes a merchandising service of a
                    given type (e.g. Seat) that can be purchased for the indicated
                    amount. If this service is for only a given passenger or segment
                    the references will indicate the restrictions. If this service
                    applies to all passenger or the entire itinerary, the references
                    will not be present.</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="common:ServiceData" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:ServiceInfo" minOccurs="0"/>
				<xs:element ref="common:Remark" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Information regarding any specific
                            for this service.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TaxInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="FeeInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="EMD" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="BundledServices" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="AdditionalInfo" minOccurs="0" maxOccurs="16"/>
				<xs:element ref="FeeApplication" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Specifies how the Optional Service fee is to be applied.  The choices are Per One Way, Per Round Trip, Per Item (Per Piece), Per Travel, Per Ticket, Per 1 Kilo, Per 5 Kilos.  Provider: 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Text" minOccurs="0" maxOccurs="4"/>
				<xs:element ref="PriceRange" minOccurs="0" maxOccurs="5"/>
				<xs:element ref="TourCode" minOccurs="0"/>
				<xs:element ref="BrandingInfo" minOccurs="0"/>
				<xs:element ref="Title" minOccurs="0" maxOccurs="2"/>
			</xs:sequence>
			<xs:attributeGroup ref="common:attrProviderSupplier"/>
			<xs:attribute name="OptionalServicesRuleRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>UniqueID to associate a rule to the
                        Optional Service</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Type" type="common:typeMerchandisingService" use="required">
				<xs:annotation>
					<xs:documentation>Specify the type of service offered (e.g.
                        seats, baggage, etc.)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Confirmation" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Confirmation number provided by the
                        supplier</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SecondaryType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The secondary option code type required for
                        certain options</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PurchaseWindow" type="common:typePurchaseWindow" use="optional">
				<xs:annotation>
					<xs:documentation>Describes when the Service is available
                        for confirmation or purchase (e.g. Booking Only, Check-in Only,
                        Anytime, etc.)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Priority" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Numeric value that represents the priority
                        order of the Service</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Available" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Boolean to describe whether the Service is
                        available for sale or not</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Entitled" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Boolean to describe whether the passenger
                        is entitled for the service without charge or not
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PerTraveler" type="xs:boolean" use="optional" default="true">
				<xs:annotation>
					<xs:documentation>Boolean to describe whether the Amount on
                        the Service is charged per traveler.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CreateDate" type="xs:dateTime" use="optional">
				<xs:annotation>
					<xs:documentation>Timestamp when this service/offer got
                        created.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PaymentRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to a payment for merchandising
                        services.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceStatus" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Specify the service status (e.g. active,
                        canceled, etc.)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Quantity" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The number of units availed for each optional
                        service (e.g. 2 baggage availed will be specified as 2 in quantity
                        for optional service BAGGAGE)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SequenceNumber" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The sequence number associated with the
                        OptionalService </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceSubCode" use="optional">
				<xs:annotation>
					<xs:documentation>The service subcode associated with the
                        OptionalService </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SSRCode" type="common:typeSSRCode" use="optional">
				<xs:annotation>
					<xs:documentation>The SSR Code associated with the
                        OptionalService </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IssuanceReason" use="optional">
				<xs:annotation>
					<xs:documentation>A one-letter code specifying the reason for
                        issuance of the OptionalService</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ProviderDefinedType" use="optional">
				<xs:annotation>
					<xs:documentation>Original Type as sent by the provider
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrPrices"/>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="AssessIndicator" type="typeAssessIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether price is assessed by mileage or currency or both</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Mileage" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates mileage fee/amount</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ApplicableFFLevel" use="optional">
				<xs:annotation>
					<xs:documentation>Numerical value of the loyalty card level for which this service is available.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Private" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Describes if service is private or not.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SSRFreeText" type="common:typeSSRFreeText" use="optional">
				<xs:annotation>
					<xs:documentation>Certain SSR types sent in OptionalService SSRCode require a free text message. For example, PETC Pet in Cabin.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IsPricingApproximate" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>When set to True indicates that the pricing returned is approximate. Supported providers are MCH/ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
			<xs:attribute name="Chargeable" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates if the optional service is not offered, is available for a charge, or is included in the brand</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InclusiveOfTax" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies if the service was filed with a fee that is inclusive of tax.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InterlineSettlementAllowed" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies if the interline settlement is allowed in service .</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GeographySpecification" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Sector, Portion, Journey.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExcessWeightRate" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The cost of the bag per unit weight.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Source" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The Source of the optional service. The source can be ACH, MCE, or MCH.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ViewableOnly" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Describes if the OptionalService is viewable only or not.
                           If viewable only then the service cannot be sold.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DisplayText" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Title of the Optional Service.  Provider: ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="WeightInExcess" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The excess weight of a bag. Providers: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TotalWeight" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The total weight of a bag. Providers: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BaggageUnitPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The per unit price of baggage. Providers: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FirstPiece" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the minimum occurrence of excess baggage.Provider: 1G, 1V, 1P, 1J.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LastPiece" type="xs:int" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the maximum occurrence of excess baggage. Provider: 1G, 1V, 1P, 1J.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Restricted" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>When set to “true”, the Optional Service is restricted by an embargo. Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IsRepriceRequired" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>When set to “true”, the Optional Service must be re-priced. Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookedQuantity" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the Optional Service quantity already booked. Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Group" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Associates Optional Services with the same ServiceSub Code, Air Segment, Passenger, and EMD Associated Item. Provider:1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="common:typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>
                        The PCC or SID that booked the Optional Service.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Tag" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Optional service group name.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="DisplayOrder" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Optional service group display order.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="999"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BrandingInfo">
		<xs:annotation>
			<xs:documentation> Branding information for the Ancillary Service.  Returned in Seat Map only.  Providers: 1G, 1V, 1P, 1J, ACH 
</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PriceRange" minOccurs="0" maxOccurs="5">
					<xs:annotation>
						<xs:documentation>The price range of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Text" minOccurs="0" maxOccurs="99"/>
				<xs:element ref="Title" minOccurs="0" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>The additional titles associated to the brand or optional service. Providers: ACH, 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ImageLocation" minOccurs="0" maxOccurs="3"/>
				<xs:element ref="ServiceGroup" minOccurs="0"/>
				<xs:element name="AirSegmentRef" type="common:typeSegmentRef" maxOccurs="99">
					<xs:annotation>
						<xs:documentation>Specifies the AirSegment the branding information is for. Providers: ACH, 1G, 1V, 1P, 1J</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attribute name="ServiceSubCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The Service Sub Code of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH 
</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExternalServiceName" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The external name of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The type of Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CommercialName" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The commercial name of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Chargeable" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates if the optional service is not offered, is available for a charge, or is included in the brand.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ServiceGroup">
		<xs:annotation>
			<xs:documentation>The Service Group of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ServiceSubGroup" minOccurs="0" maxOccurs="15"/>
			</xs:sequence>
			<xs:attribute name="Code" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The Service Group Code of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ServiceSubGroup">
		<xs:annotation>
			<xs:documentation>The Service Sub Group of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="xs:string">
				<xs:annotation>
					<xs:documentation>The Service Sub Group Code of the Ancillary Service.  Providers: 1G, 1V, 1P, 1J, ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FeeApplication">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Describes how the fees are to be applied.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="typeFeeApplication">
					<xs:attribute name="Code" use="optional">
						<xs:annotation>
							<xs:documentation>The code associated to the fee application. The  choices are: 1, 2, 3, 4, 5, K, F</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BundledServices">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BundledService" minOccurs="0" maxOccurs="16"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BundledService">
		<xs:annotation>
			<xs:documentation>Displays the services bundled together</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>Carrier the service is applicable.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CarrierSubCode" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Carrier sub code. True means the carrier used their own sub code. False means the carrier used an ATPCO sub code </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The type of service or what the service is used for, e.g. F type is flight type, meaning the service is used on a flight </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceSubCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The sub code of the service, e.g. OAA for Pre paid baggage </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Name" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Name of the bundled service.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Booking" use="optional">
				<xs:annotation>
					<xs:documentation>Booking method for the bundled service, e..g SSR.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="typeBooking"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Occurrence" type="xs:positiveInteger" use="optional">
				<xs:annotation>
					<xs:documentation>How many of the service are included in the bundled service.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AdditionalInfo">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Displays additional text about the service.</xs:documentation>
			</xs:annotation>
			<xs:attribute name="Category" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The category code is the code the AdditionalInfo text came from, e.g. S5 or S7.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="EMD">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>A container for EMD elements related to an
                    OptionalService</xs:documentation>
			</xs:annotation>
			<xs:attribute name="FulfillmentType" use="optional">
				<xs:annotation>
					<xs:documentation>A one digit code specifying how the service must be fulfilled.
                        See FulfillmentTypeDescription for the description of this value.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FulfillmentTypeDescription" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>EMD description.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AssociatedItem" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The type of Optional Service.  The choices are Flight, Ticket, Merchandising, Rule Buster, Allowance, Chargeable Baggage, Carry On Baggage Allowance, Prepaid Baggage.  Provider: 1G, 1V, 1P, 1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AvailabilityChargeIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>A one-letter code specifying whether the service
                        is available or if there is a charge associated with it.
                        X = Service not available
                        F = No charge for service (free) and an EMD is not issued to
                        reflect free service
                        E = No charge for service (free) and an EMD is issued to reflect
                        the free service.
                        G = No charge for service (free), booking is not required and an
                        EMD is not issued to reflect free service
                        H = No charge for service (free), booking is not required, and an
                        EMD is issued to reflect the free service.
                        Blank = No application. Charges apply according to the data in the
                        Service Fee fields.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="X"/>
						<xs:enumeration value="E"/>
						<xs:enumeration value="F"/>
						<xs:enumeration value="G"/>
						<xs:enumeration value="H"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="RefundReissueIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>An attribute specifying whether the service is
                        refundable or reissuable.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Refundable"/>
						<xs:enumeration value="NonRefundable"/>
						<xs:enumeration value="Reuse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Commissionable" use="optional">
				<xs:annotation>
					<xs:documentation>True/False value to whether or not the
                        service is comissionable.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="MileageIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>True/False value to whether or not the
                        service has miles.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Location" use="optional">
				<xs:annotation>
					<xs:documentation>3 letter location code where the service will be availed.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="common:typeIATACode"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Date" use="optional">
				<xs:annotation>
					<xs:documentation>The date at which the service will be used.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:dateTime"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Booking" use="optional">
				<xs:annotation>
					<xs:documentation>Holds the booking description for the service, e.g., SSR.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="typeBooking"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="DisplayCategory" use="optional">
				<xs:annotation>
					<xs:documentation>Describes when the service should be displayed.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="typeDisplayCategory"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Reusable" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies if the service can be re-used towards a future purchase.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeAssessIndicator">
		<xs:annotation>
			<xs:documentation>The type of AssessIndicator</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="MileageAndCurrency"/>
			<xs:enumeration value="MileageOrCurrency"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeFeeApplication">
		<xs:annotation>
			<xs:documentation>The type of FeeApplication</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="typeBooking">
		<xs:annotation>
			<xs:documentation>Type of booking</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SSR"/>
			<xs:enumeration value="Auxillary Segment"/>
			<xs:enumeration value="Available for Display/Pricing"/>
			<xs:enumeration value="Contact Carrier for Booking"/>
			<xs:enumeration value="No Booking Required"/>
			<xs:enumeration value="Apply booking per service"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeDisplayCategory">
		<xs:annotation>
			<xs:documentation>Type of booking</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="With Itinerary Pricing"/>
			<xs:enumeration value="Store"/>
			<xs:enumeration value="SpecialService"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="PrintBlankFormItinerary">
		<xs:annotation>
			<xs:documentation>Produce a customized itinerary/Invoice document in blank form format.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="IncludeDescription" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>If it is true then document will be printed including descriptions.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IncludeHeader" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>If it is true then document will be printed including it's header.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketingModifiers">
		<xs:annotation>
			<xs:documentation>A container to identify individual ticketing
            modifiers.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BookingTravelerRef" type="common:typeRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Reference to a booking traveler for which ticketing modifier is applied.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NetRemit" type="typeTicketModifierAmountType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to override the net
                     remittance amount - varies by BSP agreement</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NetFare" type="typeTicketModifierAmountType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Net Fare amount for a ticketed fare
                  </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ActualSellingFare" type="typeTicketModifierAmountType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to report an Actual
                     Selling Fare as part of the net remittance BSP agreement
                  </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="InvoiceFare" type="typeTicketModifierAccountingType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to report an Invoice Fare
                     as part of the net remittance BSP agreement</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CorporateDiscount" type="typeTicketModifierAccountingType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to add a corporate
                     discount to the itinerary to be ticketed </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AccountingInfo" type="typeTicketModifierAccountingType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to report Accounting
                     Information as part of the net remittance BSP agreement
                  </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BulkTicket" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to update the fare as a
                     Bulk ticket - Optional SuppressOnFareCalc attribute will control
                     how fare calculation is printed on the ticket</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="typeBulkTicketModifierType">
								<xs:attribute name="NonRefundable" type="xs:boolean" use="optional">
									<xs:annotation>
										<xs:documentation>Indicates bulk ticket being
                                 non-refundable</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="GroupTour" type="typeBulkTicketModifierType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to update the fare as a
                     Group Tour (inclusive tour) ticket - Optional SuppressOnFareCalc
                     attribute will control how fare calculation is printed on the
                     ticket</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:Commission" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to update the commission
                     to a new or different commission rate which will be applied at
                     time of ticketing. The commission Modifier allows the user
                     specify how the commission change is to applied
                  </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TourCode" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to modify the tour code
                     information on the ticket</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TicketEndorsement" minOccurs="0" maxOccurs="3">
					<xs:annotation>
						<xs:documentation>Allows an agency to add user defined
                     ticketing endorsements the ticket</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ValueModifier" type="typeTicketModifierValueType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Allows an agency to modify value or
                     commission of the ticket. The modifier is generic and depends on
                     the specific GDS and BSP implementation</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="DocumentSelect" minOccurs="0"/>
				<xs:element ref="DocumentOptions" minOccurs="0"/>
				<xs:element ref="SegmentSelect" minOccurs="0"/>
				<xs:element ref="SegmentModifiers" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:SupplierLocator" minOccurs="0"/>
				<xs:element ref="DestinationPurposeCode" minOccurs="0"/>
				<xs:element ref="LanguageOption" minOccurs="0" maxOccurs="2"/>
				<xs:element ref="LandCharges" minOccurs="0"/>
				<xs:element ref="PrintBlankFormItinerary" minOccurs="0"/>
				<xs:element ref="ExcludeTicketing" minOccurs="0"/>
				<xs:element ref="ExemptOBFee" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<xs:attribute name="IsPrimaryDI" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates if the DI is Primary DI. 1P only</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DocumentInstructionNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The Document Instruction line number. 1P only</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Reference" type="common:StringLength1to30" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies if TicketingModifiers contains DI line information. 1P only.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" use="optional">
				<xs:annotation>
					<xs:documentation>DI line status - ex:Ticketed</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="30"/>
				</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FreeText" use="optional">
				<xs:annotation>
					<xs:documentation>DI line information shown as free text as in Host. 1P only</xs:documentation>
				</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
                  <xs:maxLength value="756"/>
                </xs:restriction>
			</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="NameNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Host Name Number</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketRecord" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Ticket Record Number</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PlatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>Allows an agency to specify the Plating
                  Carrier for ticketing</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExemptVAT" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Allows an agency to update if VAT is
                  Exemtped on the fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NetRemitApplied" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicator to the BSP net remittance
                  scheme applies to ticketed fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FreeTicket" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates free ticket.
               </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CurrencyOverrideCode" use="optional">
				<xs:annotation>
					<xs:documentation>This modifier allows an agency to specify the currency like L for Local, E for Euro, U for USD, C for CAD (Canadian dollars).
               </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExemptOBFee">
		<xs:annotation>
			<xs:documentation>Used to specify which OB fees are exempt; if none are listed, it means all should be exempt.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SubCode" type="typeSubCode" minOccurs="0" maxOccurs="8"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeSubCode">
		<xs:annotation>
			<xs:documentation>Used to specify an OB fee as exempt.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:element name="ExcludeTicketing">
		<xs:annotation>
			<xs:documentation>Exclude ticketing of traveler referenced. Supported Provider: JAL.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BookingTravelerRef" type="common:typeRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Reference to a booking traveler for which ticketing modifier is applied.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LandCharges">
		<xs:annotation>
			<xs:documentation>Prints non-air charges on a document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Tax" minOccurs="0" maxOccurs="3"/>
			</xs:sequence>
			<xs:attribute name="Base" type="common:typeMoney" use="optional"/>
			<xs:attribute name="Total" type="common:typeMoney" use="optional"/>
			<xs:attribute name="Miscellaneous" type="common:typeMoney" use="optional"/>
			<xs:attribute name="PrePaid" type="common:typeMoney" use="optional"/>
			<xs:attribute name="Deposit" type="common:typeMoney" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Tax">
		<xs:annotation>
			<xs:documentation>Taxes for Land Charges</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Category" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The tax category represents a valid IATA tax code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Amount" type="common:typeMoney" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocumentOptions">
		<xs:annotation>
			<xs:documentation>Allows an agency to set different document
                options for the itinerary.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PassengerReceiptOverride" minOccurs="0"/>
				<xs:element name="OverrideOption" type="typeOverrideOption" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Allows an agency to override print options for documents during document generation.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="SuppressItineraryRemarks" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation> True when itinerary remarks are suppressed.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GenerateItinNumbers" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>True when itinerary numbers are system
                        generated.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeOverrideOption">
		<xs:annotation>
			<xs:documentation>Allows an agency to override print options
                for documents options during document generation.
            </xs:documentation>
			<xs:documentation>Below mentioned values are only supported in schema.
   
                "SuppressItineraryInvoicePrinting" - Suppress sending itinerary/invoice document to a printer. 
                    The itinerary/invoice can be sent to a printer at a
                    future time using duplicate itinerary/invoice. 
                    This is used by Worldspan.
   
                "PrintTerminalCodes" - Produces an itinerary/invoice or pocket
                    itinerary that includes departure and /or arrival terminal codes.
                    This is used by Worldspan.

                "PrintDirectAccessRecordLocator" - Print record locator for direct access carriers. 
                    This option prints the record locator below the segmnet information. 
                    This option can be used along with PrintProviderReservationRecordLocator as well. 
                    This is used by Worldspan.

                "PrintProviderReservationRecordLocator" - Print PNR record locator on documents. 
                    This option can be used along with PrintDirectAccessRecordLocator as well. 
                    This is used by Worldspan.
   
                "ReIssueTicketedStoredFare" - This modifier is used to issue a ticket against a 
                    ticket record that was previously issued. 
                    This is used by Worldspan.
   
                "PrintFrequentTravelerNumber" - This modifier is used to print frequent traveller number.
                    This is used by Worldspan.
   
                "SuppressInvoiceNumberPrinting" - This modifier prevents the invoice number from printing on documents. 
                    This is used by Worldspan.
   
                "PrintItineraryInvoicePerTraveler" - Issues itinerary/invoice for per traveler. 
                    This is used by Worldspan.
   
                "PrintItineraryInvoicePerSurname" - Issues itinerary/invoice for per surname. 
                    This is used by Worldspan.
   
                "PrintMultipleCustomizedNameData" - Prints names individually with associated 
                    customized name data on documents. 
                    This is used by Worldspan.
   
                "PrintInvoiceRemarkOnly" - Prints invoice remark only. overrides the system remarks and  
                    directs only specific remarks selected to print on documents. 
                    This is used by Worldspan.
             
                "PrintItineraryRemarkOnly" - Prints itinerary remark only. 
                    This is used by Worldspan.
                
                "PrintBaggageAllowance" - Print the baggage allowance field with the autoprice or stored in the 
                    ticket record on credit memos/pocket Itineraries or itinerary invoices issued in conjunction
                    with a non electronic ticket. 
                    This is used by Worldspan.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="PassengerReceiptOverride">
		<xs:annotation>
			<xs:documentation>It is required when a passenger receipt is
                required immediately ,GDS overrides the default value
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="common:typeNonBlanks"/>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SegmentSelect">
		<xs:annotation>
			<xs:documentation>To be used to pass the selected segment. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AirSegmentRef" type="typeSegmentRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Reference to AirSegment from an Air Reservation.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HotelReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Specify the locator code of Hotel reservation if it needs to be considered as Auxiliary segment</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="VehicleReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Specify the locator code of Vehicle reservation if it needs to be considered as Auxiliary segment</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PassiveSegmentRef" type="typeSegmentRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Reference to PassiveSegment from a Passive Reservation.Specify the passive segment if it needs to be considered as Auxiliary segment</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="AllConfirmedAir" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Set to true to consider all Confirmed segments including active and passive and set to false to discard confirmed segments</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AllWaitlistedAir" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Set to true to consider all Waitlisted segments and false to discard all waitlisted segments</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AllHotel" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Set to true to consider all Hotel reservations as Auxiliary segment and false to discard all Hotel reservations</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AllVehicle" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Set to true to consider all Vehicle reservations as Auxiliary segment and false to discard all Vehicle reservations</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AllPassive" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Set to true to consider all Passive segments as Auxiliary segment and false to discard passive segments</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeNonAirReservationRef">
		<xs:attribute name="LocatorCode" type="common:typeLocatorCode" use="required"/>
	</xs:complexType>
	<xs:complexType name="typeSegmentRef">
		<xs:attribute name="Key" type="common:typeRef" use="required"/>
	</xs:complexType>
	<xs:element name="BookingCodeInfo">
		<xs:annotation>
			<xs:documentation>Details Cabin class info and class of
                service information with availability counts. Only provided on
                search results and grouped by Cabin class</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="CabinClass" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Specifies Cabin class for a group of
                        class of services. Cabin class is not identified if it is not
                        present.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingCounts" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Lists class of service and their counts for
                        specific cabin class</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeTicketModifierValueType">
		<xs:annotation>
			<xs:documentation>Ticketing Modifier used to add value
                discount information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value" type="xs:string"/>
		</xs:sequence>
		<xs:attribute name="NetFareValue" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Treat the value as net fare discount
                    information</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="DocumentSelect">
		<xs:annotation>
			<xs:documentation>Allows an agency to select the documents to
                produce for the itinerary.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BackOfficeHandOff" minOccurs="0"/>
				<xs:element ref="Itinerary" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="IssueTicketOnly" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Set to true to alter system default of
                        itinerary,ticket and back office.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IssueElectronicTicket" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Set to true for electronic tickets.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FaxIndicator" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Set to true for providing fax details.
                   </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeItinerary">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Invoice"/>
			<xs:enumeration value="Pocket"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeItineraryOption">
		<xs:restriction base="xs:string">
			<xs:enumeration value="NoFare"/>
			<xs:enumeration value="NoAmount"/>
			<xs:enumeration value="SequenceNumber"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Itinerary">
		<xs:annotation>
			<xs:documentation>Allows an agency to select the itinenary
                option for ticket.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" type="typeItinerary" use="optional">
				<xs:annotation>
					<xs:documentation>Specifies the type of itinenary option
                        for ticket like Invoice type or Pocket itinenary.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Option" type="typeItineraryOption" use="optional">
				<xs:annotation>
					<xs:documentation>Specifies the itinerary option like
                        NoFare,NoAmount.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SeparateIndicator" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Set to true if one itinerary to be printed per
                        passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BackOfficeHandOff">
		<xs:annotation>
			<xs:documentation>Allows an agency to select the back office
                documents and also route to different host to produce for the
                itinerary.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" type="typeBackOffice" use="required">
				<xs:annotation>
					<xs:documentation>The type of back office document,valid options
                        are Accounting,Global,NonAccounting,NonAccountingRemote,Dual.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Location" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>This is required for NonAccountingRemote,Dual
                        and Global type back office.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="common:typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>The PCC of the host system where it would be
                        routed.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeBackOffice">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Accounting"/>
			<xs:enumeration value="Global"/>
			<xs:enumeration value="NonAccounting"/>
			<xs:enumeration value="NonAccountingRemote"/>
			<xs:enumeration value="Dual"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AirPricingTicketingModifiers">
		<xs:annotation>
			<xs:documentation>AirPricing TicketingModifier information
                - used to associate Ticketing Modifiers with one or more
                AirPricingInfos/ProviderReservationInfo</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingInfoRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TicketingModifiers"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeBulkTicketModifierType">
		<xs:annotation>
			<xs:documentation>Bulk ticketing modifier type.
            </xs:documentation>
		</xs:annotation>
		<xs:attribute name="SuppressOnFareCalc" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Optional attribute to allow a modifier
                    impact such as Bulk Ticketing to have information suppressed on the
                    Fare Calc when generating supporting documents Check the specific
                    host system which may or may not support this function
                </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeTicketModifierAmountType">
		<xs:annotation>
			<xs:documentation>Ticketing Modifier used to alter a fare
                amount before or during the ticketing operation.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Amount" type="common:typeMoney" use="required">
			<xs:annotation>
				<xs:documentation>Amount associated with a ticketing modifier
                </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeTicketModifierPercentType">
		<xs:annotation>
			<xs:documentation>Ticketing Modifier used to alter a fare
                percentage before or during the ticketing operation.
            </xs:documentation>
		</xs:annotation>
		<xs:attribute name="Percent" type="common:typePercentageWithDecimal" use="required">
			<xs:annotation>
				<xs:documentation>Percent associated with a ticketing
                    modifier</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeTicketModifierAccountingType">
		<xs:annotation>
			<xs:documentation>Ticketing Modifier used to add accounting
                - discount information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TicketingModifiersRef">
		<xs:annotation>
			<xs:documentation>Reference to a shared list of Ticketing
                Modifers</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AsyncProviderSpecificResponse" type="common:BaseAsyncProviderSpecificResponse">
		<xs:annotation>
			<xs:documentation>Identifies pending responses from a specific provider using MoreResults attribute</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:attributeGroup name="attrLinkInfo">
		<xs:annotation>
			<xs:documentation>Holds Participant Level information.
            </xs:documentation>
		</xs:annotation>
		<xs:attribute name="ParticipantLevel" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Type of sell agreement between host and link
                    carrier.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LinkAvailability" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates if carrier has link (carrier
                    specific) display option.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PolledAvailabilityOption" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates if carrier has Inside
                    (polled)Availability option.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AvailabilityDisplayType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The type of availability from which the segment is sold.Possible Values (List):
                                    G - General
                                    S - Flight Specific
                                    L - Carrier Specific/Direct Access
                                    M - Manual Sell
                                    F - Fare Shop/Optimal Shop
                                    Q - Fare Specific Fare Quote unbooked
                                    R - Redemption Availability used to complete the sell. Supported Providers: 1G,1V.
                </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="OptionalServicesInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingSolution"/>
				<xs:element ref="common:FormOfPayment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:FormOfPaymentRef" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--<xs:simpleType name="typeTicketNumber">
        <xs:annotation>
            <xs:documentation>Reference Ticket Number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="13"/>
        </xs:restriction>
    </xs:simpleType>-->
	<xs:simpleType name="typePassengerTicketNumber">
		<xs:annotation>
			<xs:documentation>Reference Ticket Number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="13"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="IncludeAddlBookingCodeInfo">
		<xs:annotation>
			<xs:documentation>Used to include primary or secondary carrier's
                booking code details</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" type="typeCarrierCode" use="required">
				<xs:annotation>
					<xs:documentation>The type defines that the booking code info is
                        for primary or secondary carrier.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SecondaryCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The secondary carrier code is required when
                        type is secondary .</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeCarrierCode">
		<xs:annotation>
			<xs:documentation>Defines the type of booking codes (Primary
                or Secondary)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Primary"/>
			<xs:enumeration value="Secondary"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Adjustment">
		<xs:annotation>
			<xs:documentation>An indentifier which indentifies adjustment made
                on original pricing. It can a flat amount or percentage of original
                price. The value of Amount/Percent can be negetive. Negative value
                implies a discount.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="Amount" type="common:typeMoney">
					<xs:annotation>
						<xs:documentation>Implies a flat amount to be adjusted.
                            Negetive value implies a discount.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Percent" type="xs:float">
					<xs:annotation>
						<xs:documentation>Implies an adjustment to be made on
                            original price. Negetive value implies a discount.
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="AdjustedTotalPrice" type="common:typeMoney" use="required">
				<xs:annotation>
					<xs:documentation>The adjusted price after applying adjustment
                        on Total price</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ApproximateAdjustedTotalPrice" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The Converted adjusted total price in Default
                        Currency for this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to a booking traveler for which adjustment is applied.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Yield">
		<xs:annotation>
			<xs:documentation>An identifier which identifies yield made
                on original pricing. It can be a flat amount of original
                price. The value of Amount can be negative. Negative value
                implies a discount.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Amount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Yield per passenger level in Default
                        Currency for this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to a booking traveler for which Yield is applied.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeAvailabilitySource">
		<xs:annotation>
			<xs:documentation>Availability Source value for Sell.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="FaxDetailsInformation">
		<xs:annotation>
			<xs:documentation>Container to send Fax details Information for ticketing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirPricingInfoRef" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Returns related air pricing infos.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="FaxDetails"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TermConditions">
		<xs:annotation>
			<xs:documentation>The terms and conditions to be included in Fax details</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LanguageOption" minOccurs="0" maxOccurs="2"/>
			</xs:sequence>
			<xs:attribute name="IncludeTermConditions" type="xs:boolean" use="required">
				<xs:annotation>
					<xs:documentation>Specifies whether Term and Conditions included in the Fax or not .</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="LanguageOption">
		<xs:annotation>
			<xs:documentation>Enables itineraries and invoices to print in different languages.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Language" type="common:typeLanguage" use="required">
				<xs:annotation>
					<xs:documentation>2 Letter ISO Language code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Country" type="common:typeCountry" use="required">
				<xs:annotation>
					<xs:documentation>2 Letter ISO Country code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FaxDetails">
		<xs:annotation>
			<xs:documentation>The Fax Details Information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:PhoneNumber">
					<xs:annotation>
						<xs:documentation>Send type as Fax for fax number.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="TermConditions" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Term and Conditions for the fax .</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="common:Remark" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="IncludeCoverSheet" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Specifies whether to include a cover page with fax or not.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="To" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>To address.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="From" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>From address.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DeptBillingCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Department billing code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InvoiceNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Invoice number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DetailedBillingInformation">
		<xs:annotation>
			<xs:documentation>Container to send Detailed Billing
                Information for ticketing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:FormOfPaymentRef" minOccurs="0"/>
				<xs:element ref="AirPricingInfoRef" minOccurs="1" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Returns related air pricing infos.
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="BillingDetailItem" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BillingDetailItem">
		<xs:annotation>
			<xs:documentation>The Billing Details Information
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Name" type="typeBillingDetailsName" use="required">
				<xs:annotation>
					<xs:documentation>Detailed Billing Information Name(e.g
                        Personal ID, Account Number)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DataType" type="typeBillingDetailsDataType" use="required">
				<xs:annotation>
					<xs:documentation>Detailed Billing Information DataType
                        (Alpha, Numeric, etc.)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MinLength" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Detailed Billing Information Minimum
                        Length.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MaxLength" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Detailed Billing Information Maximum
                        Length.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Value" type="xs:string">
				<xs:annotation>
					<xs:documentation>Detailed Billing Information Value
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeBillingDetailsName">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PersonalId"/>
			<xs:enumeration value="CostAccountNumber"/>
			<xs:enumeration value="AccountNumber"/>
			<xs:enumeration value="ProjectNumber"/>
			<xs:enumeration value="ActionCode"/>
			<xs:enumeration value="DepartmentCode"/>
			<xs:enumeration value="AccountingUnit"/>
			<xs:enumeration value="OrderNumber"/>
			<xs:enumeration value="Destination"/>
			<xs:enumeration value="FileDate"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeBillingDetailsDataType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Alpha"/>
			<xs:enumeration value="Numeric"/>
			<xs:enumeration value="AlphaNumeric"/>
			<xs:enumeration value="Date"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AirPricingAdjustment">
		<xs:annotation>
			<xs:documentation>This is a container to adjust price of
                AirPricingInfo. Pass zero values to remove existing adjustment.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Adjustment"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Key of AirPricingInfo from booking.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeFareBreak">
		<xs:annotation>
			<xs:documentation>Types of fare break.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="MustBreak">
				<xs:annotation>
					<xs:documentation>Break Fare at the associated segment.
                        Multiple Breaks or No Breaks may be allowed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MustOnlyBreak">
				<xs:annotation>
					<xs:documentation>Only Break Fare at the associated segment.
                        Fare Break in the entire itinerary is allowed only at the
                        concerned segment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MustNotBreak">
				<xs:annotation>
					<xs:documentation>No Fare Break allowed at the associated
                        segment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
