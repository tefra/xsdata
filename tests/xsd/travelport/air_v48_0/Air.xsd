	<xs:element name="Document">
		<xs:annotation>
			<xs:documentation>APIS Document Details.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Sequence" type="xs:positiveInteger" use="optional">
				<xs:annotation>
					<xs:documentation>Sequence number for the document.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Type" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Type of the Document. Visa, Passport,
                        DriverLicense etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Level" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Applicability level of the Document.
                        Required, Supported, API_Supported or Unknown.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="APISRequirements">
		<xs:annotation>
			<xs:documentation>Specific details for APIS Requirements.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Document" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key">
				<xs:annotation>
					<xs:documentation>Unique identifier for this APIS
                        Requirements - use this key when a single APIS Requirements is
                        shared by multiple elements. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Level" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Applicability level of the Document.
                        Required, Supported, API_Supported or Unknown</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GenderRequired" type="xs:boolean" use="optional"/>
			<xs:attribute name="DateOfBirthRequired" type="xs:boolean" use="optional"/>
			<xs:attribute name="RequiredDocuments" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>What are required documents for the APIS
                        Requirement. One, FirstAndOneOther or All</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NationalityRequired" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Nationality of the traveler is required for booking for some suppliers.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="APISRequirementsList">
		<xs:annotation>
			<xs:documentation>The shared object list of APISRequirements
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="APISRequirements" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SegmentModifiers">
		<xs:annotation>
			<xs:documentation>To be used to modify the ticket modifiers for air
                segment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentRef"/>
				<xs:element ref="TicketValidity" minOccurs="0">
					<xs:annotation>
						<xs:documentation>To be used to pass the ticket validity dates
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="BaggageAllowance" minOccurs="0"/>
				<xs:element name="TicketDesignator" type="typeTicketDesignator" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketValidity">
		<xs:annotation>
			<xs:documentation>To be used to pass the selected segment
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="NotValidBefore" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Fare not valid before this date.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NotValidAfter" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Fare not valid after this date.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Meals" type="typeMealService">
		<xs:annotation>
			<xs:documentation>Available Meal Service.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InFlightServices" type="xs:string">
		<xs:annotation>
			<xs:documentation>Available InFlight Services. They are: 'Movie', 'Telephone', 'Telex', 'AudioProgramming', 'Television' ,'ResvBookingService' ,'DutyFreeSales' ,'Smoking' ,'NonSmoking' ,'ShortFeatureVideo' ,'NoDutyFree' ,'InSeatPowerSource' ,'InternetAccess' ,'Email' ,'Library' ,'LieFlatSeat' ,'Additional service(s) exists' ,'WiFi' ,'Lie-Flat seat first' ,'Lie-Flat seat business' ,'Lie-Flat seat premium economy' ,'Amenities subject to change' etc.. These follow the IATA standard. Please see the IATA standards for a more complete list.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="typeVarianceType">
		<xs:annotation>
			<xs:documentation>Type code for Variance.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Actual"/>
			<xs:enumeration value="Estimated"/>
			<xs:enumeration value="Canceled"/>
			<xs:enumeration value="Diversion"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeVarianceIndicator">
		<xs:annotation>
			<xs:documentation>Type code for Variance.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Early"/>
			<xs:enumeration value="Late"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Variance">
		<xs:annotation>
			<xs:documentation>Indicates any variance in the requested
                flight.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" type="typeVarianceType" use="required">
				<xs:annotation>
					<xs:documentation>Indicates type Variance, i.e. Actual,
                        Estimated, Canceled and Diversion.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Time" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates time for Variance.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Indicator" type="typeVarianceIndicator" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates VAriance Indicator, i.e.
                        Early, Late.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Reason" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Reason for Variance</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FlightInfoDetail" minOccurs="0" maxOccurs="999"/>
				<xs:element name="FlightInfoErrorMessage" type="common:typeResultMessage" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Errors, Warnings and informational
                            messages for the Flight referenced above.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="CriteriaKey" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>An identifier to link the flightinfo responses
                        to the criteria in request. The value populated here is passed in
                        request.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="required">
				<xs:annotation>
					<xs:documentation>The carrier that is marketing this segment
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FlightNumber" type="common:typeFlightNumber" use="required">
				<xs:annotation>
					<xs:documentation>The flight number under which the marketing
                        carrier is marketing this flight</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Origin" type="common:typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>The IATA location code for this origination of
                        this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>The IATA location code for this destination of
                        this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DepartureDate" type="xs:date" use="required">
				<xs:annotation>
					<xs:documentation>The date at which this entity departs. This
                        does not include time zone information since it can be derived
                        from the origin location.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ClassOfService" type="common:typeClassOfService" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightInfoDetail">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CodeshareInfo" minOccurs="0"/>
				<xs:element ref="Meals" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="InFlightServices" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="Variance" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Origin" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The IATA location code for this origination of
                        this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The IATA location code for this destination of
                        this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ScheduledDepartureTime" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The date and time at which this entity is
                        scheduled to depart. This does not include time zone information
                        since it can be derived from the origin location.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ScheduledArrivalTime" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The date and time at which this entity is
                        scheduled to arrive at the destination. This does not include time
                        zone information since it can be derived from the origin location.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelTime" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Total time spent (minutes) traveling
                        including flight time and ground time.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ETicketability" type="typeEticketability" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies if this particular segment
                        is E-Ticketable</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Equipment" type="typeEquipment" use="optional"/>
			<xs:attribute name="OriginTerminal" type="xs:string" use="optional"/>
			<xs:attribute name="OriginGate" use="optional">
				<xs:annotation>
					<xs:documentation>To be used to display origin flight gate number</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="DestinationTerminal" type="xs:string" use="optional"/>
			<xs:attribute name="DestinationGate" use="optional">
				<xs:annotation>
					<xs:documentation>To be used to display destination flight gate number</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="AutomatedCheckin" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>“True” indicates that the flight allows automated check-in. The default is “False”.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FlightInfoCriteria">
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>An identifier to link the flightinfo responses
                        to the criteria. The value passed here will be returned in the
                        resulting flightinfo(s) from this command.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="required">
				<xs:annotation>
					<xs:documentation>The carrier that is marketing this segment
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FlightNumber" type="common:typeFlightNumber" use="required">
				<xs:annotation>
					<xs:documentation>The flight number under which the marketing
                        carrier is marketing this flight</xs:documentation>
