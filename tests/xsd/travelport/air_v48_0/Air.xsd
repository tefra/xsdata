	<xs:simpleType name="typeFareRuleCategoryCode">
		<xs:annotation>
			<xs:documentation>Kestrel Long Fare Rule Category Codes
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="APP">
				<xs:annotation>
					<xs:documentation>Rule App/Other Conditions</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WHO">
				<xs:annotation>
					<xs:documentation>Eligibility</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DAY">
				<xs:annotation>
					<xs:documentation>Day/Time</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SEA">
				<xs:annotation>
					<xs:documentation>Seasonal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FLT">
				<xs:annotation>
					<xs:documentation>Flight App</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ADV">
				<xs:annotation>
					<xs:documentation>Advance Res/Tkt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MIN">
				<xs:annotation>
					<xs:documentation>Minimum Stay</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MAX">
				<xs:annotation>
					<xs:documentation>Maximum Stay</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STP">
				<xs:annotation>
					<xs:documentation>Stopovers</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRF">
