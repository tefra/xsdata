					<xs:documentation>Round The World.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SA">
				<xs:annotation>
					<xs:documentation>South Atlantic only</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SP">
				<xs:annotation>
					<xs:documentation>Via South Polar Route</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TB">
				<xs:annotation>
					<xs:documentation>Trans-border</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
