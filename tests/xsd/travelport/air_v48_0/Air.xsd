			<xs:attribute name="CurrencyCode" type="common:typeCurrency" use="optional"/>
			<xs:attribute name="CurrentBalance" type="xs:decimal" use="required"/>
			<xs:attribute name="InitialCredit" type="xs:decimal" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AccountRelatedRules">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BookingRules" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="RoutingRules" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BookingRules">
		<xs:annotation>
			<xs:documentation>Rules related to pre pay booking</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Account Related Rules for pre pay profiles</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="BookingRulesFareReference" minOccurs="0" maxOccurs="999"/>
				<xs:element name="RuleInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Pre pay booking rule information</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="ChargesRules" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Restriction" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Booking restrictions for associated pre pay account</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="DocumentRequired" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Detail about required documents for this pre pay id </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="GenderDobRequired" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Vendor populates if gender/DOB data is required in book.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BookingRulesFareReference">
		<xs:annotation>
			<xs:documentation>Fare Reference associated with the BookingRules. Containing a text container for vendor response text.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="ClassOfService" type="common:typeClassOfService" use="optional"/>
					<xs:attribute name="TicketDesignatorCode" type="typeTicketDesignator" use="optional"/>
					<xs:attribute name="AccountCode" type="xs:string" use="optional"/>
					<xs:attribute name="UpgrageAllowed" type="xs:boolean" use="optional"/>
					<xs:attribute name="UpgradeClassOfService" type="common:typeClassOfService" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ChargesRules">
		<xs:annotation>
			<xs:documentation>Fare Reference associated with the BookingRules</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="VoluntaryChanges" minOccurs="0" maxOccurs="999">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Penalty" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="VolChangeInd" type="xs:boolean" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="VoluntaryRefunds" minOccurs="0" maxOccurs="999">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Penalty" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="VolChangeInd" type="xs:boolean" use="optional"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Penalty">
		<xs:complexType>
			<xs:attribute name="Amount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Penalty Amount</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PenaltyType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>This is the PPC (Price Processing Code)code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Restriction">
		<xs:annotation>
			<xs:documentation>Fare Reference associated with the BookingRules</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
