				<xs:annotation>
					<xs:documentation>Involuntary Exchanges
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VOR">
				<xs:annotation>
					<xs:documentation>Voluntary Refunds</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IVR">
				<xs:annotation>
					<xs:documentation>Involuntary Refunds</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NET">
				<xs:annotation>
					<xs:documentation>Negotiated Fares</xs:documentation>
