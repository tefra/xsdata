		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="FailureRemark" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Container to show all provider failure information.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="common:attrDocument"/>
			<xs:attribute name="ResultType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Successful Or Failed result indicator.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="VoidDocumentInfo">
		<xs:annotation>
			<xs:documentation>Container to represent document information. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attributeGroup ref="common:attrDocument"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeConnectionIndicator">
		<xs:annotation>
			<xs:documentation> Types of connection indicator : 
                     AvailabilityAndPricing : Specified availability and pricing connection;
                     TurnAround : Specified turn around;
                     Stopover : Specified stopover;</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AvailabilityAndPricing"/>
			<xs:enumeration value="TurnAround"/>
			<xs:enumeration value="Stopover"/>
		</xs:restriction>
	</xs:simpleType>
