	<xs:complexType name="typeFarePenalty">
		<xs:annotation>
			<xs:documentation>Penalty applicable on a Fare for change/
                cancellation etc- expressed in both Money and Percentage.
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Amount" type="common:typeMoney" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The penalty (if any) - expressed as the actual
                        amount of money. Both Amount and Percentage can be present.
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Percentage" type="common:typePercentageWithDecimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The penalty (if any) - expressed in
                        percentage. Both Amount and Percentage can be present.
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PenaltyApplies"	use="optional" >
			<xs:simpleType>
				<xs:annotation>
					<xs:documentation>The values can be "Anytime", "Before Departure" or "After Departure".
                        </xs:documentation>
				</xs:annotation>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Anytime" />
					<xs:enumeration value="Before Departure" />
					<xs:enumeration value="After Departure" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="NoShow" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>The No Show penalty (if any) to change/cancel the fare.</xs:documentation>
				</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="PriceChangeType">
		<xs:annotation>
			<xs:documentation>Indicates a price change is found in Fare Control Manager
            </xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="Amount" type="xs:string" use="required">
					<xs:annotation>
						<xs:documentation>Contains the currency and amount information.  
                        Assume the amount is added unless a hyphen is present to indicate subtraction.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Carrier" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Contains carrier code information</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="SegmentRef" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Contains segment reference information</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="typeRestrictionLengthOfStay">
		<xs:annotation>
			<xs:documentation>Length Of Stay Restriction ( e.g. 2 day
                minimum..)</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Length" type="xs:integer" use="optional"/>
		<xs:attribute name="StayUnit" type="typeStayUnit" use="optional"/>
		<xs:attribute name="StayDate" type="xs:date" use="optional"/>
		<xs:attribute name="MoreRulesPresent" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>If true, specifies that advance purchase
                    information will be present in fare rules.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="typeStayUnit">
		<xs:annotation>
			<xs:documentation>Units for the Length of Stay
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Minutes"/>
			<xs:enumeration value="Hours"/>
			<xs:enumeration value="Days"/>
			<xs:enumeration value="Months"/>
			<xs:enumeration value="Monday"/>
			<xs:enumeration value="Tuesday"/>
			<xs:enumeration value="Wednesday"/>
			<xs:enumeration value="Thursday"/>
			<xs:enumeration value="Friday"/>
			<xs:enumeration value="Saturday"/>
			<xs:enumeration value="Sunday"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeUnitWeight">
		<xs:annotation>
			<xs:documentation>The available units of weight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Kilograms"/>
			<xs:enumeration value="Pounds"/>
		</xs:restriction>
