					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="RFISC" use="optional">
				<xs:annotation>
					<xs:documentation>Reason For Issueance Sub code for the EMD coupon</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="RFIDescription" use="optional">
				<xs:annotation>
					<xs:documentation>Reason for Issueance Description for the EMD coupon</xs:documentation>
				</xs:annotation>
