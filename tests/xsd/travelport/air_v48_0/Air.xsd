			<xs:enumeration value="FBR">
				<xs:annotation>
					<xs:documentation>Fare By Rule</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GRP">
				<xs:annotation>
					<xs:documentation>Groups</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
