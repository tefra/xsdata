				<xs:element ref="common:FormOfPayment" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:FormOfPaymentRef" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
	</xs:group>
	<xs:element name="PreferredCarriers">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:Carrier" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ProhibitedCarriers">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:Carrier" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PermittedCarriers">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:Carrier" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PermittedCabins">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:CabinClass" maxOccurs="5"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PreferredCabins">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:CabinClass"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ProhibitedCabins">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:CabinClass" maxOccurs="3"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PreferredBookingCodes">
		<xs:annotation>
			<xs:documentation>This is the container to specify all preferred booking codes</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BookingCode" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirPricingPayment">
		<xs:annotation>
			<xs:documentation>AirPricing Payment information - used to
                associate a FormOfPayment withiin the UR with one or more
                AirPricingInfos</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="BaseAirPricingPaymentGroup"/>
				<xs:element ref="AirPricingInfoRef" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareNote">
		<xs:annotation>
			<xs:documentation>A simple textual fare note. Used within several
                other objects.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Key" type="common:typeRef" use="required"/>
					<xs:attribute name="Precedence" type="xs:integer" use="optional"/>
					<xs:attribute name="NoteName" type="xs:string" use="optional"/>
					<xs:attribute name="FareInfoMessageRef" type="common:typeRef" use="optional"/>
					<xs:attributeGroup ref="common:attrElementKeyResults"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketDesignator">
		<xs:annotation>
			<xs:documentation>Ticket Designator used to further qualify a
                Fare Basis Code.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Value" type="typeTicketDesignator" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ConjunctedTicketInfo">
		<xs:complexType>
			<xs:attribute name="Number" type="xs:string" use="required"/>
				<xs:attribute name="IATANumber" type="common:typeIATA" use="optional" />
			<xs:attribute name="TicketIssueDate" type="xs:dateTime"	use="optional" />
			<xs:attribute name="TicketingAgentSignOn" use="optional" >
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CountryCode" type="common:typeCountry" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Contains Ticketed PCC’s Country code.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" type="common:typeTicketStatus" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExchangedTicketInfo">
		<xs:annotation>
			<xs:documentation>Contains Exchanged/Reissued Ticket Information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Number" type="common:typeTicketNumber" use="required">
				<xs:annotation>
					<xs:documentation>Original Ticket that was Exchange/Reissued</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeCouponStatus">
		<xs:annotation>
			<xs:documentation>ATA/IATA Standard coupon status.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>Code="A" Status="Airport Controlled".
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Code="C" Status="Checked In"
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="F">
				<xs:annotation>
					<xs:documentation>Code="F" Status="Flown/Used"
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L">
				<xs:annotation>
					<xs:documentation>Code="L" Status="Boarded/Lifted"
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="O">
				<xs:annotation>
					<xs:documentation>Code="O" Status="Open"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation>Code="P" Status="Printed"
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation>Code="R" Status="Refunded"
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation>Code="E" Status="Exchanged"
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="V">
				<xs:annotation>
					<xs:documentation>Code="V" Status="Void"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Z">
				<xs:annotation>
					<xs:documentation>Code="Z" Status="Archived/Carrier
                        Modified"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="U">
				<xs:annotation>
					<xs:documentation>Code="U" Status="Unavailable"
                    </xs:documentation>
				</xs:annotation>
