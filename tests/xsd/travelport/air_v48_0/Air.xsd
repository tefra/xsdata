		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:typePassengerType">
					<xs:sequence>
						<xs:element ref="common:AirSeatAssignment" minOccurs="0" maxOccurs="999"/>
					</xs:sequence>
					<xs:attribute name="Key" type="common:typeRef" use="optional"/>
				</xs:extension>
			</xs:complexContent>
