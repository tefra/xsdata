					<xs:element ref="FlightDetails" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="FlightDetailsRef" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="AlternateLocationDistanceRef" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="Connection" minOccurs="0"/>
					<xs:element ref="common:SellMessage" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="RailCoachDetails" minOccurs="0" maxOccurs="999"/>
				</xs:sequence>
				<xs:attribute name="OpenSegment" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Indicates OpenSegment when True
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Group" type="xs:int" use="required">
					<xs:annotation>
						<xs:documentation>The Origin Destination Grouping of this
                                segment.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Carrier" type="common:typeCarrier">
					<xs:annotation>
						<xs:documentation>The carrier that is marketing this segment
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CabinClass" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Specifies Cabin class for a group of
                                class of services. Cabin class is not identified if it is not
                                present.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="FlightNumber" type="common:typeFlightNumber">
					<xs:annotation>
						<xs:documentation>The flight number under which the marketing
                                carrier is marketing this flight</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="common:attrOrigDestDepatureInfo"/>
				<xs:attributeGroup ref="common:attrFlightTimes"/>
				<xs:attributeGroup ref="common:attrProviderSupplier"/>
				<xs:attributeGroup ref="attrLinkInfo"/>
				<xs:attribute name="ClassOfService" type="common:typeClassOfService" use="optional"/>
				<xs:attribute name="ETicketability" type="typeEticketability" use="optional">
					<xs:annotation>
						<xs:documentation>Identifies if this particular segment
                                is E-Ticketable</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Equipment" type="typeEquipment" use="optional">
					<xs:annotation>
						<xs:documentation>Identifies the equipment that this
                                segment is operating under.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="MarriageGroup" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation>Identifies this segment as being a
                                married segment. It is paired with other segments of the same
                                value.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="NumberOfStops" type="xs:int" use="optional">
					<xs:annotation>
						<xs:documentation>Identifies the number of stops for
                                each within the segment.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Seamless" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Identifies that this segment was sold
                                via a direct access channel to the marketing carrier.
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ChangeOfPlane" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation>Indicates the traveler must change
                                planes between flights.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="GuaranteedPaymentCarrier" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Identifies that this segment has
                                Guaranteed Payment Carrier.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="HostTokenRef" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Identifies that this segment has
                                Guaranteed Payment Carrier.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ProviderReservationInfoRef" type="common:typeRef" use="optional">
					<xs:annotation>
						<xs:documentation>Provider reservation reference key.
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PassiveProviderReservationInfoRef" type="common:typeRef" use="optional">
					<xs:annotation>
						<xs:documentation>Provider reservation reference key.
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="OptionalServicesIndicator" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Indicates true if flight provides
                                optional services.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AvailabilitySource" type="typeAvailabilitySource" use="optional">
					<xs:annotation>
						<xs:documentation>Indicates Availability source of
                                AirSegment.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="APISRequirementsRef" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Reference to the APIS Requirements for
                                this AirSegment.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="BlackListed" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Indicates blacklisted carriers which are banned from servicing points to, from and within the European Community.
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="OperationalStatus" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Refers to the flight operational status for the segment.
                            This attribute will only be returned in the AvailabilitySearchRsp and not used/returned in any other request/responses.
                            If this attribute is not returned back in the response, it means the flight is operational and not past scheduled departure. 
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="NumberInParty">
					<xs:annotation>
						<xs:documentation>Number of person traveling in this air segment excluding the number of infants on lap.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:positiveInteger">
							<xs:minInclusive value="1"/>
							<xs:maxInclusive value="99"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="RailCoachNumber" use="optional">
					<xs:annotation>
						<xs:documentation>Coach number for which rail seatmap/coachmap is returned.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="BookingDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation>Used for rapid reprice. The date the booking was made. 
                        Providers: 1G/1V/1P/1S/1A</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="FlownSegment" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation>Used for rapid reprice. Tells whether or not the air segment has been flown. 
                        Providers: 1G/1V/1P/1S/1A</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ScheduleChange" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation>Used for rapid reprice. Tells whether or not the air segment had a 
                        schedule change by the carrier. This tells rapid reprice that the change in the air 
                        segment was involuntary and because of a schedule change, not because the user is 
                        changing the segment. Providers: 1G/1V/1P/1S/1A</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="BrandIndicator" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Value “B” specifies that the carrier supports Rich Content and Branding.  The Brand Indicator is only returned in the availability search response.  Provider: 1G, 1V, 1P, 1J, ACH</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="typeNumberOfPassengers">
		<xs:restriction base="xs:int"/>
	</xs:simpleType>
	<xs:element name="AutoPricingInfo">
		<xs:annotation>
			<xs:documentation>Auto Pricing based on Segment and Traveler Association.
              </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentRef" minOccurs="0" maxOccurs="100"/>
				<xs:element ref="common:BookingTravelerRef" minOccurs="0" maxOccurs="100"/>
				<xs:element ref="AirPricingModifiers" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="AirSegmentPricingModifiers" minOccurs="0" maxOccurs="100"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="PricingType" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the Pricing Type used.
                        The possible values are TicketRecord, StoredFare, PricingInstruction.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PlatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The Plating Carrier for this journey
                      </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailCoachDetails">
		<xs:complexType>
			<xs:attribute name="RailCoachNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>Rail coach number for the returned coach details.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AvailableRailSeats" type="xs:string">
				<xs:annotation>
					<xs:documentation>Number of available seats present in this rail coach.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RailSeatMapAvailability" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates if seats are available in this rail coach which can be mapped.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="EMDInfo">
		<xs:annotation>
			<xs:documentation>This is the parent container to display EMD information. Occurrence of multiple unique EMDs inside this container indicate that those EMDs are conjunctive to each other. Supported providers are 1G/1V/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="EMDTravelerInfo">
