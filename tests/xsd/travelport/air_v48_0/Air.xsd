			<xs:annotation>
				<xs:documentation>EMD Number</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PrimaryDocumentIndicator" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates whether the EMD is a primary EMD.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="InConjunctionWith" type="typeEMDNumber" use="optional">
			<xs:annotation>
				<xs:documentation>Returns the number of the Primary EMD, if this EMD is a conjunctive EMD</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AssociatedTicketNumber" type="common:typeTicketNumber" use="optional">
			<xs:annotation>
				<xs:documentation>This number indicates the e-Ticket number associated with this EMD</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PlatingCarrier" type="common:typeCarrier" use="optional">
			<xs:annotation>
				<xs:documentation>Plating carrier code for which this EMD is issued</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IssueDate" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation>Issue Date for this EMD</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="EMDEndorsement">
		<xs:annotation>
			<xs:documentation>Endorsement for EMD. Supported providers are 1V/1G/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="IssuanceModifiers">
		<xs:annotation>
			<xs:documentation>General modifiers supported for EMD Issuance.Supported providers are 1V/1G/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice minOccurs="0">
					<xs:annotation>
						<xs:documentation>Override PNR FOP for EMD  Issuance.</xs:documentation>
					</xs:annotation>
					<xs:element ref="common:FormOfPaymentRef" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reference to FormOfPayment present in the UR to be used for EMD issuance.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="common:FormOfPayment" minOccurs="0">
						<xs:annotation>
							<xs:documentation>FormOfPayment information to be used for EMD issuance.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element ref="CustomerReceiptInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information about customer receipt via email.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="EMDEndorsement" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Endorsement details to be used during EMD issuance.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="EMDCommission" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Commission information to be used for EMD issuance.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PlatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>Plating carrier code for which this EMD is issued.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="EMDCommission">
		<xs:annotation>
			<xs:documentation>Commission information to be used for EMD issuance. Supported providers are 1V/1G/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" type="common:typeAdjustmentType" use="required">
				<xs:annotation>
					<xs:documentation>Type of the commission applied.One of Amount/Percentage</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Value" type="xs:decimal" use="required">
				<xs:annotation>
					<xs:documentation>Value of the commission applied for EMD issuance.Could represent amount or percentage depending on the type</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CurrencyCode" type="common:typeCurrency">
				<xs:annotation>
					<xs:documentation>Currency of the commission amount applied.Applicable only with type - Amount</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomerReceiptInfo">
		<xs:annotation>
			<xs:documentation>Information about customer receipt via email. Supported providers are 1V/1G/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Refererence of the Booking Traveler related to the email.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EmailRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Reference to the email address used for receipt of EMD.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SelectionModifiers">
		<xs:annotation>
			<xs:documentation>Modifiers supported for selection of services during EMD Issuance. Supported providers are 1V/1G/1P/1J</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>References to airsegments for which EMDs will be generated on all the associated services.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SvcSegmentRef" type="common:typeRef" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>SVC segment reference to which the EMD is being issued</xs:documentation>
				</xs:annotation>
			</xs:element>
			</xs:sequence>
			<xs:attribute name="SupplierCode" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>Supplier/Vendor code for which EMDs will be generated on all the associated services. Required if PNR contains more than one supplier.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RFIC" use="optional">
				<xs:annotation>
					<xs:documentation>Reason for issuance code for which EMDs will be generated on all the associated services.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BrandModifiers">
		<xs:annotation>
			<xs:documentation>Used to specify the level of branding requested.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
     	<xs:choice minOccurs="1" maxOccurs="1">
			<xs:sequence>
				<xs:element name="FareFamilyDisplay" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation> Used to request a fare family display.
						</xs:documentation>
					</xs:annotation>
					<xs:complexType>
					     <xs:attribute name="ModifierType" use="required" type="xs:string">
				             <xs:annotation>
						         <xs:documentation> "FareFamily" returns the lowest branded fares in a fare family.
													"MaintainBookingCode" attempts to return the lowest branded fare in a fare family display based on the permitted booking code. Any brand that does not have a fare for the permitted booking code will then have the lowest fare returned.
													"LowestFareInBrand" returns the lowest fare within each branded fare in a fare family display. 
						         </xs:documentation>
					         </xs:annotation>
						 </xs:attribute>	 
					</xs:complexType>		 
		        </xs:element>
		       </xs:sequence>
		       <xs:sequence>
				<xs:element name="BasicDetailsOnly" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						 <xs:documentation>Used to request basic details of the brand.
						 </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						 <xs:attribute name="ReturnBasicDetails" type="xs:boolean" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="MerchandisingDetails">
		<xs:annotation>
			<xs:documentation>Rich Content and Branding for a fare brand.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirItineraryDetails" maxOccurs="99"/>
				<xs:element ref="common:AccountCode" minOccurs="0" maxOccurs="10"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MerchandisingAvailabilityDetails">
		<xs:annotation>
			<xs:documentation>Rich Content and Branding for an air segment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirItineraryDetails"/>
				<xs:element ref="common:AccountCode" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BrandID">
		<xs:annotation>
			<xs:documentation>Brand ids for Merchandising details.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Id" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirItineraryDetails">
		<xs:annotation>
			<xs:documentation>Itinerary details containing brand details</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegmentDetails" minOccurs="1" maxOccurs="16"/>
				<xs:element ref="PassengerDetails" maxOccurs="15"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Air itinerary details key</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSegmentDetails">
		<xs:annotation>
			<xs:documentation>An Air marketable travel segment.
               </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PassengerDetailsRef" minOccurs="1" maxOccurs="999"/>
				<xs:element ref="BrandID" minOccurs="0" maxOccurs="99"/>
				<xs:element name="BookingCodeList" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Lists classes of service and their counts separated by delimiter |.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required"/>
			<xs:attribute name="Carrier" type="common:typeCarrier" use="required"/>
			<xs:attribute name="Origin" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The IATA location code for this origination of this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="required">
				<xs:annotation>
					<xs:documentation>The IATA location code for this destination of this entity.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DepartureTime" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>The date and time at which this entity departs. This does not include time zone information since it can be derived from the origin location.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ArrivalTime" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>The date and time at which this entity arrives at the destination. This does not include time zone information since it can be derived from the origin location.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Equipment" type="typeEquipment" use="optional"/>
			<xs:attribute name="ClassOfService" type="common:typeClassOfService"/>
			<xs:attribute name="CabinClass" type="xs:string" use="optional"/>
			<xs:attribute name="OperatingCarrier" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The actual carrier that is operating the flight.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FlightNumber" type="common:typeFlightNumber" use="required">
				<xs:annotation>
					<xs:documentation>Flight Number for the Search Leg Detail.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PassengerDetailsRef">
		<xs:annotation>
			<xs:documentation>Reference of the Passenger</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareDetailsRef">
		<xs:annotation>
			<xs:documentation>Reference of the Fare</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PassengerDetails">
		<xs:annotation>
			<xs:documentation>Details of passenger</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LoyaltyCardDetails" minOccurs="0" maxOccurs="9"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Passenger key</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Code" type="common:typePTC" use="required">
				<xs:annotation>
					<xs:documentation>Passenger code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Age" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Passenger age</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareDetails">
		<xs:annotation>
			<xs:documentation>Information about this fare component</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FareTicketDesignator" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Fare key</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassengerDetailRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>PassengerRef key</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareBasis" type="common:typeFareBasisCode" use="required">
				<xs:annotation>
					<xs:documentation>The fare basis code for this fare</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="LoyaltyCardDetails">
		<xs:annotation>
			<xs:documentation>Passenger Loyalty card details</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="SupplierCode" type="common:typeCarrier" use="required">
				<xs:annotation>
					<xs:documentation>Carrier Code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PriorityCode" type="common:typePriorityCode" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="OptionalServiceModifiers">
		<xs:annotation>
			<xs:documentation>Rich Content and Branding for an optional service</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="OptionalServiceModifier" maxOccurs="99"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OptionalServiceModifier">
		<xs:annotation>
			<xs:documentation>Optional service modifiers</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" type="common:typeMerchandisingService" use="required">
				<xs:annotation>
					<xs:documentation>Optional service type</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SecondaryType" type="common:typeMerchandisingService">
				<xs:annotation>
					<xs:documentation>Secondary optional service type</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SupplierCode" type="common:typeSupplierCode" use="required">
				<xs:annotation>
					<xs:documentation>Optional service supplier code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ServiceSubCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>As published by ATPCO</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelDate" type="xs:date" use="required">
				<xs:annotation>
					<xs:documentation>The departure date of the air segment the optional service is valid for.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Description" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>This allows MDS to return specific image and text corresponding to the ancillary name (S5 ancillary name).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PriceRange">
		<xs:complexType>
			<xs:attribute name="DefaultCurrency" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates if the currency code of StartPrice / EndPrice is the default currency code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="StartPrice" type="common:typeMoney">
				<xs:annotation>
					<xs:documentation>Price range start value</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EndPrice" type="common:typeMoney">
				<xs:annotation>
					<xs:documentation>Price range end value</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Text" type="typeTextElement">
		<xs:annotation>
			<xs:documentation>Type of Text, Eg-'Upsell','Marketing Agent','Marketing Consumer','Strapline','Rule'.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicableSegment" type="typeApplicableSegment">
		<xs:annotation>
			<xs:documentation>Applicable air segment.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DefaultBrandDetail" type="typeDefaultBrandDetail">
		<xs:annotation>
			<xs:documentation>Applicable air segment.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="JourneyData">
		<xs:annotation>
			<xs:documentation>Performs journey aware air availability</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AirSegment" maxOccurs="99"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeTextElement">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="Type" type="xs:string" use="required"/>
				<xs:attribute name="LanguageCode" type="xs:language" use="optional">
					<xs:annotation>
						<xs:documentation>ISO 639 two-character language codes are used to retrieve specific information in the requested language. For Rich Content and Branding, language codes ZH-HANT (Chinese Traditional), ZH-HANS (Chinese Simplified), FR-CA (French Canadian) and PT-BR (Portuguese Brazil) can also be used. For RCH, language codes ENGB, ENUS, DEDE, DECH can also be used. Only certain services support this attribute. Providers: ACH, RCH, 1G, 1V, 1P, 1J.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="typeApplicableSegment">
		<xs:attribute name="Key" type="common:typeRef" use="required"/>
		<xs:attribute name="AirItineraryDetailsRef" type="common:typeRef"/>
		<xs:attribute name="BookingCounts" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Classes of service and their counts.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeDefaultBrandDetail">
		<xs:sequence>
			<xs:element ref="Text" minOccurs="0" maxOccurs="4">
				<xs:annotation>
					<xs:documentation>Text associated to the brand</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ImageLocation" minOccurs="0" maxOccurs="3">
				<xs:annotation>
					<xs:documentation>Images associated to the brand</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ApplicableSegment" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>Defines for which air segment the brand is applicable.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="BrandID" type="typeBrandId" use="optional">
			<xs:annotation>
				<xs:documentation>The unique identifier of the brand</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="PCC">
		<xs:annotation>
			<xs:documentation>Specify pseudo City</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:OverridePCC" minOccurs="0"/>
				<xs:element ref="common:PointOfSale" minOccurs="0" maxOccurs="5"/>
				<xs:element ref="TicketAgency" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketAgency">
		<xs:annotation>
			<xs:documentation>This modifier will override the pseudo of the ticketing agency found in the AAT (TKAG). Used for all plating carrier validation.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ProviderCode" use="required">
				<xs:annotation>
					<xs:documentation>The code of the Provider (e.g. 1G, 1P)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" use="required">
				<xs:annotation>
					<xs:documentation>The PCC of the host system.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="MaxLayoverDurationType">
		<xs:annotation>
			<xs:documentation>User can specify its attribute's value in Minutes. Maximum size of each attribute is 4.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Domestic" type="MaxLayoverDurationRangeType" use="optional">
			<xs:annotation>
				<xs:documentation>It will be applied for all Domestic-to-Domestic connections.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Gateway" type="MaxLayoverDurationRangeType" use="optional">
			<xs:annotation>
				<xs:documentation>It will be applied for all Domestic to International and International to Domestic connections.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="International" type="MaxLayoverDurationRangeType" use="optional">
			<xs:annotation>
				<xs:documentation>It will be applied for all International-to-International connections.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="MaxLayoverDurationRangeType">
		<xs:annotation>
			<xs:documentation>Layover duration range of valid values 0-9999</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="typeNativeSearchModifier">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required">
					<xs:annotation>
						<xs:documentation>The host for which the NativeModfier being added to</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="RepricingModifiers">
		<xs:annotation>
			<xs:documentation>Used for rapid reprice to provide additional options for the reprice. Providers: 1G/1V/1P/1S/1A</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PrivateFareOptions" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Public and/or Private Fares requested for pricing.            Currently supported: AccountCodeOnly, PrivateFaresOnly, PublicPrivateFaresOnly.
                        </xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="FareType" minOccurs="0" maxOccurs="100"/>
				<xs:element ref="FareTicketDesignator" minOccurs="0" maxOccurs="1"/>
				<xs:element name="OverrideCurrency" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:attribute name="CurrencyCode" type="common:typeCurrency" use="optional"/>
						<xs:attribute name="CountryCode" type="common:typeCountry" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element ref="AirSegmentPricingModifiers" minOccurs="0" maxOccurs="999"/>
				<xs:element name="WithholdTaxCode" minOccurs="0" maxOccurs="4">
					<xs:annotation>
						<xs:documentation>Used to request tax withholding for the tax code specified. Providers supported 1G/1P</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PriceClassOfService" type="common:typePriceClassOfService" use="optional">
				<xs:annotation>
					<xs:documentation>Values allowed are ClassBooked or LowestClass. This tells how to price the new itinerary.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CreateDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>This is either today’s date or the date the repriced itinerary was created</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReissueLocCityCode" type="common:typeCity" use="optional">
				<xs:annotation>
					<xs:documentation>This is the city code of the reissue location</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReissueLocCountryCode" type="common:typeCountry" use="optional">
				<xs:annotation>
					<xs:documentation>This is the country code of the reissue location</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BulkTicket" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Set to true and the itinerary is/will be a bulk ticket. 
                                        Set to false and the itinerary being repriced will not be a bulk ticket.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AccountCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>May be used in conjunction with PrivateFareOptions</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PenaltyAsTaxCode" use="optional">
				<xs:annotation>
					<xs:documentation>Used to request that the penalty be applied as a tax, to the tax code specified. Providers supported 1G/1P </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="AirPricingSolutionRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to a AirPricingSolution. Providers: 1G, 1V, 1P, 1J.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PenaltyToFare" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Will add the change fee/penalty amount to the total fare amount. Supported Providers: 1P</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PricePTCOnly" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>A value of true forces the price for the PTC even if that fare is not the lowest fare for the passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandDetails" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Set to true full brand details will be returned.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BrandModifier" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>A value of MaintainBrand will maintain the brand from the original ticket if applicable.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="JetServiceOnly" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>Request flights that are jet service only. Available in AirExchangeMultiQuoteReq only.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TimeWindow" use="optional">
				<xs:annotation>
					<xs:documentation>A value of Time Window is optional. Available in AirExchangeMultiQuoteReq only.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="01"/>
						<xs:maxInclusive value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FlightType" use="optional" default="Direct">
				<xs:annotation>
					<xs:documentation>Type of flights to be returned. Values are 'NonStop', 'Direct', 'SingleConnection' and 'NoRestrictions'. Available in AirExchangeMultiQuoteReq only.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Direct"/>
						<xs:enumeration value="NonStop"/>
						<xs:enumeration value="SingleConnection"/>
						<xs:enumeration value="NoRestrictions"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="MultiAirportSearch" type="xs:boolean" use="optional" default="true">
				<xs:annotation>
					<xs:documentation>A value of Multi Airport Search Indicator is optional. Available in AirExchangeMultiQuoteReq only.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ConnectionPoint" type="common:typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>A value of Connection City Code is optional. Available in AirExchangeMultiQuoteReq only.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OriginalItineraryDetails">
		<xs:annotation>
			<xs:documentation>Used for rapid reprice to provide additional information about the original itinerary. Providers: 1G/1V/1P/1S/1A</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ItineraryType" type="common:typeItineraryCode" use="optional">
				<xs:annotation>
					<xs:documentation>Values allowed are International or Domestic. This tells if the itinerary is international or domestic.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BulkTicket" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Set to true and the itinerary is/will be a bulk ticket. 
                                        Set to false and the itinerary being repriced will not be a bulk ticket. 
                                        Default is false.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketingPCC" type="common:typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>This is the PCC or SID where the ticket was issued</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketingIATA" type="common:typeIATA" use="optional">
				<xs:annotation>
					<xs:documentation>This is the IATA where the ticket was issued.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketingCountry" type="common:typeCountry" use="optional">
				<xs:annotation>
					<xs:documentation>This is the country where the ticket was issued.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TourCode" type="typeTourCode" use="optional"/>
			<xs:attribute name="TicketingDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>The date the repriced itinerary was ticketed</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PricingDetails">
		<xs:annotation>
			<xs:documentation>Used for rapid reprice. This is a response element.  Additional information about how pricing was obtain, messages, etc.  Providers: 1G/1V/1P/1S/1A</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AdvisoryMessage" type="xs:string" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Advisory messages returned from the host.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EndorsementText" type="xs:string" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Endorsement text returned from the host.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="WaiverText" type="xs:string" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Waiver text returned from the host.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="LowFarePricing" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This tells if Low Fare Finder was used.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LowFareFound" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This tells if the lowest fare was found.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PenaltyApplies" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This tells if penalties apply.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DiscountApplies" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This tells if a discount applies.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ItineraryType" type="common:typeItineraryCode" use="optional">
				<xs:annotation>
					<xs:documentation> Values allowed are International or Domestic. This tells if the itinerary is international or domestic. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ValidatingVendorCode" type="common:typeCarrier" use="optional">
				<xs:annotation>
					<xs:documentation>The vendor code of the validating carrier.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ForTicketingOnDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>The ticketing date of the itinerary.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LastDateToTicket" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>The last date to issue the ticket.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FormOfRefund" type="common:typeFormOfRefund" use="optional">
				<xs:annotation>
					<xs:documentation>How the refund will be issued. Values will be MCO or FormOfPayment</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AccountCode" type="xs:string" use="optional"/>
			<xs:attribute name="BankersSellingRate" type="xs:decimal" use="optional">
				<xs:annotation>
					<xs:documentation>The selling rate at time of quote.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PricingType" type="common:typePricingType" use="optional">
				<xs:annotation>
					<xs:documentation>Ties with the RepricingModifiers sent in the request and tells how the itinerary was priced.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ConversionRate" type="xs:decimal" use="optional">
				<xs:annotation>
					<xs:documentation>The conversion rate at the time of quote.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RateOfExchange" type="xs:decimal" use="optional">
				<xs:annotation>
					<xs:documentation>The exchange rate at time of quote.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OriginalTicketCurrency" type="common:typeCurrency" use="optional">
				<xs:annotation>
					<xs:documentation>The currency of the original ticket.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirExchangeBundleTotal">
		<xs:annotation>
			<xs:documentation>Total exchange and penalty information for one ticket number</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:AirExchangeInfo"/>
				<xs:element ref="common:Penalty" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Only used within an AirExchangeQuoteRsp
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FareRulesFilterCategory">
		<xs:annotation>
			<xs:documentation>
				Fare Rules Filter if requested will return rules for requested category in the response. Applicable for providers 1G,1V,1P,1J.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CategoryCode" type="xs:string" minOccurs="1" maxOccurs="35">
					<xs:annotation>
						<xs:documentation>Fare Rules Filter category can be requested. Currently only '˜MIN, MAX, ADV, CHG, OTH' is supported. Applicable for Providers 1G,1V,1P,1J.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="FareInfoRef" type="xs:string" use="optional" >
				<xs:annotation>
					<xs:documentation>This tells if Low Fare Finder was used.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeRefundabilityValue">
		<xs:annotation>
			<xs:documentation>Currently returned: FullyRefundable (1G,1V), RefundableWithPenalty (1G,1V), Refundable (1P,1J),  NonRefundable (1G,1V,1P,1J).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:element name="FareRulesFilter">
		<xs:annotation>
			<xs:documentation>Fare Rules Filter about this fare component. Applicable Providers are 1P,1J,1G,1V.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Refundability" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Refundability/Penalty Fare Rules about this fare component.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Value" type="typeRefundabilityValue" use="required">
							<xs:annotation>
								<xs:documentation>Currently returned: FullyRefundable (1G,1V), RefundableWithPenalty (1G,1V), Refundable (1P,1J),  NonRefundable (1G,1V,1P,1J).Refundable.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="LatestTicketingTime" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>For Future Use</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CHG" type="CHGType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>For Penalties</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MIN" type="MINType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>For Minimum Stay</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAX" type="MAXType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>For Maximum Stay</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ADV" type="ADVType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>For Advance Res/Tkt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OTH" type="OTHType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CHGType">
		<xs:annotation>
			<xs:documentation> PenFee list will be populated 
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PenFee" type="PenFeeType" minOccurs="0" maxOccurs="2"/>			
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MINType">
		<xs:attribute name="HoursMin" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Minimum hours. True if unit of time is hours.  False if unit of time is not hours.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DaysMin" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Minimum days. True if unit of time is days. False if unit of time is not days.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MonthsMin" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Minimum months. True if unit of time is months. False if unit of time is not months.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OccurIndMin" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Minimum occurance indicator. True if day of the week is used. False if day of the week is not used. 
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SameDayMin" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Same day minimum. True if Stay is same day. False if Stay is not same day.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TmDOWMin" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>If a min unit of time is true then number corrolates to day of the week starting with 1 for Sunday.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FareComponent" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Fare component number of the most restrictive fare.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="NumOccurMin" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Number of min occurances. This field is used in conjunction with the Day of Week.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="MAXType">
		<xs:attribute name="HoursMax" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum hours. True if unit of time is hours. False if unit of time is not hours.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DaysMax" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum days. True if unit of time is days. False if unit of time is not days.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MonthsMax" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum months. True if unit of time is months. False if unit of time is not months.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OccurIndMax" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum cccurance indicator. True if day of the week is used. False if day of the week is not used.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SameDayMax" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Same day maximum. True if Stay is same day. False if Stay is not same day.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StartIndMax" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Start indicator. True if start indicator. False if not a start indicator.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CompletionInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Completion indicator. True if Completion C Indicator. False if not Completion C Indicator.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TmDOWMax" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>If a max unit of time is true then number corrolates to day of the week starting with 1 for Sunday.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="NumOccurMax" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Number of maximum occurances.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ADVType">
		<xs:attribute name="AdvRsvnOnlyIfTk" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Reservation only if ticketed. True is advanced reservations only if tickets. False is no advanced reservations
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AdvRsvnAnyTm" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Reservation anytime. True if advanced reservatiosn anytime. False if advanced reservations for a limited time.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AdvRsvnHrs" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Reservation hours. True if advanced reservation time in hours. False if advanced reservation time not in hours.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AdvRsvnDays" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Reservation days. True if advanced reservation time in days. False if advanced reservation time not in days.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AdvRsvnMonths" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Reservation months. True if advanced reservation time in months. False if advanced reservation time not in months.
				</xs:documentation>
