					<xs:choice>
						<xs:element ref="SearchAirLeg" maxOccurs="16"/>
						<xs:element ref="SearchSpecificAirSegment" maxOccurs="999"/>
					</xs:choice>
					<xs:element ref="AirSearchModifiers" minOccurs="0"/>
					<xs:element ref="JourneyData" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
