	<xs:element name="AirExchangeEligibilityRsp">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:BaseRsp">
					<xs:sequence>
						<xs:element ref="ExchangeEligibilityInfo"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="BaseAirExchangeMultiQuoteReq">
		<xs:complexContent>
			<xs:extension base="common:BaseCoreReq">
				<xs:sequence>
					<xs:element ref="common:TicketNumber" minOccurs="0" maxOccurs="999"/>
					<xs:element name="ProviderReservationInfo" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Provider: 1P - Represents a valid Provider Reservation/PNR whose itinerary is to be exchanged</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attributeGroup ref="common:ProviderReservation"/>
						</xs:complexType>
					</xs:element>
					<xs:element ref="AirPricingSolution" minOccurs="0" maxOccurs="2"/>
					<xs:element ref="RepricingModifiers" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="OriginalItineraryDetails" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="common:OverridePCC" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="AirExchangeMultiQuoteReq">
		<xs:annotation>
			<xs:documentation>Request multiple quotes for the exchange of an itinerary. 1P transactions only</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="BaseAirExchangeMultiQuoteReq">
					<xs:attribute name="Type" type="xs:string" use="optional" default="Summary">
						<xs:annotation>
							<xs:documentation>Type choices are "Detail" or "Summary"  Default will be Summary</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirExchangeMultiQuoteRsp">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:BaseRsp">
					<xs:sequence>
						<xs:element ref="AirSegmentList" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="BrandList" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="AirExchangeMulitQuoteList" minOccurs="0" maxOccurs="999"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
