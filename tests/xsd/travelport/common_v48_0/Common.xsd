	<xs:element name="AirSeatAssignment">
		<xs:annotation>
			<xs:documentation>Identifies the seat assignment for a passenger.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="SeatAssignment"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailSeatAssignment">
		<xs:annotation>
			<xs:documentation>Identifies the seat assignment for a passenger on RailSegment.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Characteristic" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Status" type="typeStatusCode" use="required"/>
			<xs:attribute name="Seat" type="xs:string" use="required"/>
			<xs:attribute name="RailSegmentRef" type="typeRef" use="optional"/>
			<xs:attribute name="CoachNumber" type="xs:string" use="optional"/>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Characteristic">
		<xs:annotation>
			<xs:documentation>Identifies the characteristics of the seat with seat type, value and description.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="SeatType" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates codeset of values such as Seat Type like Place,Position, Smoking Choice, Place Arrangement, Place Direction, Compartment. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SeatDescription" use="optional">
				<xs:annotation>
					<xs:documentation>Description of the seat type.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SeatValue" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates the value specific to the selected type.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SeatValueDescription" use="optional">
				<xs:annotation>
					<xs:documentation>Description of the seat value.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeSeatTypeCode">
		<xs:annotation>
			<xs:documentation>Valid 4 letter Seat Type Code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="4"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeStatusCode">
		<xs:annotation>
			<xs:documentation>Valid 2 letter Status Code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ServiceData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SeatAttributes" minOccurs="0"/>
				<xs:element ref="CabinClass" minOccurs="0"/>
				<xs:element name="SSRRef" type="typeKeyBasedReference" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>References to the related SSRs. At present, only reference to ASVC SSR is supported. Supported providers are 1G/1V/1P/1J</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Data" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Data that specifies the details of the merchandising offering (e.g. seat number for seat service)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirSegmentRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to a segment if the merchandising offering only pertains to that segment. If no segment reference is present this means this offering is for the whole itinerary.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to a passenger if the merchandising offering only pertains to that passenger. If no passenger reference is present this means this offering is for all passengers.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="StopOver" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates that there is a significant
                        delay between flights (usually 12 hours or more)
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelerType" type="typePTC" use="optional">
				<xs:annotation>
					<xs:documentation>Passenger Type Code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EMDSummaryRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to the corresponding EMD issued. Supported providers are 1G/1V/1P/1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EMDCouponRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to the corresponding EMD coupon issued. Supported providers are 1G/1V/1P/1J</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ServiceInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Description" type="xs:string" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Description of the Service.  Usually used in tandem with  one or more media items.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="MediaItem" minOccurs="0" maxOccurs="3"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SSR">
		<xs:annotation>
			<xs:documentation>Special serivces like wheel chair, or pet carrier.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="SegmentRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to the air segment. May be required for some Types.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassiveSegmentRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to the passive segment.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider reservation reference key.
               </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Type" type="typeSSRCode" use="required">
				<xs:annotation>
					<xs:documentation>Programmatic SSRs use codes recognized by the provider/supplier (example, VGML=vegetarian meal code). Manual SSRs do not have an associated programmatic code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" type="xs:string" use="optional"/>
			<xs:attribute name="FreeText" type="typeSSRFreeText" use="optional">
				<xs:annotation>
					<xs:documentation>Certain SSR types will require a free text message. For example MAAS (Meet and assist).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Carrier" type="typeCarrier" use="optional"/>
			<xs:attribute name="CarrierSpecificText" use="optional">
				<xs:annotation>
					<xs:documentation>Carrier specific information which are not captured in the FreeText field(not present in IATA's standard SSR DOCO format). An example is VISA Expiration Date.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Description" type="xs:string" use="optional"/>
			<xs:attribute name="ProviderDefinedType" use="optional">
				<xs:annotation>
					<xs:documentation>Original Type as sent by the provider</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SSRRuleRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>UniqueID to associate a rule to the SSR</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="URL" type="xs:anyURI"/>
			<xs:attributeGroup ref="attrElementKeyResults"/>
			<xs:attribute name="ProfileID" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Key assigned for Secure Flight Document value from the specified profile</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProfileSecureFlightDocKey" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Unique ID of Booking Traveler's Profile that contains the Secure flight Detail</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="TravelerType">
		<xs:annotation>
			<xs:documentation>The 3-char IATA traveler type code</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="typePTC" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TicketNumber" type="StringLength1to13">
		<xs:annotation>
			<xs:documentation>The identifying number for the actual ticket</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PaymentRef">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Reference Element for Payment</xs:documentation>
			</xs:annotation>
			<xs:attribute name="Key" type="typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BookingTravelerRef">
		<xs:annotation>
			<xs:documentation>Reference Element for Booking Traveler and Loyalty cards</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LoyaltyCardRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="DriversLicenseRef" minOccurs="0"/>
				<xs:element ref="DiscountCardRef" minOccurs="0" maxOccurs="9"/>
				<xs:element ref="PaymentRef" minOccurs="0" maxOccurs="3"/>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DiscountCardRef">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Reference Element for Discount Card</xs:documentation>
			</xs:annotation>
			<xs:attribute name="Key" type="typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DriversLicenseRef">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Reference Element for Drivers Licenses</xs:documentation>
			</xs:annotation>
			<xs:attribute name="Key" type="typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="LoyaltyCardRef">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Reference Element for Loyalty cards</xs:documentation>
			</xs:annotation>
			<xs:attribute name="Key" type="typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeRemark">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
					<xs:annotation>
						<xs:documentation>Provider reservation reference key.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
					<xs:annotation>
						<xs:documentation>Contains the Provider Code of the provider for which this element is used</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="UnassociatedRemark">
		<xs:annotation>
			<xs:documentation>A textual remark container to hold non-associated itinerary remarks</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeRemarkWithTravelerRef">
					<xs:attribute name="Key" type="typeRef" use="optional"/>
					<xs:attributeGroup ref="attrElementKeyResults"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeRemarkWithTravelerRef">
		<xs:sequence>
			<xs:element name="RemarkData" type="xs:string">
				<xs:annotation>
					<xs:documentation>Actual remarks data.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BookingTravelerRef" type="typeRef" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Reference to Booking Traveler.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>Provider reservation reference key.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
			<xs:annotation>
				<xs:documentation>Contains the Provider Code of the provider for which this element is used</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="CustomerID">
		<xs:annotation>
			<xs:documentation>A provider reservation field used to store customer information. It may be used to identify reservations which will/will not be available for access.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeRemark">
					<xs:attribute name="Key" type="typeRef" use="optional"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<!--*** PNR Reference Elements ***-->
	<xs:complexType name="BaseReservation">
		<xs:sequence>
			<!-- <xs:element ref="BookingTraveler" minOccurs="0" maxOccurs="999"/>-->
			<xs:element ref="AccountingRemark" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="GeneralRemark" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="Restriction" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="PassiveInfo" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="LocatorCode" type="typeLocatorCode" use="required">
			<xs:annotation>
				<xs:documentation>The unique identifier for this reservation. If this is this View Only UR LocatorCode is '999999'.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CreateDate" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The date and time that this reservation was created.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ModifiedDate" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The date and time that this reservation was last modified for any reason.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CustomerNumber" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:element name="MCO">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="MCOInformation">
					<xs:sequence>
						<xs:element ref="FormOfPayment" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="Endorsement" minOccurs="0"/>
						<xs:element ref="MCOExchangeInfo" minOccurs="0"/>
						<xs:element ref="MCOFeeInfo" minOccurs="0"/>
						<xs:element ref="MCORemark" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="MCOPriceData" minOccurs="0"/>
						<xs:element ref="StockControl" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="MCOText" minOccurs="0" maxOccurs="999"/>
					</xs:sequence>
					<xs:attribute name="TicketType" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Ticket issue indicator. Possible values "Pre-paid ticket advice", "Ticket on departure" and "Other" .</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TicketNumber" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>The ticket that this MCO was issued in connection with. Could be the ticket that caused the fee, a residual from an exchange, or an airline service fee.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MCOIssued" type="xs:boolean" use="required">
						<xs:annotation>
							<xs:documentation>Set to true when the MCO is to be issued and set to false if it is stored for issue at a later time.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MCOIssueDate" type="xs:dateTime" use="optional">
						<xs:annotation>
							<xs:documentation>Date and time in which the MCO was issued.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MCODocNum" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>MCO document number.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="IssueReasonCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>MCO issuing reason code. Possible Values (List): A - Air transportation, B - Surface transportation</xs:documentation>
							<xs:documentation>C - Bag shipped as cargo, D - Land arrgs for it, E - Car hire, F - Sleeper / berth</xs:documentation>
							<xs:documentation>G - Up-grading, H - Under collections, I - Taxes/fees/charges, J - Deposits down payments</xs:documentation>
							<xs:documentation>K - Refundable Balances, L - Hotel accommodations, M - Sundry charges, N - Cancellation fee</xs:documentation>
							<xs:documentation>O - Other, P thru Z - airline specific, 1 thru 9 - market specific</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="PlatingCarrier" type="typeCarrier" use="optional">
						<xs:annotation>
							<xs:documentation>The Plating Carrier for this MCO</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TourOperator" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Tour Operator - name of honoring carrier or operator.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Location" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Location of honoring carrier or operator.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TourCode" type="xs:string">
						<xs:annotation>
							<xs:documentation>The Tour Code of the MCO.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
						<xs:annotation>
							<xs:documentation>Contains the Provider Code of the provider that houses this MCO.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ProviderLocatorCode" type="typeProviderLocatorCode" use="optional">
						<xs:annotation>
							<xs:documentation>Contains the Provider Locator Code of the Provider Reservation that houses this MCO.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="PseudoCityCode" type="typePCC" use="optional">
						<xs:annotation>
							<xs:documentation>The PCC in the host system.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ExpiryDate" type="xs:date" use="optional">
						<xs:annotation>
							<xs:documentation>E-Voucherâ€™s Expiry Date. This expiry date is specific to Rail product</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="MCOText">
		<xs:annotation>
			<xs:documentation>All type of free format text messages related to MCO like - Command Text, Agent Entry, MCO Modifiers, Text Message</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeFreeFormText">
					<xs:attribute name="Type" type="xs:string">
						<xs:annotation>
							<xs:documentation>The type of text. Possible values: Command Text, Agent Entry, MCO Modifiers, Text Message</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeFreeFormText">
		<xs:annotation>
			<xs:documentation>Free form Text</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string"/>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="MCOPriceData">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TaxInfo" type="typeTaxInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element name="Commission" minOccurs="0">
					<xs:complexType>
						<xs:attributeGroup ref="attrAmountPercent"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="MCOAmount" type="typeMoney" use="required">
				<xs:annotation>
					<xs:documentation>The total value of the MCO including any processing fees.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MCOEquivalentFare" type="typeMoney">
				<xs:annotation>
					<xs:documentation>Exchange value of the currency actually collected.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MCOTotalAmount" type="typeMoney">
				<xs:annotation>
					<xs:documentation>The Total amount for the MCO.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="PassengerInfo">
		<xs:annotation>
			<xs:documentation>Booking Traveler information tied to invoice </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Name" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="BookingTravelerRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to a passenger related to a ticket.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassengerType" type="typePTC" use="optional">
				<xs:annotation>
					<xs:documentation>Passenger Type Code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="MCOInformation">
		<xs:sequence>
			<xs:element ref="PassengerInfo" minOccurs="0" maxOccurs="999"/>
		</xs:sequence>
		<xs:attribute name="MCONumber" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The unique MCO number</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Status" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Current status of the MCO</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MCOType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The Type of MCO. Once of Agency Fee, Airline Service Fee, or Residual value from an Exchange.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="MCOExchangeInfo">
		<xs:annotation>
			<xs:documentation>Information related to the exchange tickets available for the MCO</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FormOfPayment" minOccurs="0"/>
				<xs:element ref="ExchangedCoupon" minOccurs="0" maxOccurs="4"/>
			</xs:sequence>
			<xs:attribute name="OriginalTicketNumber" type="typeTicketNumber" use="optional">
				<xs:annotation>
					<xs:documentation>Airline form and serial number of the original ticket issued.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OriginalCityCode" type="typeCity" use="optional">
				<xs:annotation>
					<xs:documentation>Location of honoring carrier or operator.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OriginalTicketDate" type="typeDate" use="optional">
				<xs:annotation>
					<xs:documentation>Date that the Original ticket was issued.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IATACode" type="typeIATA" use="optional">
				<xs:annotation>
					<xs:documentation>IATA code of the issuing agency.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="MCOFeeInfo">
		<xs:annotation>
			<xs:documentation>Information related to the PTA/TOD (Prepaid Ticket Advice / Ticket on Departure) related to the MCO</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attributeGroup ref="attrAmountPercent"/>
			<xs:attribute name="FeeAppliesToInd" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates if PTA/TOD fee is for the entire MCO or is per person.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Per-Person"/>
						<xs:enumeration value="Per-MCO"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="attrAmountPercent">
		<xs:annotation>
			<xs:documentation>Amount or Percentage</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Amount" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The monetary amount.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Percentage" type="typePercentageWithDecimal" use="optional">
			<xs:annotation>
				<xs:documentation>The percentage.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="typeTicketNumber">
		<xs:annotation>
			<xs:documentation>Reference Ticket Number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="13"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="MCORemark">
		<xs:annotation>
			<xs:documentation>Information related to fare construction, free form text etc. of the MCO</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="AdditionalRmk" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Indicates if the remark is additional remark or not.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="StockControl">
		<xs:annotation>
			<xs:documentation>The Stock Control Numbers related details of the MCO.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Stock control type valid options include: Pending, Failed, Plain Paper, Blank, Suppressed.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Number" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Stock control number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExchangedCoupon">
		<xs:annotation>
			<xs:documentation>The coupon numbers that were used in the exchange process to create the MCO.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="TicketNumber" type="typeTicketNumber" use="required">
				<xs:annotation>
					<xs:documentation>The ticket number for which the exchange coupons are present.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CouponNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>Coupon numbers that were exchanged specific to this ticket</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeTaxInfo">
		<xs:sequence>
			<xs:element ref="TaxDetail" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="IncludedInBase" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="Key" type="typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>The tax key represents a valid key of tax </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Category" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The tax category represents a valid IATA tax
                 code. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CarrierDefinedCategory" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Optional category, where a carrier has
                   used a non-standard IATA tax category. The tax category will be
                   set to "DU" </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SegmentRef" type="typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>The segment to which that tax is relative (if
                   applicable)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FlightDetailsRef" type="typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>The flight details that this tax is relative
                 to (if applicable)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CouponRef" type="typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>The coupon to which that tax is relative (if
                 applicable)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="attrTaxDetail"/>
		<xs:attribute name="TaxExempted" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>This indicates whether the tax specified by
                 tax category is exempted.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
			<xs:annotation>
				<xs:documentation>Code of the provider returning this TaxInfo.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierCode" type="typeSupplierCode" use="optional">
			<xs:annotation>
				<xs:documentation>Code of the supplier returning this TaxInfo.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Text" type="StringLength1to128" use="optional">
			<xs:annotation>
				<xs:documentation>Additional Information returned from Supplier.(ACH  only)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="TaxDetail">
		<xs:annotation>
			<xs:documentation>The tax idetail nformation for a fare quote tax.
           </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attributeGroup ref="attrTaxDetail"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="attrTaxDetail">
		<xs:annotation>
			<xs:documentation>Holds fare quote tax information.
           </xs:documentation>
		</xs:annotation>
		<xs:attribute name="Amount" type="typeMoney" use="required"/>
		<xs:attribute name="OriginAirport" type="typeAirport" use="optional"/>
		<xs:attribute name="DestinationAirport" type="typeAirport" use="optional"/>
		<xs:attribute name="CountryCode" type="xs:string" use="optional"/>
		<xs:attribute name="FareInfoRef" type="typeRef" use="optional"/>
	</xs:attributeGroup>
	<xs:element name="MetaData">
		<xs:annotation>
			<xs:documentation>Extra data to elaborate the parent element. This data is primarily informative and is not persisted.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RoleInfo">
		<xs:annotation>
			<xs:documentation>Container to specify the role of the agent</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Id" use="required">
				<xs:annotation>
					<xs:documentation>Unique identifier of the role.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="19"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Name" use="required">
				<xs:annotation>
					<xs:documentation>Agent's role name </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="128"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Source" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Role inheritance level. Needed in the response, not in the request </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Description" use="optional">
				<xs:annotation>
					<xs:documentation>Description of role</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1024"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OSI">
		<xs:annotation>
			<xs:documentation>Other Service information sent to the carriers during air bookings</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Carrier" type="typeCarrier" use="required"/>
			<xs:attribute name="Code" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Text" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider reservation reference key.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
				<xs:annotation>
					<xs:documentation>Contains the Provider Code of the provider for which this OSI is used</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Segment">
		<xs:annotation>
			<xs:documentation>The base segment type</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="SegmentRemark" minOccurs="0" maxOccurs="999"/>
		</xs:sequence>
		<xs:attribute name="Key" type="typeRef" use="required"/>
		<xs:attribute name="Status" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Status of this segment.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Passive" type="xs:boolean" use="optional"/>
		<xs:attribute name="TravelOrder" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>To identify the appropriate travel sequence for Air/Car/Hotel segments/reservations based on travel dates. This ordering is applicable across the UR not provider or traveler specific</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderSegmentOrder" use="optional">
			<xs:annotation>
				<xs:documentation>To identify the appropriate travel sequence for Air/Car/Hotel/Rail segments/reservations in the provider reservation.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:maxInclusive value="999"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="attrElementKeyResults"/>
	</xs:complexType>
	<xs:element name="TravelSegment">
		<xs:annotation>
			<xs:documentation>Generic segment used to provide travel information that was not processed by the system</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Segment">
					<xs:attribute name="Origin" type="typeIATACode" use="optional">
						<xs:annotation>
							<xs:documentation>The IATA location code for this origination of this entity.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Destination" type="typeIATACode" use="optional">
						<xs:annotation>
							<xs:documentation>The IATA location code for this destination of this entity.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DepartureTime" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>The date and time at which this entity departs. This does not include time zone information since it can be derived from the origin location.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ArrivalTime" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>The date and time at which this entity arrives at the destination. This does not include time zone information since it can be derived from the origin location.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<!--*** Search Data Types ***-->
	<xs:element name="SearchEvent">
		<xs:annotation>
			<xs:documentation>Search for various reservation events</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeTimeRange">
					<xs:attribute name="Type" type="typeEventType"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SearchTicketing">
		<xs:annotation>
			<xs:documentation>Search restriction by Agent</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="TicketStatus" use="optional" default="Both">
				<xs:annotation>
					<xs:documentation>Return only PNRs with ticketed, non-ticketed or both</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Ticketed"/>
						<xs:enumeration value="Unticketed"/>
						<xs:enumeration value="Both"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ReservationStatus" use="optional" default="Both">
				<xs:annotation>
					<xs:documentation>Used only if "TicketStatus" set to "No" or "Both". Return only PNRs with specific reservation status or both statuses.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="OnHold"/>
						<xs:enumeration value="SetForTicketing"/>
						<xs:enumeration value="Both"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TicketDate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>Identifies when this reservation was ticketed, or when it should be ticketed by (in the event of a TTL)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SearchPassenger">
		<xs:annotation>
			<xs:documentation>Passenger type with code and optional age information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typePassengerType">
					<xs:attribute name="Key" type="typeRef" use="optional"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SimpleName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Free text name</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SpecialEquipment">
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Type" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Special equipment associated with a specific vehicle</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContinuityCheckOverride">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Performs an override of continuity validation errors.</xs:documentation>
			</xs:annotation>
			<xs:simpleContent>
				<xs:extension base="typeNonBlanks">
					<xs:attribute name="Key" type="typeRef" use="optional">
						<xs:annotation>
							<xs:documentation>
                               Will use key to map continuity remark to a particular segment
                           </xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:group name="BaseBookingTravelerInfoA">
		<xs:sequence>
			<xs:element ref="BookingTravelerName"/>
			<xs:element ref="DeliveryInfo" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="PhoneNumber" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="Email" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="LoyaltyCard" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="DiscountCard" minOccurs="0" maxOccurs="9"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="BaseBookingTravelerInfoB">
		<xs:sequence>
			<xs:element name="EmergencyInfo" type="xs:string" minOccurs="0"/>
			<xs:element name="Address" type="typeStructuredAddress" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="DriversLicense" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="AppliedProfile" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="CustomizedNameData" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="TravelComplianceData" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Travel Compliance and Preferred Supplier information of the booking traveler specific to a segment. Not applicable to Saved Trip.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="TravelInfo" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:attributeGroup name="attrBookingTravelerGrp">
		<xs:attribute name="Key" type="typeRef" use="optional"/>
		<xs:attribute name="TravelerType" type="typePTC" use="optional">
			<xs:annotation>
				<xs:documentation>Defines the type of traveler used for booking which could be a non-defining type (Companion, Web-fare, etc), or a standard type (Adult, Child, etc).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Age" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>BookingTraveler age</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="VIP" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation>When set to True indicates that the Booking Traveler is a VIP based on agency/customer criteria</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DOB" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation>Traveler Date of Birth</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Gender" type="typeGender" use="optional">
			<xs:annotation>
				<xs:documentation>The BookingTraveler gender type</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Nationality" type="typeCountry" use="optional">
			<xs:annotation>
				<xs:documentation>Specify ISO country code for nationality of the Booking Traveler</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<!--*** Shared Elements ***-->
	<!--*** Simple Data Types ***-->
	<xs:complexType name="typeAgentInfo"/>
	<xs:simpleType name="StringLength1to128">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 128.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="128"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength6to128">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 6 to 128.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="6"/>
			<xs:maxLength value="128"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to1024">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 1024.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1024"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to2000">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 2000.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to1000">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 1000.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeProfileID">
		<xs:annotation>
			<xs:documentation>A type for unique party identifiers of any party role. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<xs:simpleType name="typeProvisioningCode">
		<xs:annotation>
			<xs:documentation>User defined provisioning identifier.</xs:documentation>
