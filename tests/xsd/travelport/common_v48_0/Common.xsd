			</xs:attribute>
			<xs:attribute name="Reason" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The reason it is restricted</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="SeatAssignment">
