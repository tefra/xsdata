<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2012 rel. 2 (x64) (http://www.altova.com) by Jack Jagoda (TRAVELPORT GDS) -->
<xs:schema xmlns="http://www.travelport.com/schema/common_v48_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.travelport.com/schema/common_v48_0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation>(c) Travelport GDS 2008 All rights reserved. Proprietary Information of Travelport GDS.</xs:documentation>
	</xs:annotation>
	<xs:element name="NextResultReference">
		<xs:annotation>
			<xs:documentation>Container to return/send additional retrieve/request additional search results</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="typeNonBlanks">
					<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
						<xs:annotation>
							<xs:documentation>The code of the Provider (e.g 1G,1S)</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<!--*** Common Reference Elements ***-->
	<xs:element name="Airport">
		<xs:annotation>
			<xs:documentation>Airport identifier</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Location">
					<xs:attribute name="Code" type="typeIATACode" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="VendorLocation">
		<xs:annotation>
			<xs:documentation>Location definition specific to a Vendor in a specific provider (e.g. 1G) system.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeVendorLocation"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeVendorLocation">
		<xs:attribute name="ProviderCode" type="typeProviderCode" use="required">
			<xs:annotation>
				<xs:documentation>The code of the provider (e.g. 1G, 1S)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="VendorCode" type="typeSupplierCode" use="required">
			<xs:annotation>
				<xs:documentation>The code of the vendor (e.g.  HZ, etc.)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PreferredOption" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Preferred Option marker for Location.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="VendorLocationID" use="optional">
			<xs:annotation>
				<xs:documentation>Location identifier</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:whiteSpace value="collapse"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Key" type="typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>Key which maps vendor location with vehicles</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MoreRatesToken" use="optional">
			<xs:annotation>
				<xs:documentation>Enter the Token when provided by hotel property, more rates exist. HADS/HSS  support only.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="30"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="AgencySellInfo">
		<xs:annotation>
			<xs:documentation>Information about the agency selling the reservation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="IataCode" type="typeIATA" use="optional">
				<xs:annotation>
					<xs:documentation>The IATA code that pertains to this Agency and Branch.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Country" type="typeCountry" use="optional">
				<xs:annotation>
					<xs:documentation>The country code of the requesting agency.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CurrencyCode" type="typeCurrency" use="optional">
				<xs:annotation>
					<xs:documentation>The currency code in which the reservation will be ticketed.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
				<xs:annotation>
					<xs:documentation>The IATA assigned airline/GDS code.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>The PCC in the host system.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CityCode" type="typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>IATA code of "home" city or airport.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgencyInfo">
		<xs:annotation>
			<xs:documentation>Tracks the various agent/agency information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AgentAction" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgentAction">
		<xs:annotation>
			<xs:documentation>Depending on context, this will represent
	            information about which agent perform different actions.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ActionType" use="required">
				<xs:annotation>
					<xs:documentation>The type of action the agent performed.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Created"/>
						<xs:enumeration value="Modified"/>
						<xs:enumeration value="Ticketed"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="AgentCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The AgenctCode who performed the action.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BranchCode" type="typeBranchCode" use="required">
				<xs:annotation>
					<xs:documentation>The BranchCode of the branch (working branch, branchcode used for the request. If nothing specified, branchcode for the agent) who performed the action.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AgencyCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The AgencyCode of the agent who performed the action.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AgentSine" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The sign in user name of the agent logged into the terminal. PROVIDER SUPPORTED: ACH</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EventTime" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>Date and time at which this event took place.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrAgentOverride"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirExchangeInfo">
		<xs:annotation>
			<xs:documentation>Provides results of a exchange quote</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TotalPenaltyTaxInfo" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PenaltyTaxInfo" type="typeTax" minOccurs="0" maxOccurs="999"/>
						</xs:sequence>
						<xs:attribute name="TotalPenaltyTax" type="typeMoney" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="PaidTax" type="typeTax" minOccurs="0" maxOccurs="999"/>
				<xs:element name="TicketFeeInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Used for rapid reprice. Providers: 1G/1V/1P/1S/1A</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Base" type="typeMoney" use="optional"/>
						<xs:attribute name="Tax" type="typeMoney" use="optional"/>
						<xs:attribute name="Total" type="typeMoney" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Reason" type="xs:string" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Used for rapid reprice. The reason code or text is returned if the 
						PricingTag is not equal to A, and explains why A was not returned. 
						Providers: 1G/1V/1P/1S/1A</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FeeInfo" type="typeFeeInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element name="TaxInfo" type="typeTaxInfo" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Itinerary level taxes</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ExchangeAmount" type="typeMoney" use="required"/>
			<xs:attribute name="BaseFare" type="typeMoney" use="optional"/>
			<xs:attribute name="EquivalentBaseFare" type="typeMoney" use="optional"/>
			<xs:attribute name="Taxes" type="typeMoney" use="optional"/>
			<xs:attribute name="ChangeFee" type="typeMoney" use="optional"/>
			<xs:attribute name="ForfeitAmount" type="typeMoney" use="optional"/>
			<xs:attribute name="Refundable" type="xs:boolean" use="optional"/>
			<xs:attribute name="Exchangeable" type="xs:boolean" use="optional"/>
			<xs:attribute name="FirstClassUpgrade" type="xs:boolean" use="optional"/>
			<xs:attribute name="TicketByDate" type="xs:string" use="optional"/>
			<xs:attribute name="PricingTag" type="xs:string" use="optional"/>
			<xs:attribute name="EquivalentChangeFee" type="typeMoney" use="optional"/>
			<xs:attribute name="EquivalentExchangeAmount" type="typeMoney" use="optional"/>
			<xs:attribute name="AddCollection" type="typeMoney" use="optional"/>
			<xs:attribute name="ResidualValue" type="typeMoney" use="optional"/>
			<xs:attribute name="TotalResidualValue" type="typeMoney" use="optional"/>
			<xs:attribute name="OriginalFlightValue" type="typeMoney" use="optional"/>
			<xs:attribute name="FlownSegmentValue" type="typeMoney" use="optional"/>
			<xs:attribute name="BulkTicketAdvisory" type="xs:boolean" use="optional"/>
			<xs:attribute name="FarePull" type="typeFarePull" use="optional"/>
			<xs:attribute name="PassengerTypeCode" type="typePTC" use="optional"/>
			<xs:attribute name="PassengerCount" type="xs:integer" use="optional"/>
			<xs:attribute name="FormOfRefund" type="typeFormOfRefund" use="optional">
				<xs:annotation>
					<xs:documentation>How the refund will be issued. Values will be MCO or FormOfPayment</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Refund" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Total refund amount.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Auxdata">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Entry" maxOccurs="999">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Reason" type="xs:string"/>
							<xs:element name="Description" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Carrier">
		<xs:annotation>
			<xs:documentation>Carrier identifier</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="typeCarrier" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Provider">
		<xs:annotation>
			<xs:documentation>Provider identifier</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" type="typeProviderCode" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Certificate">
		<xs:annotation>
			<xs:documentation>Certificate Form of Payment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Number" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The Certificate number</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Amount" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The monetary value of the certificate.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DiscountAmount" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The monetary discount amount of this certificate.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DiscountPercentage" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The percentage discount value of this certificate.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NotValidBefore" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>The date that this certificate becomes valid.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NotValidAfter" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>The date that this certificate expires.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Check">
		<xs:annotation>
			<xs:documentation>Check Form of Payment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="MICRNumber" use="optional">
				<xs:annotation>
					<xs:documentation>Magnetic Ink Character Reader Number of check.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="29"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="RoutingNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The bank routing number of the check.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AccountNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The account number of the check</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CheckNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The sequential check number of the check.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="City">
		<xs:annotation>
			<xs:documentation>City identifier</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Location">
					<xs:attribute name="Code" type="typeIATACode" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CityOrAirport">
		<xs:annotation>
			<xs:documentation>This element can be used when it is not known whether the value is an airport or a city code.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Location">
					<xs:attribute name="Code" type="typeIATACode" use="required">
						<xs:annotation>
							<xs:documentation>The airport or city IATA code.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="PreferCity" type="xs:boolean" use="optional" default="false">
						<xs:annotation>
							<xs:documentation>Indicates that the search should prefer city results over airport results.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CoordinateLocation">
		<xs:annotation>
			<xs:documentation>Specific lat/long location, usually associated with a Distance</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Location">
					<xs:attribute name="latitude" type="xs:double" use="required"/>
					<xs:attribute name="longitude" type="xs:double" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailLocation">
		<xs:annotation>
			<xs:documentation>RCH specific location code (a.k.a UCodes) which uniquely identifies a train station.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="Location">
					<xs:attribute name="Code" type="typeRailLocationCode" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Credentials">
		<xs:annotation>
			<xs:documentation>Container to send login id and password on each request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="UserId" use="required">
				<xs:annotation>
					<xs:documentation>The UserID associated with the entity using this request withing this BranchCode.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CreditCard">
		<xs:annotation>
			<xs:documentation>Container for all credit card information.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeCreditCardType">
					<xs:attributeGroup ref="attrAppliedProfilePaymentInfo"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomizedNameData">
		<xs:annotation>
			<xs:documentation>Customized Name Data is used to print customized name on the different documents.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Key" type="typeRef" use="optional"/>
					<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DebitCard">
		<xs:annotation>
			<xs:documentation>Container for all debit card information.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typePaymentCard">
					<xs:attribute name="IssueNumber" use="optional">
						<xs:annotation>
							<xs:documentation>Verification number for Debit Cards
                            </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="8"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attributeGroup ref="attrAppliedProfilePaymentInfo"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Requisition">
		<xs:annotation>
			<xs:documentation>Requisition Form of Payment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Number" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Requisition number used for accounting</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Category" use="optional">
				<xs:annotation>
					<xs:documentation>Classification Category for the requisition payment</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Government"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Type" use="optional">
				<xs:annotation>
					<xs:documentation>Type can be Cash or Credit for category as Government</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Cash"/>
						<xs:enumeration value="Credit"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="MiscFormOfPayment">
		<xs:annotation>
			<xs:documentation>Miscellaneous Form of Payments</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="CreditCardType" use="optional">
				<xs:annotation>
					<xs:documentation>The 2 letter credit/ debit card type or code which may not have been issued using the standard bank card types  - i.e. an airline issued card</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CreditCardNumber" type="typeCreditCardNumber" use="optional"/>
			<xs:attribute name="ExpDate" type="xs:gYearMonth" use="optional">
				<xs:annotation>
					<xs:documentation>The Expiration date of this card in YYYY-MM format.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Text" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Any free form text which may be associated with the Miscellaneous Form of Payment. This text may be provider or GDS specific</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Category" use="required">
				<xs:annotation>
					<xs:documentation>Indicates what Category the Miscellaneous Form Of Payment is being used for payment - The category may vary by GDS.</xs:documentation>
					<xs:documentation>Allowable values are
                        "Text"
                        "Credit"
                        "CreditCard"
                        "FreeFormCreditCard"
                        "Invoice"
                        "NonRefundable"
                        "MultipleReceivables"
                        "Exchange"
                        "Cash"
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="AcceptanceOverride" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Override airline restriction on the credit card.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SupplierLocator">
		<xs:annotation>
			<xs:documentation>Locator code on the host carrier system</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SegmentRef" type="typeGeneralReference" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Air/Passive Segment Reference</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="SupplierCode" type="typeCarrier" use="required">
				<xs:annotation>
					<xs:documentation>Carrier Code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SupplierLocatorCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Carrier reservation locator code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider Reservation  reference</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CreateDateTime" type="xs:dateTime" use="optional">
				<xs:annotation>
					<xs:documentation>The Date and Time which the reservation is received from the Vendor as a SupplierLocator creation Date.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typePaymentCard">
		<xs:annotation>
			<xs:documentation>Container for all credit and debit card
                information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PhoneNumber" minOccurs="0"/>
			<xs:element name="BillingAddress" type="typeStructuredAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The address to where the billing statements
                        for this card are sent. Used for address verification purposes.
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Type" type="typeCardMerchantType" use="optional">
			<xs:annotation>
				<xs:documentation>The 2 letter credit/ debit card type.
                </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Number" type="typeCreditCardNumber" use="optional"/>
		<xs:attribute name="ExpDate" type="xs:gYearMonth" use="optional">
			<xs:annotation>
				<xs:documentation>The Expiration date of this card in YYYY-MM format.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Name" use="optional">
			<xs:annotation>
				<xs:documentation>The name as it appears on the card.
                </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="128"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="CVV" use="optional">
			<xs:annotation>
				<xs:documentation>Card Verification Code</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="4"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ApprovalCode" use="optional">
			<xs:annotation>
				<xs:documentation>This code is required for an authorization process from the Credit Card company directly,required for some of the CCH carriers.This attribute is also used for EMD retrieve and issuance transactions.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="16"/>
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="CreditCardAuth">
		<xs:annotation>
			<xs:documentation>The result of a Credit Auth Request. Will contain all the authorization info and result codes.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="PaymentRef" type="typeRef" use="optional"/>
			<xs:attribute name="TransId" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The transaction id from the credit processing system</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Number" type="typeCreditCardNumber" use="optional"/>
			<xs:attribute name="Amount" type="typeMoney" use="required">
				<xs:annotation>
					<xs:documentation>The amount that was authorized.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AuthCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The authorization code to confirm card acceptance</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AuthResultCode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The result code of the authorization command.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AVSResultCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The address verification result code (if AVS was requested)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Message" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The message explains the result of the authorization command.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional"/>
			<xs:attribute name="FormOfPaymentRef" type="typeRef" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeGeneralReference">
		<xs:attribute name="Key" type="typeRef" use="required"/>
	</xs:complexType>
	<xs:complexType name="typeFormOfPaymentPNRReference">
		<xs:attribute name="Key" type="typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>Unique ID to identify a ProviderReservationInfo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderReservationLevel" type="xs:boolean" use="optional" default="true">
			<xs:annotation>
				<xs:documentation>It means that the form of payment is applied at ProviderReservation level.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="FormOfPayment">
		<xs:annotation>
			<xs:documentation>A Form of Payment used to purchase all or part of
                a booking.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="CreditCard" minOccurs="0"/>
					<xs:element ref="DebitCard" minOccurs="0"/>
					<xs:element ref="EnettVan" minOccurs="0"/>
					<xs:group ref="FormOfPaymentSequenceGroup"/>
				</xs:choice>
				<xs:element name="ProviderReservationInfoRef" type="typeFormOfPaymentPNRReference" minOccurs="0" maxOccurs="999"/>
				<xs:element name="SegmentRef" type="typeGeneralReference" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="BSPPayment" minOccurs="0"/>
				<xs:element ref="ARCPayment" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>Allowable values are
                            "Certificate"
                            "Cash"
                            "Credit"
                            "Check"
                            "Ticket"
                            "Debit"
                            "Invoice"
                            "Requisition"
                            "MiscFormOfPayment"
                            "AgencyPayment"
                            "DirectBill"
                            "UnitedNations"
                            "DirectPayment"
                            "AgentVoucher"
                            "AccountReceivable"
                            "AgentNonRefundable"
                            "Enett"      
                            "BSPPayment"
                            "ARCPayment"      
                        </xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FulfillmentType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Defines how the client wishes to receive travel documents. Type does not define where or how payment is made. The supported values are  "Ticket on Departure",
							"Travel Agency",
							"Courier",
							"Standard Mail",
							"Ticketless",
							"Ticket Office",
							"Express Mail",
							"Corporate Kiosk",
							"Train Station Service Desk",
							"Direct Printing of Ticket",
							"Ticket by Email",
							"Digital Printing of Ticket at Home",
							"Retrieve Ticket at Eurostar in London"
					</xs:documentation>
					<xs:documentation>Collect booking ticket at a Kiosk, print in agency.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FulfillmentLocation" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Information about the location of the printer.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FulfillmentIDType" type="typeFulfillmentIDType" use="optional">
				<xs:annotation>
					<xs:documentation>Identification type, e.g. credit card, to define how the customer will identify himself when collecting the ticket</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FulfillmentIDNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Identification number, e.g. card number, to define how the customer will identify himself when collecting the ticket </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IsAgentType" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>If this is true then FormOfPayment mention in Type is anAgent type FormOfPayment.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AgentText" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>This is only relevent when IsAgentType is specified as true. Otherwise this will be ignored.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReuseFOP" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Key of the FOP Key to be reused as this Form of Payment.Only Credit and Debit Card will be supported for FOP Reuse.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExternalReference" type="typeExternalReference" use="optional"/>
			<xs:attribute name="Reusable" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates whether the form of payment can be reused or not. Currently applicable for Credit and Debit form of payment</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProfileID" type="xs:string">
				<xs:annotation>
					<xs:documentation>The unique ID of the profile that contains the payment details to use.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProfileKey" type="typeRef">
				<xs:annotation>
					<xs:documentation>The Key assigned to the payment details value from the specified profile.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BSPPayment" >
		<xs:annotation>
			<xs:documentation>BSP form of payment.ACH Only</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="BSPIdentifier" use="required">
				<xs:annotation>
					<xs:documentation>Value of the BSP Direct Bill id</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="128"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="BSPPassword"  use="optional">
				<xs:annotation>
					<xs:documentation>Value of the BSP Direct Bill id password</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="128"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ARCPayment" >
		<xs:annotation>
			<xs:documentation>ARC form of payment.ACH Only</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ARCIdentifier"  use="required">
				<xs:annotation>
					<xs:documentation>Value of the ARC Direct Bill id</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="128"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ARCPassword"  use="optional">
				<xs:annotation>
					<xs:documentation>Value of the ARC Direct Bill id password</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="128"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="EnettVan">
		<xs:annotation>
			<xs:documentation>Container for all eNett Van information.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="MinPercentage" type="typeIntegerPercentage" use="optional">
				<xs:annotation>
					<xs:documentation>The minimum percentage that will be applied on the Total price and sent to enett,which will denote the minimum authorized amount approved by eNett.uApi will default this to zero for multi-use Van's.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MaxPercentage" type="typeIntegerPercentage" use="optional">
				<xs:annotation>
					<xs:documentation>The maximum percentage that will be applied on the Total price and sent to enett, which will denote the maximum authorized amount as approved by eNett. This value will be ignored and not used for Multi-Use VANâ€™s.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExpiryDays" type="typeDurationYearInDays" use="optional">
				<xs:annotation>
					<xs:documentation>The number of days from the VAN generation date that the VAN will be active for, after which the VAN cannot be used.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="MultiUse" type="xs:boolean" use="optional" default="true">
				<xs:annotation>
					<xs:documentation>Acceptable values are true or false. If set to true it will denote that the VAN being requested is multi-use else it will indicate a single -use VAN.A Single use VAN can only be debited once while the multiple use VAN's can be debited multiple times subjected to the maximum value it has been authorized for. The default value will be TRUE to indicate a multi-use VAN is being issued.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeDurationYearInDays">
		<xs:annotation>
			<xs:documentation>Value of the Duration in P[NumberOfDays]D format.Ranges Permitted are P001D to P366D . </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:duration">
			<xs:minInclusive value="P1D"/>
			<xs:maxInclusive value="P366D"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeIntegerPercentage">
		<xs:annotation>
			<xs:documentation>Percentage value</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="typeVoucherInformation">
		<xs:annotation>
			<xs:documentation>Information pertaining to the payment of a Vehicle Rental.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="VoucherType" type="typeVoucherType" use="required">
			<xs:annotation>
				<xs:documentation>Specifies if the Voucher is for Full Credit or a Group/Day or a Monetary Amount or RegularVoucher.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Amount" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>Amount associated with the Voucher.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ConfirmationNumber" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Confirmation from the vendor for the voucher</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccountName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Associated account name for the voucher</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Number" type="StringLength1to16" use="optional">
			<xs:annotation>
				<xs:documentation>To advise car associates of the voucher number and store in the car segment. It is required when VoucherType selected as   "RegularVoucher" for 1P, 1J only.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="typeVoucherType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FullCredit"/>
			<xs:enumeration value="GroupOrDay"/>
			<xs:enumeration value="SpecificValue"/>
			<xs:enumeration value="RegularVoucher"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="UnitedNations">
		<xs:annotation>
			<xs:documentation>United Nations Form of Payments</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Number" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DirectPayment">
		<xs:annotation>
			<xs:documentation>Direct Payment Form of Payments</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Text" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgentVoucher">
		<xs:annotation>
			<xs:documentation>Agent Voucher Form of Payments</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Number" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgencyPayment" type="typeAgencyPayment">
		<xs:annotation>
			<xs:documentation>Container for Agency Payment</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="typeAgencyPayment">
		<xs:annotation>
			<xs:documentation>Type for Agency Payment.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="AgencyBillingIdentifier" use="required">
			<xs:annotation>
				<xs:documentation>Value of the billing id</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="128"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="AgencyBillingNumber" use="optional">
			<xs:annotation>
				<xs:documentation>Value of billing number</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="128"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="AgencyBillingPassword" use="optional">
			<xs:annotation>
				<xs:documentation>Value of billing password</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="128"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="FormOfPaymentRef">
		<xs:annotation>
			<xs:documentation>A reference to a Form of Payment in the existing UR</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Distance">
		<xs:annotation>
			<xs:documentation>Container to encapsulate the a distance value with its unit of measure.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Units" use="optional" default="MI">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
						<xs:enumeration value="MI"/>
						<xs:enumeration value="KM"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Value" type="xs:integer" use="required"/>
			<xs:attribute name="Direction">
				<xs:annotation>
					<xs:documentation>Directions: S, N, E, W, SE, NW, ...</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Email">
		<xs:annotation>
			<xs:documentation>Container for an email address with a type specifier (max 128 chars)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Tagging provider reservation info with Email.</xs:documentation>
				</xs:annotation>
				<xs:element ref="ProviderReservationInfoRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Tagging provider reservation info with Email.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Type" type="typeEmailType" use="optional"/>
			<xs:attribute name="Comment" type="typeEmailComment" use="optional"/>
			<xs:attribute name="EmailID" type="xs:string" use="required"/>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeEmailComment">
		<xs:annotation>
			<xs:documentation>Mail comment is used to include one line of freeform information.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="LocatorCode">
		<xs:annotation>
			<xs:documentation>A locator code that identifies a PNR or searches for one.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MealRequest">
		<xs:annotation>
			<xs:documentation>Special meal requests like Vegetarian</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Name">
		<xs:annotation>
			<xs:documentation>Complete name fields</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Prefix" use="optional">
				<xs:annotation>
					<xs:documentation>Name prefix. Size can be up to 20 characters</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="First" use="required">
				<xs:annotation>
					<xs:documentation>First Name. Size can be up to 256 characters</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Middle" use="optional">
				<xs:annotation>
					<xs:documentation>Midle name. Size can be up to 256 characters</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Last" use="required">
				<xs:annotation>
					<xs:documentation>Last Name. Size can be up to 256 characters</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Suffix" use="optional">
				<xs:annotation>
