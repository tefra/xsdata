									<xs:documentation>Reference to AirSegment from an Air
										Reservation.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="HotelReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Specify the locator code of Hotel reservation.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="VehicleReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="1">
