		<xs:attribute name="SeatTypeCode" type="typeSeatTypeCode" use="optional">
			<xs:annotation>
				<xs:documentation>The 4 letter SSR code like SMSW,NSSW,SMST etc.</xs:documentation>
			</xs:annotation>
