	<!--*** Attribute Groups ***-->
	<xs:attributeGroup name="attrLocatorInfo">
		<xs:annotation>
			<xs:documentation>Holds the Universal Record and Provider Reservation Locators for an individual product.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="UniversalRecordLocatorCode" type="typeLocatorCode" use="optional">
			<xs:annotation>
				<xs:documentation>Contains the Locator Code of the Universal Record that houses this reservation.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
			<xs:annotation>
				<xs:documentation>Contains the Provider Code of the provider that houses this reservation.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderLocatorCode" type="typeProviderLocatorCode" use="optional">
			<xs:annotation>
				<xs:documentation>Contains the Locator Code of the Provider Reservation that houses this reservation.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="attrPrices">
		<xs:annotation>
			<xs:documentation>Basic monetary value for Air pricing structures</xs:documentation>
		</xs:annotation>
		<xs:attribute name="TotalPrice" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The total price for this entity including base price and all taxes.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BasePrice" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>Represents the base price for this entity. This does not include any taxes or surcharges.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ApproximateTotalPrice" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The Converted total price in Default Currency for this entity including base price and all taxes.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ApproximateBasePrice" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The Converted base price in Default Currency for this entity. This does not include any taxes or surcharges.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EquivalentBasePrice" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>Represents the base price in the related currency for this entity. This does not include any taxes or surcharges.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Taxes" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The aggregated amount of all the taxes that are associated with this entity. See the associated TaxInfo array for a breakdown of the individual taxes.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Fees" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The aggregated amount of all the fees that are associated with this entity. See the associated FeeInfo array for a breakdown of the individual fees.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Services" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The total cost for all optional services.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ApproximateTaxes" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The Converted tax amount in Default Currency.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ApproximateFees" type="typeMoney" use="optional">
			<xs:annotation>
				<xs:documentation>The Converted fee amount in Default Currency.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="attrFlightTimes">
		<xs:annotation>
			<xs:documentation>Basic attributes used to describe flight time information</xs:documentation>
		</xs:annotation>
		<xs:attribute name="FlightTime" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Time spent (minutes) traveling in flight, including airport taxi time.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TravelTime" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Total time spent (minutes) traveling including flight time and ground time.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Distance" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>The distance traveled. Units are specified in the parent response element.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="attrName">
		<xs:annotation>
			<xs:documentation>Basic attributes used to describe a name</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Prefix" type="xs:string" use="optional"/>
		<xs:attribute name="First" type="xs:string" use="optional"/>
		<xs:attribute name="Middle" type="xs:string" use="optional"/>
		<xs:attribute name="Last" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Suffix" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="attrOrigDestInfo">
		<xs:annotation>
			<xs:documentation>Basic attributes used to describe an origin destination pair</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Origin" type="typeIATACode" use="required">
			<xs:annotation>
				<xs:documentation>The IATA location code for this origination of this entity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Destination" type="typeIATACode" use="required">
			<xs:annotation>
				<xs:documentation>The IATA location code for this destination of this entity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DepartureTime" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>The date and time at which this entity departs. This does not include time zone information since it can be derived from the origin location.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ArrivalTime" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The date and time at which this entity arrives at the destination. This does not include time zone information since it can be derived from the origin location.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="attrOrigDestDepatureInfo">
		<xs:annotation>
			<xs:documentation>Basic attributes used to describe an origin destination pair</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Origin" type="typeIATACode" use="required">
			<xs:annotation>
				<xs:documentation>The IATA location code for this origination of this entity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Destination" type="typeIATACode" use="required">
			<xs:annotation>
				<xs:documentation>The IATA location code for this destination of this entity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DepartureTime" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The date and time at which this entity departs. Date and time are represented as Airport Local Time at the place of departure. The correct time zone offset is also included.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ArrivalTime" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The date and time at which this entity arrives at the destination. Date and time are represented as Airport Local Time at the place of arrival. The correct time zone offset is also included.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="attrProviderSupplier">
