			<xs:sequence>
				<xs:element name="SegmentRef" type="typeGeneralReference" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Air/Passive Segment Reference</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ThirdPartyCode">
				<xs:annotation>
					<xs:documentation>Third party supplier code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="2"/>
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ThirdPartyLocatorCode">
				<xs:annotation>
					<xs:documentation>Confirmation number for third party supplier.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ThirdPartyName" type="typeThirdPartySupplier">
				<xs:annotation>
					<xs:documentation>Third party supplier name.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider Reservation  reference</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="typeRef">
				<xs:annotation>
					<xs:documentation>Unique identifier of the third party supplier. Key can be used to modify or delete saved third party information.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TravelComplianceData">
		<xs:annotation>
			<xs:documentation>Travel Compliance and Preferred Supplier information of the traveler specific to a segment.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PolicyCompliance" minOccurs="0" maxOccurs="2">
					<xs:complexType>
						<xs:attribute name="InPolicy" type="xs:boolean" use="required">
							<xs:annotation>
								<xs:documentation>Policy Compliance Indicator. For In-Policy set to 'true', For Out-Of-Policy set to 'false''.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="PolicyToken" type="StringLength1to128" use="optional">
							<xs:annotation>
								<xs:documentation>Optional text message to set the rule or token for which it's In Policy or Out Of Policy.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="ContractCompliance" minOccurs="0" maxOccurs="2">
					<xs:complexType>
						<xs:attribute name="InContract" type="xs:boolean" use="required">
							<xs:annotation>
								<xs:documentation>Contract Compliance Indicator. For In-Contract set to 'true', For Out-Of-Contract set to 'false'.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ContractToken" type="StringLength1to128" use="optional">
							<xs:annotation>
								<xs:documentation>Optional text message to set the rule or token for which it's In Contract or Out Of Contract.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="PreferredSupplier" minOccurs="0" maxOccurs="999">
					<xs:complexType>
						<xs:attribute name="Preferred" type="xs:boolean" use="required">
							<xs:annotation>
								<xs:documentation>Preferred Supplier - 'true', 'false'.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ProfileType" type="typeProfileType" use="required">
							<xs:annotation>
								<xs:documentation>Indicate profile type. e.g. if Agency Preferred then pass Agency, if Traveler Preferred then pass Traveler.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>System generated key, returned back in the response. This can be used to modify or delete a saved TravelComplianceData.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirSegmentRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Refers to Air Segment. Applicable only for Air. Ignored for others.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassiveSegmentRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Refers to Passive Segment. Applicable only for Passive. Ignored for others.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RailSegmentRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Refers to Rail Segment. Applicable only for Rail. Ignored for others.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReservationLocatorRef" type="typeLocatorCode" use="optional">
				<xs:annotation>
					<xs:documentation>This is returned in the response. Any input will be ignored for this attribute. This represents the association of Travel Compliance Data with the uAPI reservation locator code, mainly relevant to Hotel and Vehicle.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="StringLength3">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 3.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to16">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to250">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 250.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="250"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to116">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 116.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="116"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to500">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 500.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="500"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeProfileLevelWithCredential">
		<xs:annotation>
			<xs:documentation>The "profile level" used for association of workflow etc.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Agency"/>
			<xs:enumeration value="Branch"/>
			<xs:enumeration value="Agent"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="SellMessage" type="xs:string">
		<xs:annotation>
			<xs:documentation>Sell Message from Vendor. This is applicable in response messages only, any input in request message will be ignored.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TerminalSessionInfo" type="xs:string">
		<xs:annotation>
			<xs:documentation>Travelport use only. This element contains CDATA information representing existing GDS session data or ACH credentials information of the terminal user</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:attributeGroup name="attrLoyalty">
		<xs:attribute name="Key" type="typeRef" use="optional"/>
		<xs:attribute name="SupplierCode" type="typeCarrier" use="required">
			<xs:annotation>
				<xs:documentation>The code used to identify the Loyalty supplier, e.g. AA, ZE, MC</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AllianceLevel" type="xs:string" use="optional"/>
		<xs:attribute name="MembershipProgram" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation>Loyalty Program membership Id of the traveler specific to Amtrak(2V) Guest Rewards</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="attrElementKeyResults"/>
	</xs:attributeGroup>
	<xs:simpleType name="typePriorityCode">
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z0-9]{1,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="LoyaltyProgram">
		<xs:complexType>
			<xs:attributeGroup ref="attrLoyalty"/>
			<xs:attribute name="Level" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeErrorInfo">
		<xs:annotation>
			<xs:documentation>Container for error data when there is an application error.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code" type="xs:string"/>
			<xs:element name="Service" type="xs:string"/>
			<xs:element name="Type" type="xs:string"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="TransactionId" type="xs:string"/>
			<xs:element name="TraceId" type="xs:string" minOccurs="0"/>
			<xs:element name="CommandHistory" type="xs:string" minOccurs="0"/>
			<xs:element ref="Auxdata" minOccurs="0"/>
			<xs:element name="StackTrace" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="URTicketStatus">
		<xs:annotation>
			<xs:documentation>
               Information on whether the Universal Record ticket status is Ticketed, Unticketed , Partially Ticketed or Not Applicable status.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Ticketed"/>
			<xs:enumeration value="Unticketed"/>
			<xs:enumeration value="Partially Ticketed"/>
			<xs:enumeration value="Not Applicable"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="PermittedProviders">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Provider"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="attrDocument">
		<xs:annotation>
			<xs:documentation>Containing all document information</xs:documentation>
		</xs:annotation>
		<xs:attribute name="DocumentNumber" type="StringLength1to13">
			<xs:annotation>
				<xs:documentation>Identifies the document number to be voided.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocumentType">
			<xs:annotation>
				<xs:documentation>Identifies the document type to be voided, Document Type can have four values like Service Fee, Paper Ticket , MCO and E-Ticket.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Service Fee"/>
					<xs:enumeration value="Paper Ticket"/>
					<xs:enumeration value="MCO"/>
					<xs:enumeration value="E-Ticket"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="StringLength1to13">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 13.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="13"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeAdjustmentTarget">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Base"/>
			<xs:enumeration value="Total"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeAdjustmentType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Amount"/>
			<xs:enumeration value="Percentage"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ReviewBooking">
		<xs:annotation>
			<xs:documentation>Review Booking or Queue Minders is to add the reminders in the Provider Reservation along with the date time and Queue details. On the date time defined in reminders, the message along with the PNR goes to the desired Queue.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Returned in response. Use it for update of saved review booking.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Queue" use="required">
				<xs:annotation>
					<xs:documentation>Queue number, Must be numeric and less than 100.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:maxInclusive value="99"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="QueueCategory" use="optional">
				<xs:annotation>
					<xs:documentation>Queue Category, 2 Character Alpha or Numeric.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="DateTime" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>Date and Time to place message on designated Queue, Should be prior to the last segment date in the PNR.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>Input PCC optional value for placing the PNR into Queue. If not passed, will add as default PNR's Pseudo.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
				<xs:annotation>
					<xs:documentation>The code of the Provider (e.g 1G,1V).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider Reservation reference. Returned in the response. Use it for update of saved Review Booking.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Remarks" use="required">
				<xs:annotation>
					<xs:documentation>Remark or reminder message. It can be truncated depending on the provider.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:group name="FormOfPaymentSequenceGroup">
		<xs:choice>
			<xs:element ref="Certificate" minOccurs="0"/>
			<xs:element ref="TicketNumber" minOccurs="0"/>
			<xs:element ref="Check" minOccurs="0"/>
			<xs:element ref="Requisition" minOccurs="0"/>
			<xs:element ref="MiscFormOfPayment" minOccurs="0"/>
			<xs:element ref="AgencyPayment" minOccurs="0"/>
			<xs:element ref="UnitedNations" minOccurs="0"/>
			<xs:element ref="DirectPayment" minOccurs="0"/>
			<xs:element ref="AgentVoucher" minOccurs="0"/>
			<xs:element ref="PaymentAdvice" minOccurs="0"/>
		</xs:choice>
	</xs:group>
	<xs:complexType name="typeCreditCardType">
		<xs:complexContent>
			<xs:extension base="typePaymentCard">
				<xs:attribute name="ExtendedPayment" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Used for American Express cards.
                            </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="CustomerReference" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Agencies use this to pass the traveler information to the credit card company.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="AcceptanceOverride" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Override airline restriction on the credit card.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="ThirdPartyPayment" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation>If true, this indicates that the credit card holder is not one of the passengers.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="BankName" type="xs:string">
					<xs:annotation>
						<xs:documentation>Issuing bank name for this credit card</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="BankCountryCode" type="typeCountry">
					<xs:annotation>
						<xs:documentation>ISO Country code associated with the issuing bank</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="BankStateCode" type="typeState">
					<xs:annotation>
						<xs:documentation>State code associated with the issuing bank.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Enett" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation>Acceptable values are true or false. If set to true it will denote that the credit card used has been issued through Enett. For all other credit card payments this value will be set to false.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="PersonalGeography">
		<xs:annotation>
			<xs:documentation>Personal geography details of the associated passenger.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="CountryCode" type="typeCountry" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Passenger country code.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StateProvinceCode" type="typeState" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Passenger state/province code.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CityCode" type="typeCity" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Passenger city code.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Group">
		<xs:annotation>
			<xs:documentation>Represents a traveler group for Group booking and
            all their accompanying data. SUPPORTED PROVIDER: Worldspan and JAL.
        </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name">
					<xs:annotation>
						<xs:documentation>Name of the group in group booking.
                    </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="typeNonBlanks"/>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element ref="DeliveryInfo" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="PhoneNumber" minOccurs="0" maxOccurs="999"/>
				<xs:element name="SSRRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Reference Element for SSR.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Key" type="typeRef" use="required"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Address" type="typeStructuredAddress" minOccurs="0" maxOccurs="1"/>
				<xs:element name="BookingTravelerRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Reference Element for Booking Traveler.
                    </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Key" type="typeRef" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="TravelerType" type="typePTC" use="optional">
				<xs:annotation>
					<xs:documentation>Defines the type of traveler used for booking
                    which could be a non-defining type (Companion, Web-fare, etc), or a
                    standard type (Adult, Child, etc).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="GroupSize" type="xs:positiveInteger" use="required">
				<xs:annotation>
					<xs:documentation>Represents size of the group</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeCity">
		<xs:annotation>
			<xs:documentation>3 Letter City Code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:attributeGroup name="attrElementKeyResults">
		<xs:annotation>
			<xs:documentation>ElementStatus and KeyOverride to show the changes in the element keys</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ElStat" type="typeElementStatus" use="optional">
			<xs:annotation>
				<xs:documentation>This attribute is used to show the action results of an element. 
              Possible values are "A" (when elements have been added to the UR) and "M" (when existing elements have been modified). Response only.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="KeyOverride" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>If a duplicate key is found where we are adding elements in some cases like URAdd, then instead of erroring out set this attribute to true.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="typeElementStatus">
		<xs:annotation>
			<xs:documentation>Values to specify the state of the element. "A" refers to "Add" , "M" refers to "Modified"
            and "C" refers to error conditions when value provided in "Key" attribute is not retained in response</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="C"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ConsolidatorRemark">
		<xs:annotation>
			<xs:documentation>
                Authorization remark for Consolidator access to a PNR . Contains PCC information created by retail agent to  allow a consolidator or other Axess users to service their PNR. PROVIDER SUPPORTED: Worldspan and JAL.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PseudoCityCode" minOccurs="1" maxOccurs="5"/>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Key to be used for internal processing.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider reservation reference key.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">
				<xs:annotation>
					<xs:documentation>Contains the Provider Code of the provider for which this element is used</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PseudoCityCode">
