		<xs:annotation>
			<xs:documentation>A booking code or fare basis code or fare class.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
		</xs:restriction>
