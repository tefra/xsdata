	<xs:simpleType name="typeTypeCode">
		<xs:annotation>
			<xs:documentation>Reference data TypeCode type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRecordStatus">
		<xs:annotation>
			<xs:documentation>
                Information on whether the Universal Record is Current, Past , Cancelled or Any status. 
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="All"/>
			<xs:enumeration value="Past"/>
			<xs:enumeration value="Current"/>
			<xs:enumeration value="Canceled"/>
			<xs:enumeration value="Unknown"/>
		</xs:restriction>
