			<xs:documentation>A textual remark container to hold any printable text. (max 512 chars)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RemarkData" type="xs:string">
					<xs:annotation>
						<xs:documentation>Actual remarks data.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BookingTravelerRef" type="typeRef" minOccurs="0" maxOccurs="999">
