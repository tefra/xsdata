	</xs:element>
	<xs:element name="HostToken">
		<xs:annotation>
			<xs:documentation>This is a host token. It contains some kind of payload we got from a host</xs:documentation>
			<xs:documentation>that must be passed in on successive calls they know who you are as our system</xs:documentation>
			<xs:documentation>does not maintain state. The format of this string isn't important as long as it</xs:documentation>
			<xs:documentation>is not altered in any way between calls. Since a host token is only valid on</xs:documentation>
			<xs:documentation>the host it is assocated with, there is also an attribute called Host so we know</xs:documentation>
			<xs:documentation>how to route the command(s). You can have multiple active sessions between</xs:documentation>
			<xs:documentation>one or more hosts</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Host" type="typeProviderCode">
						<xs:annotation>
							<xs:documentation>The host associated with this token</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Key">
						<xs:annotation>
							<xs:documentation>Unique identifier for this token - use this key when a single HostToken is shared by multiple elements. </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attributeGroup ref="attrElementKeyResults"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="HostTokenList">
		<xs:annotation>
			<xs:documentation>The shared object list of Host Tokens</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HostToken" minOccurs="1" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddSvc">
	<xs:annotation>
		<xs:documentation>1P - Add SVC segments to collect additional fee
		</xs:documentation>
	</xs:annotation>
	<xs:complexType>
		<xs:attribute name="RFIC" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>1P - Reason for issuance</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RFISC" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>1P - Resaon for issuance sub-code
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SvcDescription" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>1P - SVC fee description</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Origin" type="typeIATACode"
			use="optional">
			<xs:annotation>
				<xs:documentation>Origin location - Airport code. If this value not
					provided, the last air segment arrival location is taken as
					default. 1P only. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Destination" type="typeIATACode"
			use="optional">
			<xs:annotation>
				<xs:documentation>Destination location - Airport code.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="StartDate" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation>The start date of the SVC segment. If the value
					not specified, the default value is set as the date next to the
					last airsegment arrival date. 1P only</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
</xs:element>
	
	
	<xs:element name="GuaranteeType" type="typeGeneralText">
		<xs:annotation>
			<xs:documentation>A type of guarantee i.e </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AppliedProfile">
		<xs:annotation>
			<xs:documentation>A simple container to specify the profiles that were applied to a reservation.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Key for update/delete of the element</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelerID" type="xs:string">
				<xs:annotation>
					<xs:documentation>The ID of the TravelerProfile that was applied</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelerName" type="xs:string">
				<xs:annotation>
					<xs:documentation>The name from the TravelerProfile that was applied</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AccountID" type="xs:string">
				<xs:annotation>
					<xs:documentation>The ID of the AccountProfile that was applied</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AccountName" type="xs:string">
				<xs:annotation>
					<xs:documentation>The name from the AccountProfile that was applied</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ImmediateParentID" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The ID of the immediate parent that was applied</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ImmediateParentName" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The name of the immediate parent that was applied</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeGuaranteeInformation">
		<xs:annotation>
			<xs:documentation>Information pertaining to the payment of type Guarantee.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" use="required">
			<xs:annotation>
				<xs:documentation>Guarantee only or Deposit</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Guarantee"/>
					<xs:enumeration value="Deposit"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="AgencyType" use="required">
			<xs:annotation>
				<xs:documentation>Guarantee to Agency IATA or Guarantee to Another Agency IATA</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="AgencyIATA"/>
					<xs:enumeration value="OtherAgencyIATA"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="IATANumber" type="StringLength1to128" use="required">
			<xs:annotation>
				<xs:documentation>Payment IATA number. (ie. IATA of Agency or Other Agency)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--*** Air Reference Elements ***-->
	<xs:element name="AccountCode">
		<xs:annotation>
			<xs:documentation>Account Code is used to get Private Fares.If ProviderCode or SupplierCode is not specified,</xs:documentation>
			<xs:documentation>it will be considered a default AccounCode to be sent to all the Providers or Suppliers. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Code" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="attrProviderSupplier"/>
			<xs:attribute name="Type" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>An identifier to categorize this account code. For example, FlightPass for AC Flight Pass or RFB for AC corporate Rewards for Business.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AirSearchParameters">
		<xs:annotation>
			<xs:documentation>Search Parameters</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="NoAdvancePurchase" type="xs:boolean" use="optional"/>
			<xs:attribute name="RefundableFares" type="xs:boolean" use="optional"/>
			<xs:attribute name="NonPenaltyFares" type="xs:boolean" use="optional"/>
			<xs:attribute name="UnRestrictedFares" type="xs:boolean" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Commission">
		<xs:annotation>
			<xs:documentation>Identifies the agency commission</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Level" type="typeCommissionLevel" use="required">
				<xs:annotation>
					<xs:documentation>The commission percentage level.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Type" type="typeCommissionType" use="required">
				<xs:annotation>
					<xs:documentation>The commission type.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Modifier" type="typeCommissionModifier" use="optional">
				<xs:annotation>
					<xs:documentation>Optional commission modifier.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Amount" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The monetary amount of the commission.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Value" use="optional">
				<xs:annotation>
					<xs:documentation>Contains alphanumeric or alpha characters intended as 1G Value Code as applicable by BSP of client.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>Value of possible commission.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Percentage" type="typePercentageWithDecimal" use="optional">
				<xs:annotation>
					<xs:documentation>The percent of the commission.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to a passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CommissionOverride" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>This is enabled to override CAT-35 commission error during air ticketing. PROVIDER SUPPORTED:Worldspan and JAL</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="DeliveryInfo">
		<xs:annotation>
			<xs:documentation>Container to encapsulate all delivery related information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ShippingAddress" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="typeStructuredAddress"/>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element ref="PhoneNumber" minOccurs="0"/>
				<xs:element ref="Email" minOccurs="0"/>
				<xs:element ref="GeneralRemark" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="ProviderReservationInfoRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Tagging provider reservation info with Delivery Info.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Type" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>An arbitrary identifier to categorize this delivery info</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SignatureRequired" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether a signature shoud be required in order to make the delivery.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TrackingNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The tracking number of the shipping company making the delivery.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ActionStatus">
		<xs:annotation>
			<xs:documentation>Status of the action that will happen or has happened to the air reservation. One Action status for each provider reservation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Remark" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Type" use="required">
				<xs:annotation>
					<xs:documentation>
                        Identifies the type of action (if any) to take on this air reservation. Only TTL, TAU, TAX and TAW can be set by the user.
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="TAW"/>
						<xs:enumeration value="TTL"/>
						<xs:enumeration value="TLCXL"/>
						<xs:enumeration value="ACTIVE"/>
						<xs:enumeration value="CXL"/>
						<xs:enumeration value="TAU">
							<xs:annotation>
								<xs:documentation>Equivalent to TAX in Worldspan</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="TRH"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TicketDate" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Identifies when the action type will happen, or
                        has happened according to the type.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Identifies when the action type will happen, or
                        has happened according to the type.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Provider reservation reference key.
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="QueueCategory" type="typeNonBlanks" use="optional">
				<xs:annotation>
					<xs:documentation>
                        Add Category placement to ticketing queue (required in 1P - default is 00)
                    </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AirportCode" type="typeAirport" use="optional">
				<xs:annotation>
					<xs:documentation>Used with Time Limit to specify the airport location where the ticket is to be issued.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrProviderSupplier">
				<xs:annotation>
					<xs:documentation>Contains the Provider Code of the provider for which this ActionStatus is used and the airline/vendor code issuing the ticket. Used with Time Limit and TAW.</xs:documentation>
				</xs:annotation>
			</xs:attributeGroup>
			<xs:attribute name="PseudoCityCode" type="typePCC" use="optional">
				<xs:annotation>
					<xs:documentation>The Branch PCC in the host system where PNR can be queued for ticketing. When used with TAU it will auto queue to Q10. When used with TAW agent performs manual move to Q.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AccountCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Used with TAW. Used to specify a corporate or in house account code to the PNR as part of ticketing arrangement field.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Endorsement">
		<xs:annotation>
			<xs:documentation>Restrictions or instructions about the fare or ticket</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Value" type="typeEndorsement" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeProviderReservationSpecificInfo">
		<xs:sequence>
			<xs:element ref="OperatedBy" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Cross accrual carrier info </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ProviderReservationInfoRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tagging provider reservation info with LoyaltyCard.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ProviderReservationLevel" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>If true means Loyalty card is applied at ProviderReservation level.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReservationLevel" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>If true means Loyalty card is applied at Universal Record Reservation level e.g. Hotel Reservation, Vehicle Reservation etc.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="LoyaltyCard">
		<xs:annotation>
			<xs:documentation>Provider loyalty card information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ProviderReservationSpecificInfo" type="typeProviderReservationSpecificInfo" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attributeGroup ref="attrLoyalty"/>
			<xs:attribute name="CardNumber" type="typeCardNumber" use="required"/>
			<xs:attribute name="Status" type="xs:string" use="optional"/>
			<xs:attribute name="MembershipStatus" type="xs:string" use="optional"/>
			<xs:attribute name="FreeText" type="xs:string" use="optional"/>
			<xs:attribute name="SupplierType" type="typeProduct" use="optional"/>
			<xs:attribute name="Level" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[a-zA-Z0-9]{1,1}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PriorityCode" type="typePriorityCode" use="optional"/>
			<xs:attribute name="VendorLocationRef" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
