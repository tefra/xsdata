		<xs:attribute name="ODEnabled" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Allow or prohibit Flexible Origin and Destination (choice of airports within a radius) shopping option</xs:documentation>
			</xs:annotation>
		</xs:attribute>
