	<xs:element name="DiscountCard">
		<xs:annotation>
			<xs:documentation>Rail Discount Card Information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Code" type="StringLength1to8" use="required"/>
			<xs:attribute name="Description" type="StringLength1to255" use="optional"/>
			<xs:attribute name="Number" type="typeCardNumber" use="optional"/>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="OperatedBy">
		<xs:annotation>
			<xs:documentation>This is the carrier code to support Cross Accrual</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="typeNonBlanks"/>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Penalty">
		<xs:annotation>
			<xs:documentation>Exchange penalty information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="CancelRefund" type="xs:boolean" use="optional"/>
			<xs:attribute name="NonRefundable" type="xs:boolean" use="optional"/>
			<xs:attribute name="NonExchangeable" type="xs:boolean" use="optional"/>
			<xs:attribute name="CancelationPenalty" type="xs:boolean" use="optional"/>
			<xs:attribute name="ReissuePenalty" type="xs:boolean" use="optional"/>
			<xs:attribute name="NonReissuePenalty" type="xs:boolean" use="optional"/>
			<xs:attribute name="TicketRefundPenalty" type="xs:boolean" use="optional"/>
			<xs:attribute name="ChargeApplicable" type="xs:boolean" use="optional"/>
