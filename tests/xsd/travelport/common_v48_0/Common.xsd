		<xs:restriction base="xs:string">
			<xs:enumeration value="X"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeWildcard">
		<xs:annotation>
			<xs:documentation>Wildcard character is asterisk (*). Wildcard character can be specified at beginning and/or end of string. Wildcard in middle of string is treated as a normal character, not a wildcard. If no wildcard character is provided, one is assumed at end of string.
          </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="typeWildcardMax50">
		<xs:annotation>
			<xs:documentation>Wildcard character is asterisk (*). Wildcard character can be specified at beginning and/or end of string. Wildcard in middle of string is treated as a normal character, not a wildcard. If no wildcard character is provided, one is assumed at end of string.
          </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeReserveRequirement">
		<xs:annotation>
			<xs:documentation>Type of payment required to reserve travel i.e. Hotel Reservation requirement</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:enumeration value="Deposit"/>
			<xs:enumeration value="Guarantee"/>
			<xs:enumeration value="Prepayment"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="LocationAddress" type="typeStructuredAddress"/>
	<xs:simpleType name="typeEmailType">
		<xs:annotation>
			<xs:documentation>An identifier that labels this email address (Personal, Business, Agency, etc)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="128"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="PointOfSale">
		<xs:annotation>
			<xs:documentation>User can use this node to send a specific PCC to access fares allowed only for that PCC. This node gives the capability for fare redistribution at UR level.  For fare redistribution at the stored fare level see AirPricingSolution/AirPricingInfo/AirPricingModifiers/PointOfSale.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="required">
				<xs:annotation>
					<xs:documentation>The provider in which the PCC is defined.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="typePCC" use="required">
				<xs:annotation>
					<xs:documentation>The PCC in the host system.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="IATA" type="typeIATA" use="optional">
				<xs:annotation>
					<xs:documentation>Used for rapid reprice. This field is the IATA associated to this Point of Sale PCC. Providers: 1G/1V</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BookingTravelerInfo">
		<xs:annotation>
			<xs:documentation>Container that will allow modifying Universal record data that is not product specific.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BookingTravelerName" minOccurs="0"/>
				<xs:element ref="NameRemark" minOccurs="0" maxOccurs="1"/>
				<xs:element name="DOB" type="xs:date" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Traveler Date of Birth</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element ref="TravelInfo" minOccurs="0"/>
					<xs:element ref="Email" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="PhoneNumber" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Address" type="typeStructuredAddress" minOccurs="0" maxOccurs="1"/>
					<xs:element name="EmergencyInfo" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="DeliveryInfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Age" type="xs:integer" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="CustomizedNameData" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="AppliedProfile" minOccurs="0" maxOccurs="1"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="TravelerType" type="typePTC" use="optional"/>
			<xs:attribute name="Gender" type="typeGender" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TravelInfo">
		<xs:annotation>
			<xs:documentation>Traveler information details like Travel Purpose and Trip Name</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="TripName" use="optional">
				<xs:annotation>
					<xs:documentation>Trip Name</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TravelPurpose" use="optional">
				<xs:annotation>
					<xs:documentation>Purpose of the trip</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeURVersion">
		<xs:annotation>
			<xs:documentation>Version of the Universal record. Required with any request to modify the existing Universal record.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<!--*** Profile and Provisioning shared ***-->
	<xs:simpleType name="typeAgencyID">
		<xs:annotation>
			<xs:documentation>Our Agency Identifier</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="19"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeSource">
		<xs:annotation>
			<xs:documentation>The source/level at which is item is defined (available through inheritance)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Agency"/>
			<xs:enumeration value="BranchGroup"/>
			<xs:enumeration value="Branch"/>
			<xs:enumeration value="Agent"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeVersion">
		<xs:annotation>
			<xs:documentation>A sequential version number.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeDate">
		<xs:annotation>
			<xs:documentation>Date without time zones YYYY-MM-DD</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date">
			<xs:pattern value="[^:Z].*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="BookingSource">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Used as an override in a booking. </xs:documentation>
			</xs:annotation>
			<xs:attribute name="Code" use="required">
				<xs:annotation>
					<xs:documentation>Alternate booking source code or number.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Type" use="required">
				<xs:annotation>
					<xs:documentation>Type of booking source sent in the Code attribute. Possible values are â€œPseudoCityCodeâ€,â€ ArcNumberâ€,â€ IataNumberâ€, â€œCustomerIdâ€ and â€œBookingSourceOverrrideâ€. â€œBookingSourceOverrrideâ€ is only applicable in VehicleCreateReservationReq. 1P/1J.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="PseudoCityCode"/>
						<xs:enumeration value="ArcNumber"/>
						<xs:enumeration value="IataNumber"/>
						<xs:enumeration value="CustomerId"/>
						<xs:enumeration value="BookingSourceOverride">
							<xs:annotation>
								<xs:documentation>The Booking Source Override is usually used when the car supplier has assigned a number (which can be alpha/numeric) 
								to the agency/e-commerce to use in place of an IATA number. Supported provider(s) : 1P/1J</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeSearchLocation">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="Airport" minOccurs="0"/>
				<xs:element ref="City" minOccurs="0"/>
				<xs:element ref="CityOrAirport" minOccurs="0"/>
				<xs:element ref="CoordinateLocation" minOccurs="0"/>
				<xs:element ref="RailLocation" minOccurs="0"/>
			</xs:choice>
			<xs:element ref="Distance" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeTimeSpec">
		<xs:annotation>
			<xs:documentation>Specifies times as either specific times, or a time range</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="TimeRange" type="typeTimeRange" minOccurs="0"/>
			<xs:element name="SpecificTime" type="typeSpecificTime" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="PreferredTime" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Specifies a time that would be preferred within the time range specified.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeSpecificTime">
		<xs:annotation>
			<xs:documentation>Specify exact times. System will automatically convert to a range according to agency configuration.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Time" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="typeFlexibleTimeSpec">
		<xs:annotation>
			<xs:documentation>A type which can be used for flexible date/time specification -extends the generic type typeTimeSpec to provide extra options for search.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="typeTimeSpec">
				<xs:sequence>
					<xs:element name="SearchExtraDays" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Options to search for extra days on top of the specified date</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="DaysBefore" type="xs:int" use="optional">
								<xs:annotation>
									<xs:documentation>Number of days to search before the specified date</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="DaysAfter" type="xs:int" use="optional">
								<xs:annotation>
									<xs:documentation>Number of days to search after the specified date</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="typePassengerType">
		<xs:annotation>
			<xs:documentation>Passenger type code with optional age information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Optional passenger Name with associated LoyaltyCard may provide benefit when pricing itineraries using Low Cost Carriers. In general, most carriers do not consider passenger LoyalyCard information when initially pricing itineraries.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="LoyaltyCard" minOccurs="0" maxOccurs="999"/>
			<xs:element ref="DiscountCard" minOccurs="0" maxOccurs="9"/>
			<xs:element ref="PersonalGeography" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Passenger personal geography detail to be sent to Host for accessing location specific fares</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Code" type="typePTC" use="required">
			<xs:annotation>
				<xs:documentation>The 3-char IATA passenger type code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Age" type="xs:integer" use="optional"/>
		<xs:attribute name="DOB" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation>Passenger Date of Birth</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Gender" type="typeGender" use="optional">
			<xs:annotation>
				<xs:documentation>The passenger gender type</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PricePTCOnly" type="xs:boolean" use="optional"/>
		<xs:attribute name="BookingTravelerRef" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>This value should be set for Multiple Passengers in the request.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccompaniedPassenger" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation>Container to identify accompanied passenger. Set true means this passenger is accompanied</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ResidencyType" type="typeResidency" use="optional">
			<xs:annotation>
				<xs:documentation>The passenger residence type.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="typeCommissionModifier">
		<xs:annotation>
			<xs:documentation>Optional commission modifier.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FarePercent">
				<xs:annotation>
					<xs:documentation>Commission percentage applied to the fare</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FareAmount">
				<xs:annotation>
					<xs:documentation>Commission amount applied to the fare </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CommissionAmount">
				<xs:annotation>
					<xs:documentation>Specific commission amount to be applied</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LessStandardCommission">
				<xs:annotation>
					<xs:documentation>Indicates commission percentage applied to the fare less the standard commission</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="StandardPlusSupplementaryPercent">
				<xs:annotation>
					<xs:documentation>Indicates commission percentage includes standard and supplementary commission</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SupplementaryPercent">
				<xs:annotation>
					<xs:documentation>Supplementary commission percent which is applied to the fare</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SupplementaryAmount">
				<xs:annotation>
					<xs:documentation>Supplementary commission amount which is applied to the fare</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Guarantee">
		<xs:annotation>
			<xs:documentation>Payment Guarantee</xs:documentation>
			<xs:documentation>Guarantee, Deposit or PrePayment </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="CreditCard" minOccurs="0"/>
				<xs:element ref="OtherGuaranteeInfo" minOccurs="0"/>
			</xs:choice>
			<xs:attribute name="Type" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Guarantee, Deposit for 1G/1V/1P/1J and PrePayment for 1P/1J only</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Key for update/delete of the element</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReuseFOP" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Key of the FOP Key to be reused as this Form of Payment.Only Credit and Debit Card will be supported for FOP Reuse.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ExternalReference" type="typeExternalReference" use="optional"/>
			<xs:attribute name="Reusable" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Indicates whether the form of payment can be reused or not. Currently applicable for Credit and Debit form of payment</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeExternalReference">
		<xs:annotation>
			<xs:documentation>External reference string for Client application to identify the Form of Payment. Element will be a max of 32 hex characters alpha-numeric.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="OtherGuaranteeInfo">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Type" use="required">
						<xs:annotation>
							<xs:documentation>1) IATA/ARC Number
							2) Agency Address
							2) Deposit Taken
							3) Others</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="IATA/ARC Number"/>
								<xs:enumeration value="Agency Address"/>
								<xs:enumeration value="Deposit Taken"/>
								<xs:enumeration value="Others"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeFulfillmentIDType">
		<xs:annotation>
			<xs:documentation>IdentificationType to define how the customer will identify himself when collecting the ticket </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Bahn Card"/>
			<xs:enumeration value="Credit Card"/>
			<xs:enumeration value="Euro Cheque Card"/>
			<xs:enumeration value="Collection Reference"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeFulfillmentType">
		<xs:annotation>
			<xs:documentation>Defines how the client wishes to receive travel documents, e.g. collect ticket at a kiosk, print in agency. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Kiosk"/>
			<xs:enumeration value="Travel Agency"/>
			<xs:enumeration value="Courier"/>
			<xs:enumeration value="Standard Mail"/>
			<xs:enumeration value="Ticketless"/>
			<xs:enumeration value="Ticket Office"/>
			<xs:enumeration value="Express Mail"/>
			<xs:enumeration value="Corporate Kiosk"/>
			<xs:enumeration value="Train Station Service Desk"/>
			<xs:enumeration value="Direct Printing of Ticket"/>
			<xs:enumeration value="Printing of Ticket at Home"/>
			<xs:enumeration value="Digital Printing of Ticket at Home"/>
			<xs:enumeration value="Retrieve Ticket at Eurostar in London"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Payment">
		<xs:annotation>
			<xs:documentation>Payment information - must be used in conjunction with credit card info</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Type" use="required">
				<xs:annotation>
					<xs:documentation>Identifies the type of payment. This can be for an itinerary, a traveler, or a service fee for example.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="AirlineFee">
							<xs:annotation>
								<xs:documentation>Payment for all airline based fees (paper ticket fee, SSR, etc.)</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="DeliveryFee">
							<xs:annotation>
								<xs:documentation>Payment for agency ticket delivery fee</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="Itinerary">
							<xs:annotation>
								<xs:documentation>Payment for all passengers</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="Passenger">
							<xs:annotation>
								<xs:documentation>Payment for a single passenger. The BookingTravelerRef attribute must be set.</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="ServiceFee">
							<xs:annotation>
								<xs:documentation>Payment for a service fee other than an MCO</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="OptionalService">
							<xs:annotation>
								<xs:documentation>Payment for an optional service</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="TicketFee">
							<xs:annotation>
								<xs:documentation>Deprecated</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FormOfPaymentRef" type="typeRef" use="required">
				<xs:annotation>
					<xs:documentation>The credit card that is will be used to make this payment.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>If the type represents a per traveler payment, then this will reference the traveler this payment refers to.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Amount" type="typeMoney" use="required"/>
			<xs:attribute name="AmountType" type="StringLength1to32" use="optional">
				<xs:annotation>
					<xs:documentation>This field displays type of payment amount when it is non-monetary. Presently available/supported value is "Flight Pass Credits".</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ApproximateAmount" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>It stores the converted payment amount in agency's default currency</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Status" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Status to indicate the business association of the payment element.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ServiceRuleType">
		<xs:annotation>
			<xs:documentation>Contains the rules for applying service rules</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ApplicationRules" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The rules to apply the rule to the itinerary</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="RequiredForAllTravelers" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Indicates if the option needs to be applied to all travelers in the itinerary if selected</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RequiredForAllSegments" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Indicates if the option needs to be applied to all segments in the itinerary if selected</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RequiredForAllSegmentsInOD" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Indicates if the option needs to be applied to all segments in a origin / destination (connection flights) if selected for one segment in the OD</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="UnselectedOptionRequired" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>If an UnselectedOption is present in the option, then the Unselected option  needs to be selected even if the option is not selected when this flag is set to true</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SecondaryOptionCodeRequired" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>If set to true, the secondary option code is required for this option</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ApplicationLevel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lists the levels where the option is applied in the itinerary. Some options are applied for the entire itinerary, some for entire segments, etc.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ApplicationLimits" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Adds the limits on the number of options that can be selected for a particular type</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ApplicationLimit" type="OptionalServiceApplicationLimitType" maxOccurs="10">
										<xs:annotation>
											<xs:documentation>The application limits for a particular level</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element ref="ServiceData" minOccurs="0" maxOccurs="999"/>
					</xs:sequence>
					<xs:attribute name="ApplicableLevels">
						<xs:annotation>
							<xs:documentation>Indicates the level in the itinerary when the option is applied.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:list itemType="OptionalServiceApplicabilityType"/>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="ProviderDefinedApplicableLevels" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Indicates the actual provider defined ApplicableLevels which is mapped to Other</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ModifyRules" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Groups the modification rules for the Option</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ModifyRule" maxOccurs="999">
							<xs:annotation>
								<xs:documentation>Indicates modification rules for the particular modification type.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attributeGroup ref="ModificationRulesGroup"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="SupportedModifications">
						<xs:annotation>
							<xs:documentation>Lists the supported modifications for the itinerary.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:list itemType="ModificationType"/>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="ProviderDefinedModificationType" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Indicates the actual provider defined modification type which is mapped to Other</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="SecondaryTypeRules" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Lists the supported Secondary Codes for the optional / additional service.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SecondaryTypeRule" maxOccurs="999">
							<xs:annotation>
								<xs:documentation>Lists a single secondary code for the optional / additional service. </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ApplicationLimit" type="OptionalServiceApplicationLimitType" minOccurs="0" maxOccurs="10"/>
								</xs:sequence>
								<xs:attribute name="SecondaryType" type="typeRef" use="required">
									<xs:annotation>
										<xs:documentation>The unique type to associate a secondary type in an optional service</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Remarks" type="FormattedTextTextType" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>Adds text remarks / rules for the optional / additional service</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Key" type="typeRef" use="required">
			<xs:annotation>
				<xs:documentation>Unique ID to identify an optional service rule</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="OptionalServiceApplicabilityType">
		<xs:annotation>
			<xs:documentation>The different levels at which an optional service may be applied</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Itinerary"/>
			<xs:enumeration value="Passenger"/>
			<xs:enumeration value="Segment"/>
			<xs:enumeration value="PassengerSegment"/>
			<xs:enumeration value="PassengerOD">
				<xs:annotation>
					<xs:documentation>PassengerOD stands for passenger origin destination.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="OptionalServiceApplicationLimitType">
		<xs:annotation>
			<xs:documentation>The optional service application limit</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="OptionalServiceApplicabilityLimitGroup"/>
	</xs:complexType>
	<xs:attributeGroup name="OptionalServiceApplicabilityLimitGroup">
		<xs:annotation>
			<xs:documentation>Holds the limits on how many options of the particular type can be applied.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ApplicableLevel" type="OptionalServiceApplicabilityType" use="required">
			<xs:annotation>
				<xs:documentation>Indicates the applicable level for the option</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderDefinedApplicableLevels" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates the actual provider defined ApplicableLevels which is mapped to Other</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaximumQuantity" type="xs:positiveInteger" use="required">
			<xs:annotation>
				<xs:documentation>The maximum quantity allowed for the type</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MinimumQuantity" type="xs:positiveInteger" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates the minimum number of the option that can be selected.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="OptionalServicesTypeCodeGroup">
		<xs:annotation>
			<xs:documentation>Holds the attributes to identify an option</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Code" type="StringLength1to8" use="required">
			<xs:annotation>
				<xs:documentation>The ACH code for the OptionalService</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="VendorOptionCode" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation>The vendor specific code for the OptionalService</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" use="optional">
			<xs:annotation>
				<xs:documentation>The type of the option</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="AdditionalOption">
						<xs:annotation>
							<xs:documentation>Identifies an additional option which has to be paid for.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="FreeOption">
						<xs:annotation>
							<xs:documentation>Identifies an additional option which can be selected without additional charge</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="DiscountOption">
						<xs:annotation>
							<xs:documentation>Identifies an option included in the base fare, and can be de-selected by the user resulting in a discount in the price</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="SecondaryCode" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation>An additional code for the option required when the option identifies a larger type and specific codes are required by the vendor. For example, sports equipment may be a Code, and the secondary code could be SKIS or BIKE.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SelectedByDefault" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Flag to indicate if the option has been selected by default</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="ModificationRulesGroup">
		<xs:annotation>
			<xs:documentation>Groups the rules for handling options when modifying an itinerary. One attribute group repreents the rules for a particular type of modification supported by the adapter for a single modification type.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Modification" type="ModificationType" use="required">
			<xs:annotation>
				<xs:documentation>The modificaiton for which this rule group applies.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AutomaticallyAppliedOnAdd" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation>Indicates if the option will be automatically added to new segments / passengers in the itinerary.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CanDelete" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates if the option can be deleted from the itinerary without segment or passenger modifications</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CanAdd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates if the option can be added to the itinerary without segment or passenger modification</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Refundable" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates if the price of the option is refundable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProviderDefinedModificationType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates the actual provider defined modification type which is mapped to Other</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="ModificationType">
		<xs:annotation>
			<xs:documentation>The modification types supported</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="AddSegment">
				<xs:annotation>
					<xs:documentation xml:lang="en">Add a segment to the itinerary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RemoveSegment">
				<xs:annotation>
					<xs:documentation xml:lang="en">Delete a segment from the itinerary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ReplaceSegment">
				<xs:annotation>
					<xs:documentation xml:lang="en">Replace a segment in the itinerary with a new segment</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AddPassenger">
				<xs:annotation>
					<xs:documentation xml:lang="en">Add a passenger to the itinerary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RemovePassenger">
				<xs:annotation>
					<xs:documentation xml:lang="en">Remove a passenger from the itinerary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OptionsOnly">
				<xs:annotation>
					<xs:documentation>Modification where only options are added / removed from the itinerary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other">
				<xs:annotation>
					<xs:documentation>Other modification types</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to32">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 32.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to255">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 255.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="FormattedTextTextType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Provides text and indicates whether it is formatted or not.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="Formatted" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">Textual information, which may be formatted as a line of information, or unformatted, as a paragraph of text.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="LanguageGroup">
					<xs:annotation>
						<xs:documentation xml:lang="en">The language in which the text is provided.</xs:documentation>
					</xs:annotation>
				</xs:attributeGroup>
				<xs:attribute name="TextFormat" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">Indicates the format of text used in the description e.g. unformatted  or html.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="PlainText">
								<xs:annotation>
									<xs:documentation xml:lang="en">Textual data that is in ASCII format.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="HTML">
								<xs:annotation>
									<xs:documentation xml:lang="en">HTML formatted text.</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:attributeGroup name="LanguageGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifies language.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Language" type="xs:language" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Language identification.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="StringLength1to64">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 64.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="64"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to50">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 64.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringLength1to8">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for Character Strings, length 1 to 8.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeAccountID">
		<xs:annotation>
			<xs:documentation>Account Identifier</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="19"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeAgentCode">
		<xs:annotation>
			<xs:documentation>The unique identifier of an agent.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z0-9\-_\.@ ]{1,128}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeTravelerID">
		<xs:annotation>
			<xs:documentation>Traveler Identifier</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="19"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeCreditCardNumber">
		<xs:annotation>
			<xs:documentation>The associated credit/debit card number without spaces or dashes.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="13"/>
			<xs:maxLength value="128"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRateCategory">
		<xs:annotation>
			<xs:documentation>The category of the rate (Best, etc)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Association"/>
			<xs:enumeration value="Business"/>
			<xs:enumeration value="Corporate"/>
			<xs:enumeration value="Government"/>
			<xs:enumeration value="Industry"/>
			<xs:enumeration value="Package"/>
			<xs:enumeration value="Inclusive"/>
			<xs:enumeration value="Promotional"/>
			<xs:enumeration value="Credential"/>
			<xs:enumeration value="Standard"/>
			<xs:enumeration value="Consortium"/>
			<xs:enumeration value="Convention"/>
			<xs:enumeration value="Negotiated"/>
			<xs:enumeration value="Prepay"/>
			
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeDiscountNumber">
		<xs:annotation>
			<xs:documentation>A supplier-specific number which may identify a special rate associated with a traveler's organization</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="25"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeVehicleLocation">
		<xs:annotation>
			<xs:documentation>The type of location requested, such as resort, city center.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Terminal"/>
			<xs:enumeration value="ShuttleOnAirport"/>
			<xs:enumeration value="ShuttleOffAirport"/>
			<xs:enumeration value="RailwayStation"/>
			<xs:enumeration value="Hotel"/>
			<xs:enumeration value="CarDealer"/>
			<xs:enumeration value="CityCenterDowntown"/>
			<xs:enumeration value="EastOfCityCenter"/>
			<xs:enumeration value="SouthOfCityCenter"/>
			<xs:enumeration value="WestOfCityCenter"/>
			<xs:enumeration value="NorthOfCityCenter"/>
			<xs:enumeration value="PortOrFerry"/>
			<xs:enumeration value="NearResort"/>
			<xs:enumeration value="Airport"/>
			<xs:enumeration value="Unknown"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRateCode">
		<xs:annotation>
			<xs:documentation>The code of this rate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRateTimePeriod">
		<xs:annotation>
			<xs:documentation>The period for the rate code (daily, weekly, etc)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Hourly"/>
			<xs:enumeration value="Daily"/>
			<xs:enumeration value="Weekly"/>
			<xs:enumeration value="Monthly"/>
			<xs:enumeration value="WeekendDay"/>
			<xs:enumeration value="Other"/>
			<xs:enumeration value="Package"/>
			<xs:enumeration value="Bundle"/>
			<xs:enumeration value="Total"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRatePlanType">
		<xs:annotation>
			<xs:documentation>Represents the rate plan code of room type for specified hotel property.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:element name="CorporateDiscountID">
		<xs:annotation>
			<xs:documentation>These are zero or more negotiated rate codes
        </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="NegotiatedRateCode" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">When set to true,
                            the data in the CorporateDiscountID is a negotiated rate code.
                            Otherwise, this data is a Corporate Discount ID rate.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeHotelChainCode">
		<xs:annotation>
			<xs:documentation>2 Letter Hotel Chain Code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeHotelCode">
		<xs:annotation>
			<xs:documentation>Unique hotel identifier for the channel</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeFlightNumber">
		<xs:annotation>
			<xs:documentation>flight number type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AgencyContactInfo">
		<xs:annotation>
			<xs:documentation>Generic agency contact information container. It must contain  at least one phone number to be used by an agency</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PhoneNumber" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeFareFamily">
		<xs:annotation>
			<xs:documentation>An alpha-numeric string which denotes fare family. 
			Some carriers may return this in lieu of or in addition to the CabinClass. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeFareBasisCode">
		<xs:annotation>
			<xs:documentation>The fare basis code to be used for pricing.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeMaxResults">
		<xs:annotation>
			<xs:documentation>
				Used to limit the number of results returned,
				particularly in more general searches that may
				return a large result set.
   		</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeStartFromResult">
		<xs:annotation>
			<xs:documentation>
				Used to browse beyond the maximum number of results
				specified with the MaxResults parameter. Acts as an
				offset to skip the specified number of PNRs from the
				begining of the result set.
   		</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeMoreResults">
		<xs:annotation>
			<xs:documentation>
				Used to browse beyond the maximum number of results
				specified with the MaxResults parameter. Acts as an
				offset to skip the specified number of PNRs from the
				begining of the result set.
   		</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:element name="ConnectionPoint" type="typeLocation">
		<xs:annotation>
			<xs:documentation>A connection point can be eith an IATA airport or cir city code.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="typeLocation">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="Airport" minOccurs="0"/>
				<xs:element ref="City" minOccurs="0"/>
				<xs:element ref="CityOrAirport" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="typeCardNumber">
		<xs:annotation>
			<xs:documentation>Loyalty Card number with maximum length as 36.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="36"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ReferencePoint" type="typeReferencePoint"/>
	<xs:simpleType name="typeReferencePoint">
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeProfileLevelWithSystem">
		<xs:annotation>
			<xs:documentation>The "profile level" used for association of workflow etc.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="System"/>
			<xs:enumeration value="Agency"/>
			<xs:enumeration value="Branch"/>
			<xs:enumeration value="BranchGroup"/>
			<xs:enumeration value="Agent"/>
			<xs:enumeration value="Account"/>
			<xs:enumeration value="TravelerGroup"/>
			<xs:enumeration value="Traveler"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="typeProfileRef">
		<xs:annotation>
			<xs:documentation>ProfileEntityID and ProfileLevel together identity a profile entity.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ProfileEntityID" type="xs:string" use="required"/>
		<xs:attribute name="ProfileLevel" type="typeProfileLevel" use="required"/>
	</xs:complexType>
	<xs:element name="ServiceFeeTaxInfo">
		<xs:annotation>
			<xs:documentation>
              The taxes associated to a particular Service Fee.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Category" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The tax category represents a valid IATA tax code. </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Amount" type="typeMoney" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeStatus">
		<xs:annotation>
			<xs:documentation>The status of the service fees.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Issued">
				<xs:annotation>
					<xs:documentation>The service fee has been issued.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ReadyToIssue">
				<xs:annotation>
					<xs:documentation>The service fee is ready to be issued.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IssueLater">
				<xs:annotation>
					<xs:documentation>The service fee can be issued later.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ServiceFeeInfo">
		<xs:annotation>
			<xs:documentation>
            Travel Agency Service Fees (TASF) are charged by the agency through BSP or Airline Reporting Corporation (ARC).
         </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FormOfPayment" minOccurs="0"/>
				<xs:element ref="ServiceFeeTaxInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="CreditCardAuth" minOccurs="0"/>
				<xs:element ref="Payment" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Status" type="typeStatus" use="optional">
				<xs:annotation>
					<xs:documentation>Status of the service fee. Possible Values â€“ Issued, ReadyToIssue, IssueLater.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Description" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The description of the service fee.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="Confirmation" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The confirmation number of the service fee in the merchant host system.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketNumber" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The ticket that this fee was issued in connection with.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to a passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to the provider reservation info to which the service is tied.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassiveProviderReservationInfoRef" type="typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>A reference to the passive provider reservation info to which the service is tied.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TotalAmount" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The total amount for this Service Fee including base amount and all taxes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BaseAmount" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Represents the base price for this entity. This does not include any taxes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Taxes" type="typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The aggregated amount of all the taxes that are associated with this entity. See the associated Service Fee TaxInfo array for a breakdown of the individual taxes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
			<xs:attribute name="BookingTravelerName" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The name of the passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="LinkedUniversalRecord">
		<xs:complexType>
			<xs:attribute name="LocatorCode" type="typeLocatorCode" use="required">
				<xs:annotation>
					<xs:documentation>A Universal Record that need to be linked to the current Universal Record.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeDateRange">
		<xs:annotation>
			<xs:documentation>Specify a range of dates</xs:documentation>
		</xs:annotation>
		<xs:attribute name="StartDate" type="xs:date" use="required"/>
		<xs:attribute name="EndDate" type="xs:date" use="required"/>
	</xs:complexType>
	<xs:simpleType name="typeAgencyProfileLevel">
		<xs:annotation>
			<xs:documentation>Profile levels in the Agency Hierarchy.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Agency"/>
			<xs:enumeration value="Branch"/>
			<xs:enumeration value="BranchGroup"/>
			<xs:enumeration value="Agent"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="MarketingInformation">
		<xs:annotation>
			<xs:documentation>Marketing text or Notices for Suppliers
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Text" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Postscript">
		<xs:annotation>
			<xs:documentation>Postscript Notes</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeRemark">
					<xs:attribute name="Key" type="typeRef" use="optional"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeThirdPartySupplier">
		<xs:annotation>
			<xs:documentation>Third Party Content Provider name.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="64"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRateGuarantee">
		<xs:annotation>
			<xs:documentation>The guarantee for this rate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Rate Guaranteed"/>
			<xs:enumeration value="Rate Quoted"/>
			<xs:enumeration value="Agent Entered"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="typeAgencyHierarchyReference">
		<xs:attribute name="ProfileID" type="typeProfileID" use="required"/>
		<xs:attribute name="ProfileType" type="typeAgencyProfileLevel" use="required"/>
	</xs:complexType>
	<xs:complexType name="typeAgencyHierarchyLongReference">
		<xs:complexContent>
			<xs:extension base="typeAgencyHierarchyReference">
				<xs:attribute name="ProfileVersion" type="xs:int" use="required"/>
				<xs:attribute name="ProfileName" use="required">
					<xs:annotation>
						<xs:documentation>Initially: Agent: Last, First, Branch: BranchCode, Agency: Name. After new profile  implementation: Agent: UserName, others levels: Name.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="102"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DriversLicense">
		<xs:annotation>
			<xs:documentation>Details of drivers license</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="typeRef" use="optional"/>
			<xs:attribute name="LicenseNumber" use="required">
				<xs:annotation>
					<xs:documentation>The driving license number of the booking traveler.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeLicenseCode">
		<xs:annotation>
			<xs:documentation>The type of license assigned to an agent.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Standard"/>
			<xs:enumeration value="Standard Plus"/>
			<xs:enumeration value="Enterprise"/>
			<xs:enumeration value="TE Only"/>
			<xs:enumeration value="uAPI"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="typeAssociatedRemarkWithSegmentRef">
		<xs:annotation>
			<xs:documentation>A textual remark container to hold Associated itinerary remarks with segment association</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="typeAssociatedRemark">
				<xs:attribute name="SegmentRef" type="typeRef" use="optional">
					<xs:annotation>
						<xs:documentation>Reference to an Air/Passive Segment</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="typeAssociatedRemark">
		<xs:annotation>
			<xs:documentation>A textual remark container to hold Associated itinerary remarks</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="typeRemarkWithTravelerRef">
				<xs:attribute name="Key" type="typeRef" use="optional"/>
				<xs:attributeGroup ref="attrElementKeyResults"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="typePolicy">
		<xs:annotation>
			<xs:documentation>Available product types</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Air"/>
			<xs:enumeration value="Vehicle"/>
			<xs:enumeration value="Hotel"/>
			<xs:enumeration value="Rail"/>
			<xs:enumeration value="Ticketing"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ShopInformation">
		<xs:annotation>
			<xs:documentation>Shopping Information required for File
				Finishing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SearchRequest" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Search parameters that were used in LFS
							request</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Origin" type="typeIATACode"/>
						<xs:attribute name="Destination" type="typeIATACode"/>
						<xs:attribute name="DepartureTime" type="xs:string">
							<xs:annotation>
								<xs:documentation>Date and Time at which this entity
									departs. This does not include Time Zone information since it
									can be derived from origin location</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ClassOfService" type="typeClassOfService"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="FlightsOffered" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Flights with lowest logical airfare
							returned as response to LFS request</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Origin" type="typeIATACode"/>
						<xs:attribute name="Destination" type="typeIATACode"/>
						<xs:attribute name="DepartureTime" type="xs:string">
							<xs:annotation>
								<xs:documentation>Date and Time at which this entity
									departs. This does not include Time Zone information since it
									can be derived from origin location</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="TravelOrder" type="xs:int"/>
						<xs:attribute name="Carrier" type="typeCarrier"/>
						<xs:attribute name="FlightNumber" type="typeFlightNumber"/>
						<xs:attribute name="ClassOfService" type="typeClassOfService"/>
						<xs:attribute name="StopOver" type="xs:boolean" default="false"/>
						<xs:attribute name="Connection" type="xs:boolean" default="false"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="CabinShopped" type="xs:string"/>
			<xs:attribute name="CabinSelected" type="xs:string"/>
			<xs:attribute name="LowestFareOffered" type="typeMoney"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PolicyInformation">
		<xs:annotation>
			<xs:documentation>Policy Information required for File
				Finishing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ReasonCode" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reason Code</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="OutOfPolicy" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Reason Code - Out of Policy
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PurposeOfTrip" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Reason Code -Purpose of Trip
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="Remark" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Type" type="typePolicy" use="required">
				<xs:annotation>
					<xs:documentation>Policy Type - Air, Hotel, Car, Rail,
						Ticketing</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Name">
				<xs:annotation>
					<xs:documentation>Policy Name</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OutOfPolicy" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>In Policy / Out of Policy Indicator
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SegmentRef" type="typeRef" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgencyInformation">
		<xs:annotation>
			<xs:documentation>Agency Information required for File
				Finishing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Address" type="typeStructuredAddress" minOccurs="0"/>
				<xs:element ref="Email" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="PhoneNumber" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AccountInformation">
		<xs:annotation>
			<xs:documentation>Account Information required for File
				Finishing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Address" type="typeStructuredAddress" minOccurs="0"/>
				<xs:element ref="PhoneNumber" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="AccountName" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TravelerInformation">
		<xs:annotation>
			<xs:documentation>Traveler Information required for File
				Finishing</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="EmergencyContact" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="PhoneNumber" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="Name">
							<xs:annotation>
								<xs:documentation>Name of Emergency Contact Person
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Relationship">
							<xs:annotation>
								<xs:documentation>Relationship between Traveler and
									Emergency Contact Person </xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="HomeAirport" type="typeAirport"/>
			<xs:attribute name="VisaExpirationDate" type="xs:date"/>
			<xs:attribute name="BookingTravelerRef" type="typeRef" use="required">
				<xs:annotation>
					<xs:documentation>A reference to a passenger.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustomProfileInformation">
		<xs:annotation>
			<xs:documentation>Custom Profile Field Data required for File
				Finishing</xs:documentation>
		</xs:annotation>
		<xs:complexType/>
	</xs:element>
	<xs:element name="FileFinishingInfo">
		<xs:annotation>
			<xs:documentation>Misc Data required for File Finishing. This
				data is transient and not saved in database.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ShopInformation" minOccurs="0"/>
				<xs:element ref="PolicyInformation" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Policy Information required for File
							Finishing. Would repeat per Policy Type</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="AccountInformation" minOccurs="0"/>
				<xs:element ref="AgencyInformation" minOccurs="0"/>
				<xs:element ref="TravelerInformation" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="CustomProfileInformation" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeClassOfService">
		<xs:annotation>
			<xs:documentation>Class of service code (Booking code) usually
				one letter, rarely two.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeSSRCode">
		<xs:annotation>
			<xs:documentation>SSR Code, exactly 4 characters (e.g. DEAF, NSST, etc.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="4"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeProfileEntityStatus">
		<xs:annotation>
			<xs:documentation>Status of the given profile/entity. Any profile with a status other than Active cannot perform most transactions.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Active"/>
			<xs:enumeration value="Inactive"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeProfileEntityStatusWithDelete">
		<xs:annotation>
			<xs:documentation>Specify whether the change is to update or delete the field.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Deleted"/>
			<xs:enumeration value="Active"/>
			<xs:enumeration value="Inactive"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="BillingPointOfSaleInfo">
		<xs:annotation>
			<xs:documentation>
				Point of Sale information for Billing
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="OriginApplication" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>
						Name of the Point of Sale application which initiated the Request.This information will be provided as part of the provisioning of the user.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CIDBNumber" use="optional">
				<xs:annotation>
					<xs:documentation>
						A 10 Digit customer number generated by CIDB system.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="\d{10}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgentIDOverride">
		<xs:annotation>
			<xs:documentation>
                Vendor specific agent identifier overrides to be used to access vendor systems.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="SupplierCode" type="typeSupplierCode" use="optional">
				<xs:annotation>
					<xs:documentation>Supplier code to determine which vendor this AgentId belongs to.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="required">
				<xs:annotation>
					<xs:documentation>Provider code to route the AgentId to proper provider.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="AgentID" use="required">
				<xs:annotation>
					<xs:documentation>The Agent ID for the applicable supplier/vendor</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OverridePCC">
		<xs:annotation>
			<xs:documentation>Used to emulate to another PCC or SID.  Providers: 1G, 1V, 1P, 1J.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="required">
				<xs:annotation>
					<xs:documentation>The code of the provider (e.g. 1G, 1S)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PseudoCityCode" type="typePCC" use="required">
				<xs:annotation>
					<xs:documentation>The PCC in the host system.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeKeyword">
		<xs:annotation>
			<xs:documentation>A complexType for keyword information.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SubKey" type="typeSubKey" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>A further breakdown of a keyword.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Text" type="xs:string" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Information for a keyword.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Name" use="required">
			<xs:annotation>
				<xs:documentation>The keyword name.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="12"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Number">
			<xs:annotation>
				<xs:documentation>The number for this keyword.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description">
			<xs:annotation>
				<xs:documentation>A brief description of the keyword</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LanguageCode" type="xs:language">
			<xs:annotation>
				<xs:documentation>ISO 639 two-character language codes are used to retrieve specific information in the requested language. For Rich Content and Branding, language codes ZH-HANT (Chinese Traditional), ZH-HANS (Chinese Simplified), FR-CA (French Canadian) and PT-BR (Portuguese Brazil) can also be used. For RCH, language codes ENGB, ENUS, DEDE, DECH can also be used. Only certain services support this attribute. Providers: ACH, RCH, 1G, 1V, 1P, 1J.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeSubKey">
		<xs:annotation>
			<xs:documentation>The attributes and elements in a SubKey.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Text" type="xs:string" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Information for a sub key.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Name" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>A subkey to identify the specific information within this keyword</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string">
			<xs:annotation>
				<xs:documentation>A brief description of a subkey.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeOTAKeyword">
		<xs:annotation>
			<xs:documentation>A complexType for keyword information.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SubKey" type="typeOTASubKey" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>A further breakdown of a keyword.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Text" type="xs:string" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Information for a keyword.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Name" use="required">
			<xs:annotation>
				<xs:documentation>The keyword name.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="6"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Number">
			<xs:annotation>
				<xs:documentation>The number for this keyword.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description">
			<xs:annotation>
				<xs:documentation>A brief description of the keyword</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeOTASubKey">
		<xs:annotation>
			<xs:documentation>The attributes and elements in a SubKey.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Text" type="xs:string" minOccurs="0" maxOccurs="999">
				<xs:annotation>
					<xs:documentation>Information for a sub key.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Name" type="typeOTACode" use="required">
			<xs:annotation>
				<xs:documentation>A subkey to identify the special equipment codes. Applicable when Policy/@Name is EQUIP. Uses OTA CODE "EQP". 1P/1J.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string">
			<xs:annotation>
				<xs:documentation>A brief description of a subkey.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Keyword">
		<xs:annotation>
			<xs:documentation>Detail information of keywords.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeKeyword"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BookingDates">
		<xs:annotation>
			<xs:documentation>Check in and Check out Date information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="CheckInDate" type="typeDate" use="optional"/>
			<xs:attribute name="CheckOutDate" type="typeDate" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="ProviderReservation">
		<xs:attribute name="ProviderCode" type="typeProviderCode" use="required"/>
		<xs:attribute name="ProviderLocatorCode" type="typeProviderLocatorCode" use="required"/>
	</xs:attributeGroup>
	<xs:element name="PaymentRestriction">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CardRestriction" maxOccurs="999"/>
				<xs:element ref="AddressRestriction"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RequiredField">
		<xs:complexType>
			<xs:attribute name="Name" use="required">
				<xs:annotation>
					<xs:documentation>The name of the required field</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="CardType"/>
						<xs:enumeration value="Number"/>
						<xs:enumeration value="CustomerReference"/>
						<xs:enumeration value="IssueNumber"/>
						<xs:enumeration value="StartDate"/>
						<xs:enumeration value="NameOnCard"/>
						<xs:enumeration value="ExpirationDate"/>
						<xs:enumeration value="CVV"/>
						<xs:enumeration value="AddressLine1"/>
						<xs:enumeration value="AddressLine2"/>
						<xs:enumeration value="City"/>
						<xs:enumeration value="State"/>
						<xs:enumeration value="PostalCode"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CardRestriction">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RequiredField" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Code" type="typeCardMerchantType" use="required">
				<xs:annotation>
					<xs:documentation>2 letter Credit/Debit Card merchant type
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Name" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Card merchant description</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddressRestriction">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RequiredField" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeCardMerchantType">
		<xs:annotation>
			<xs:documentation>2 letter Credit/Debit Card merchant type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="SSRInfo">
		<xs:annotation>
			<xs:documentation>Bundle SSR with BookingTraveler reference in order to add SSR post booking</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SSR"/>
				<xs:element name="BookingTravelerRef" type="typeRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Reference to Booking Traveler.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeNonAirReservationRef">
		<xs:attribute name="LocatorCode" type="typeLocatorCode" use="required"/>
	</xs:complexType>
	<xs:complexType name="typeSegmentRef">
		<xs:attribute name="Key" type="typeRef" use="required"/>
	</xs:complexType>
	<xs:element name="InvoiceRemark">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeAssociatedRemark">
					<xs:choice>
						<xs:element name="AirSegmentRef" type="typeSegmentRef" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Reference to AirSegment from an Air Reservation.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="HotelReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Specify the locator code of Hotel reservation.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VehicleReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Specify the locator code of Vehicle reservation.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PassiveSegmentRef" type="typeSegmentRef" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Reference to PassiveSegment from a Passive Reservation.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RequestKeyMappings">
		<xs:annotation>
			<xs:documentation>All the elements for which mapping key sent in the request is different from the mapping key comes in the response.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="KeyMapping" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="KeyMapping">
		<xs:annotation>
			<xs:documentation>Element for which mapping key sent in the request is different from the mapping key comes in the response.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ElementName" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Name of the element.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OriginalKey" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The mapping key which is sent in the request.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="NewKey" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>The mapping key that comes in the response.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="PageAttributes">
		<xs:annotation>
			<xs:documentation>Attributes to control pagination.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="MaxResults" type="typeMaxResults" use="optional"/>
		<xs:attribute name="StartFromResult" type="typeStartFromResult" use="optional"/>
	</xs:attributeGroup>
	<xs:element name="ReservationName">
		<xs:annotation>
			<xs:documentation>Container to represent reservation name as appears in GDS booking</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="BookingTravelerRef"/>
				<xs:element ref="NameOverride">
					<xs:annotation>
						<xs:documentation>To be used if the reservation name is other than booking travelers in the PNR</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="NameOverride">
		<xs:annotation>
			<xs:documentation>To be used if the name is different from booking travelers in the PNR</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="First" use="required">
				<xs:annotation>
					<xs:documentation>First Name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Last" use="required">
				<xs:annotation>
					<xs:documentation>Last Name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Age" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>Age.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- Moved APIProvider element from provisioning schema -->
	<xs:element name="APIProvider">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TransactionType" minOccurs="0"/>
				<xs:element name="AvailablePseudoCityCode" minOccurs="0" maxOccurs="999">
					<xs:complexType>
						<xs:attribute name="PseudoCityCode" type="typePCC" use="optional">
							<xs:annotation>
								<xs:documentation>The PseudoCityCode used to connect to the host.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ProviderCode" type="typeProviderCode" use="required">
				<xs:annotation>
					<xs:documentation>The Provider Code of the host</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SupplierCode" type="typeSupplierCode" use="optional">
				<xs:annotation>
					<xs:documentation>The Supplier Code of the host</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IATACode" type="typeIATA">
				<xs:annotation>
					<xs:documentation>Agency IATA or ARC code, used as an ID with airlines.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="TransactionType">
		<xs:annotation>
			<xs:documentation>Configuration for products by type. Inheritable.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Air" minOccurs="0">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="typeTransactionsAllowed">
								<xs:attributeGroup ref="attrFlexShopping"/>
								<xs:attribute name="OneWayShop" type="xs:boolean">
									<xs:annotation>
										<xs:documentation>Allows or prohibits one way shopping functionality for the associated provisioning provider configuration</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="FlexExplore" type="xs:boolean">
									<xs:annotation>
										<xs:documentation>Allows or prohibits flex explore functionality for the associated provisioning provider configuration</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="RapidRepriceEnabled" type="xs:boolean">
									<xs:annotation>
										<xs:documentation>Allows or prohibits rapid reprice functionality for the associated provisioning provider configuration. Providers: 1G/1V</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="ReturnUpsellFare" type="xs:boolean">
									<xs:annotation>
										<xs:documentation>When set to â€œtrueâ€, Upsell information will be returned in the shop response.Â  Provider: 1G, 1V, 1P, 1J, ACH</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Hotel" type="typeTransactionsAllowed" minOccurs="0"/>
				<xs:element name="Rail" type="typeTransactionsAllowed" minOccurs="0"/>
				<xs:element name="Vehicle" type="typeTransactionsAllowed" minOccurs="0"/>
				<xs:element name="Passive" type="typeBookingTransactionsAllowed" minOccurs="0">
					<xs:annotation>
						<xs:documentation>For true passive segments such as ground, cruise etc</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BackgroundPassive" type="typeBookingTransactionsAllowed" minOccurs="0">
					<xs:annotation>
						<xs:documentation>For behind the scenes or background passives Only</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeTransactionsAllowed">
		<xs:complexContent>
			<xs:extension base="typeBookingTransactionsAllowed">
				<xs:attribute name="ShoppingEnabled" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Allow or prohibit shopping transaction for the given product type on this Provider/Supplier. Inheritable.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="PricingEnabled" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Allow or prohibit pricing transaction for the given product type on this Provider/Supplier. Inheritable.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="typeBookingTransactionsAllowed">
		<xs:attribute name="BookingEnabled" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Allow or prohibit booking transaction for the given product type on this Provider/Supplier. Inheritable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:attributeGroup name="attrFlexShopping">
		<xs:attribute name="Tier">
			<xs:annotation>
				<xs:documentation>Indicate the Tier Level</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:enumeration value="1"/>
					<xs:enumeration value="2"/>
					<xs:enumeration value="3"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="DaysEnabled" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Allow or prohibit Flexible Days (within a date range) shopping option</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="WeekendsEnabled" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Allow or prohibit Flexible Weekends shopping option</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AirportsEnabled" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Allow or prohibit Flexible Airport (choice of Origin and Destination airports) shopping option</xs:documentation>
