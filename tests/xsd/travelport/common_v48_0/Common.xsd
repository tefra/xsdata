									<xs:documentation>Reference to PassiveSegment from a Passive
										Reservation.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="NextSegment" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:choice>
							<xs:element name="AirSegmentRef" type="typeSegmentRef" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Reference to AirSegment from an Air
										Reservation.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="HotelReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Specify the locator code of Hotel reservation.
									</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="VehicleReservationRef" type="typeNonAirReservationRef" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Specify the locator code of Vehicle
										reservation.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PassiveSegmentRef" type="typeSegmentRef" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Reference to PassiveSegment from a Passive
										Reservation.</xs:documentation>
