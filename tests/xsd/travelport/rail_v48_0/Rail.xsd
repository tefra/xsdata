					<xs:documentation>FareComponent amount</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Age" type="xs:integer" use="optional"/>
			<xs:attribute name="PassengerTypeCode" type="common:typePTC" use="optional">
