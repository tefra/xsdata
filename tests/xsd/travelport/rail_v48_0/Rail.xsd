			<xs:attribute name="RefundAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Amount refunded back to customer.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CancellationFee" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>Cancellation penalty imposed by the distributor.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Refund" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether vendor offers refund on rail reservation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Retain" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Indicates whether vendor retains the amount to be used later.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RetainAmount" type="common:typeMoney" use="optional">
