			<xs:documentation>Search flexibiity criteria .</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="common:DiscountCard" minOccurs="0" maxOccurs="9">
					<xs:annotation>
						<xs:documentation>Discount request for rail.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ProhibitNonRefundableFares" type="xs:boolean" use="optional" default="false">
