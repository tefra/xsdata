<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.travelport.com/schema/rail_v48_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://www.travelport.com/schema/common_v48_0" targetNamespace="http://www.travelport.com/schema/rail_v48_0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation>(c) Travelport GDS 2008 All rights reserved. Proprietary Information of Travelport GDS.</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://www.travelport.com/schema/common_v48_0" schemaLocation="../common_v48_0/Common.xsd"/>
	<xs:element name="RailReservation">
		<xs:annotation>
			<xs:documentation>The parent container for all Rail booking data</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:BaseReservation">
					<xs:sequence>
						<xs:element ref="common:BookingTravelerRef" maxOccurs="9"/>
						<xs:element ref="RailJourney" maxOccurs="999"/>
						<xs:element ref="RailPricingInfo" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="common:Payment" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="RailTicketInfo" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="RailFareNoteList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>List of RailFareNote(s) that is referenced by key in RailFare.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="common:SupplierLocator" minOccurs="0" maxOccurs="999"/>
					</xs:sequence>
					<xs:attribute name="BookingStatus" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>The Current Status of the rail booking.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailJourney">
		<xs:annotation>
			<xs:documentation>Captures all journey-related data</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:annotation>
						<xs:documentation>Use references on rail availability response, and the full segment on the reservation.</xs:documentation>
					</xs:annotation>
					<xs:element ref="RailSegment" maxOccurs="999"/>
					<xs:element ref="RailSegmentRef" maxOccurs="999"/>
				</xs:choice>
				<xs:element ref="JourneyRemark" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:HostToken" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attributeGroup ref="attrRailSegmentOrigDestInfo"/>
			<xs:attribute name="RouteDescription" use="optional">
				<xs:annotation>
					<xs:documentation>The description of the route.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="JourneyDirection" type="typeJourneyDirection" use="optional">
				<xs:annotation>
					<xs:documentation>The direction of the Journey (Outward or Return).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="JourneyDuration" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>The duration of the entire Journey in minutes</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrPrices"/>
			<xs:attributeGroup ref="common:attrProviderSupplier"/>
			<xs:attribute name="Status" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Status of this Journey.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProviderReservationInfoRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Provider reservation reference key.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassiveProviderReservationInfoRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Passive provider reservation reference key.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TravelOrder" type="xs:integer" use="optional">
				<xs:annotation>
					<xs:documentation>To identify the appropriate travel sequence for Air/Car/Hotel/Rail segments/reservations/Journeys based on travel dates. This ordering is applicable across the UR not provider or traveler specific</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RouteReference" use="optional">
				<xs:annotation>
					<xs:documentation>RouteReference is required in seat assignment purpose </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
			<xs:attribute name="Operation" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>"Type of exchange. Add - Add new Journey. Update - Modify existing Journey. Delete - Remove existing Journey"</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailTicketInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RailJourneyRef" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="TicketAdvisory" minOccurs="0" maxOccurs="10"/>
			</xs:sequence>
			<xs:attribute name="Number" use="required">
				<xs:annotation>
					<xs:documentation>Ticket number.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="19"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="IssueLocation" use="optional">
				<xs:annotation>
					<xs:documentation>Issue location is internal distributor code associated with the PCC.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="128"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TicketStatus" type="common:StringLength1to255" use="optional">
				<xs:annotation>
					<xs:documentation>Status of Ticket.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketFormType" use="optional">
				<xs:annotation>
					<xs:documentation>FormType of Ticket.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TrafficType" type="common:StringLength1to255" use="optional">
				<xs:annotation>
					<xs:documentation>Type of traffic.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IssuedDate" type="xs:dateTime" use="optional">
				<xs:annotation>
					<xs:documentation>Ticket issue date.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketType" type="common:StringLength1to255" use="optional">
				<xs:annotation>
					<xs:documentation>Type of ticket. Paper, eTicket etc.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BookingTravelerRef" type="common:typeRef" use="optional">
				<xs:annotation>
					<xs:documentation>Reference to a BookingTraveler.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SearchRailLeg">
		<xs:annotation>
			<xs:documentation>Holds Origin, Destination, and Departure times for a Rail Leg to search for.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SearchOrigin" type="common:typeSearchLocation" maxOccurs="999"/>
				<xs:element name="SearchDestination" type="common:typeSearchLocation" maxOccurs="999"/>
				<xs:element ref="RailSegmentList" minOccurs="0"/>
				<xs:choice>
					<xs:annotation>
						<xs:documentation>Availability can be searched either on departure time or on arrival tome, but not on both.</xs:documentation>
					</xs:annotation>
					<xs:element name="SearchDepTime" type="common:typeFlexibleTimeSpec" maxOccurs="999"/>
					<xs:element name="SearchArvTime" type="common:typeTimeSpec" maxOccurs="999"/>
				</xs:choice>
				<xs:element ref="RailLegModifiers" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailLegModifiers">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PermittedConnectionPoints" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:ConnectionPoint" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProhibitedConnectionPoints" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="common:ConnectionPoint" maxOccurs="999"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Class" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailSegmentList">
		<xs:annotation>
			<xs:documentation>List of Rail Segments</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RailSegment" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailSegment">
		<xs:annotation>
			<xs:documentation>Rail Segment</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:Segment">
					<xs:sequence>
						<xs:element ref="RailSegmentInfo" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="OperatingCompany" minOccurs="0" maxOccurs="1"/>
						<xs:element ref="RailAvailInfo" minOccurs="0" maxOccurs="999"/>
						<xs:element ref="FulFillmentType" minOccurs="0" maxOccurs="999"/>
					</xs:sequence>
					<xs:attribute name="TrainNumber" type="typeTrainNumber" use="optional"/>
					<xs:attributeGroup ref="attrRailSegmentOrigDestInfo"/>
					<xs:attribute name="TrainType" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Type of train used. Same as TrainServiceType.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TrainTypeCode" type="typeTrainType" use="optional">
						<xs:annotation>
							<xs:documentation>Code for type of train used. Same as TrainServiceType. </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TransportMode" type="typeTransportMode" use="optional">
						<xs:annotation>
							<xs:documentation>Type of Transport Mode used.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="SeatAssignable" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Set to true if there exists seats to be booked</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TransportCode" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Supplier specific train code</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ReservationRequired" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Set to true if a reservation is required for booking.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TravelTime" type="xs:integer" use="optional">
						<xs:annotation>
							<xs:documentation>Total time spent (minutes) traveling</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="HostTokenRef" type="common:typeRef" use="optional">
						<xs:annotation>
							<xs:documentation>The reference key for the host token. From the HostTokenList Providers RCH.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="CabinClass" type="common:typeRailCabin" use="optional">
						<xs:annotation>
							<xs:documentation>Rail Cabin class specification. The valid values are Economy, Business, First and Other</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ClassCode" type="common:typeRailClass" use="optional">
						<xs:annotation>
							<xs:documentation>A booking code or fare basis code or fare class.  </xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailSegmentInfo">
		<xs:annotation>
			<xs:documentation>A textual remark container to hold any printable text. (max 512 chars)</xs:documentation>
			<xs:documentation>Holds the ExtraSegmentInfo and VendorMessages from RCH response.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Category" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Supplier specific category.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Type" type="typeRailSegmentInfo" use="required">
						<xs:annotation>
							<xs:documentation>Either Extra for ExtraSegmentInfo or Vendor for VendorMessages.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="OperatingCompany">
		<xs:annotation>
			<xs:documentation>A textual remark identifying the OperatingCompany/Train Service other than BN orTL</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Code" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Company Short Text</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Name" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>Name Identifying the Train Service other than BN orTL</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="typeRailPricingSolution" abstract="true">
		<xs:annotation>
			<xs:documentation>Common RailPricingSolution container</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>Use references on rail availability response.
                    </xs:documentation>
				</xs:annotation>
				<xs:element ref="RailJourney" maxOccurs="999"/>
				<xs:element ref="RailJourneyRef" maxOccurs="999"/>
			</xs:choice>
			<xs:element ref="RailPricingInfo" minOccurs="0" maxOccurs="999"/>
		</xs:sequence>
		<xs:attribute name="Key" type="common:typeRef" use="required"/>
		<xs:attribute name="OfferId" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>OfferID must be included if the RailCreateReq contains a price.  If the RailCreateReq is used for the Direct Book function, the OfferID is not included.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="common:attrPrices"/>
		<xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required"/>
		<xs:attribute name="SupplierCode" type="common:typeSupplierCode" use="required"/>
		<xs:attribute name="HostTokenRef" type="common:typeRef" use="optional">
			<xs:annotation>
				<xs:documentation>HostTokenRef will reference the value in HostTokenList/HostToken @ Key</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Reference" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Offer Reference required for Booking(eg.TL).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="RailPricingSolution">
		<xs:annotation>
			<xs:documentation>Contains the fares and segments for a particular offer. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeRailPricingSolution"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailExchangeSolution">
		<xs:annotation>
			<xs:documentation>Contains the fares and segments for a particular offer.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeRailPricingSolution">
					<xs:sequence>
						<xs:element ref="RailExchangeInfo" minOccurs="0" maxOccurs="1"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailPricingInfo">
		<xs:annotation>
			<xs:documentation>Per traveler type pricing breakdown.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:annotation>
						<xs:documentation>Use references on rail availability response, and the full fare on the reservation.</xs:documentation>
					</xs:annotation>
					<xs:element ref="RailFare" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="RailFareRef" minOccurs="0" maxOccurs="999"/>
				</xs:choice>
				<xs:element ref="RailBookingInfo" minOccurs="0" maxOccurs="999"/>
				<xs:element name="PassengerType" type="common:typePassengerType" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:BookingTravelerRef" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="ExchangeAmount" type="common:typeMoney" use="optional">
				<xs:annotation>
					<xs:documentation>The amount to pay to cover the exchange of the fare (includes penalties).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ApproximateExchangeAmount" type="common:typeMoney" use="optional"/>
			<xs:attributeGroup ref="common:attrPrices"/>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailBookingInfo">
		<xs:annotation>
			<xs:documentation>Links journeys and fares together</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="RailFareRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Reference to a fare that applies to the journey below.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RailJourneyRef" type="common:typeRef" use="required">
				<xs:annotation>
					<xs:documentation>Reference to a journeys on which the above fare applies.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OptionalService" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation> Indicate the OfferFareItem elements  will be Optional or not.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="JourneyRemark">
		<xs:annotation>
			<xs:documentation>A Remark for a Journey.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Category" type="xs:string" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="typeTrainType">
		<xs:annotation>
			<xs:documentation>Code for type of train used. Same as TrainServiceType</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeTransportMode">
		<xs:annotation>
			<xs:documentation>Enumeration of all Train Transport Modes</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Bicycle"/>
			<xs:enumeration value="Boat"/>
			<xs:enumeration value="Bus"/>
			<xs:enumeration value="Cable Car"/>
			<xs:enumeration value="Car"/>
			<xs:enumeration value="Carriage"/>
			<xs:enumeration value="Courtesy car"/>
			<xs:enumeration value="Helicopter"/>
			<xs:enumeration value="Limousine"/>
			<xs:enumeration value="Metro"/>
			<xs:enumeration value="Monorail"/>
			<xs:enumeration value="Motorbike"/>
			<xs:enumeration value="Pack Animal"/>
			<xs:enumeration value="Plane"/>
			<xs:enumeration value="Rental Car"/>
			<xs:enumeration value="Rickshaw"/>
			<xs:enumeration value="Shuttle"/>
			<xs:enumeration value="Subway"/>
			<xs:enumeration value="Sedan Chair"/>
			<xs:enumeration value="Taxi"/>
			<xs:enumeration value="Train"/>
			<xs:enumeration value="Trolley"/>
			<xs:enumeration value="Tube"/>
			<xs:enumeration value="Walk"/>
			<xs:enumeration value="Water Taxi"/>
			<xs:enumeration value="Other"/>
			<xs:enumeration value="Car/Rush hour"/>
			<xs:enumeration value="Taxi/Rush hour"/>
			<xs:enumeration value="No Transportation"/>
			<xs:enumeration value="Express Train"/>
			<xs:enumeration value="Public"/>
			<xs:enumeration value="Ship/Ferry"/>
			<xs:enumeration value="Underground"/>
			<xs:enumeration value="Tram/light rail"/>
			<xs:enumeration value="Shared Taxi"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeJourneyDirection">
		<xs:annotation>
			<xs:documentation>Outbound and Return directions.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Outward"/>
			<xs:enumeration value="Return"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRailSegmentInfo">
		<xs:annotation>
			<xs:documentation>Extra for ExtraSegmentInfo and Vendor for VendorMessages</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Extra"/>
			<xs:enumeration value="Vendor"/>
			<xs:enumeration value="Services"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeRailTicketStatus">
		<xs:annotation>
			<xs:documentation>Status Types for Ticket Info.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Not Print Ready"/>
			<xs:enumeration value="Can Be Printed"/>
			<xs:enumeration value="Queued (sent to print module)"/>
			<xs:enumeration value="Printed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="RailJourneyList">
		<xs:annotation>
			<xs:documentation>List of Rail Journeys</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RailJourney" minOccurs="0" maxOccurs="999"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailJourneyRef">
		<xs:annotation>
			<xs:documentation>Reference to a RailJourney</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
		</xs:complexType>
