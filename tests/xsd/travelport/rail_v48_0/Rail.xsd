			<xs:attribute name="CancellationFee" type="common:typeMoney" use="optional"/>
			<xs:attribute name="ExchangeAmount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="ApproximateRefundAmount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="ApproximateCancellationFee" type="common:typeMoney" use="optional"/>
			<xs:attribute name="ApproximateExchangeAmount" type="common:typeMoney" use="optional">
