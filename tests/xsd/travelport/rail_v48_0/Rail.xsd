					<xs:documentation>The three character passenger code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SupplierPassengerType" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Supplier passenger type code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
