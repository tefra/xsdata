	<xs:element name="RailFare">
		<xs:annotation>
			<xs:documentation>Information about this fare component</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RailFareNoteRef" minOccurs="0" maxOccurs="999">
					<xs:annotation>
						<xs:documentation>Key reference to RailFareNote present in RailFareNotList</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:annotation>
						<xs:documentation>Use references on rail availability response, and the full fare IDs on the reservation.</xs:documentation>
					</xs:annotation>
					<xs:element ref="RailFareID" minOccurs="0" maxOccurs="999"/>
					<xs:element ref="RailFareIDRef" minOccurs="0" maxOccurs="999"/>
				</xs:choice>
				<xs:element ref="FareValidity" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="common:HostToken" minOccurs="0"/>
				<xs:element ref="FulFillmentType" minOccurs="0" maxOccurs="999"/>
				<xs:element ref="RailFareComponent" minOccurs="0" maxOccurs="99"/>
			</xs:sequence>
			<xs:attribute name="Key" type="common:typeRef" use="required"/>
			<xs:attribute name="FareBasis" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The fare basis code  or fare description for this fare</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CabinClass" type="common:typeRailCabin" use="required">
				<xs:annotation>
					<xs:documentation>The fare basis code or fare class for this fare</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="PassengerTypeCode" type="common:typePTC" use="optional">
				<xs:annotation>
					<xs:documentation>The PTC that is associated with this fare. Default to ADT </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Origin" type="common:typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the airport or city code that defines the origin market for this fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Destination" type="common:typeIATACode" use="optional">
				<xs:annotation>
					<xs:documentation>Returns the airport or city code that defines the destination market for this fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="EffectiveDate" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Returns the date on which this fare was quoted. Set as current date</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Amount" type="common:typeMoney" use="optional"/>
			<xs:attribute name="RouteDescription" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Describes the route of the train fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TicketTypeCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Describes the main identifier code of the fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FareReference" use="optional">
				<xs:annotation>
					<xs:documentation>Unique reference for the fare that is required in RailExchangeQuote request. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CrossCityFare" type="xs:boolean" use="optional" default="false">
				<xs:annotation>
					<xs:documentation>Set to 'true' if the fare is valid across a Metropolitan Area, eg. Cross-London travel via the London Underground.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="OriginStationName" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The origin station name for the Rail Fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DestinationStationName" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The destination station name for the Rail Fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ReservationRequired" type="xs:boolean" use="optional">
				<xs:annotation>
					<xs:documentation>Set to true if a seat reservation is required while booking.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="JourneyDirection" type="typeJourneyDirection" use="optional">
				<xs:annotation>
					<xs:documentation>The direction of the Journey (Outward or Return) associated with the Rail fare.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RailLocOrigin" type="common:typeRailLocationCode" use="optional">
				<xs:annotation>
					<xs:documentation>RCH specific origin code (a.k.a UCodes) which uniquely identifies a train station.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="RailLocDestination" type="common:typeRailLocationCode" use="optional">
				<xs:annotation>
					<xs:documentation>RCH specific destination code (a.k.a UCodes) which uniquely identifies a train station.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attributeGroup ref="common:attrElementKeyResults"/>
			<!-- Note: Add annotation. Total Price -->
		</xs:complexType>
	</xs:element>
	<xs:element name="FareValidity">
		<xs:annotation>
